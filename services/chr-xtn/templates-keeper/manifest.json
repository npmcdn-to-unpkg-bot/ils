{
  "name": "Templates Keeper",
  "version": "1.0.6",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
    },
    "content_scripts": [
    {
      "matches": [
      "http://*/*",
      "https://*/*"
      ],
      "js": [
      "scripts/keypress.js",
      "scripts/clipboard.js",
      "scripts/contentScript.js"
      ],
      "run_at": "document_end",
      "all_frames": true
    }
    ],
    "default_locale": "en",
        "permissions": [
        "background",
        "storage",
        "clipboardRead",
        "clipboardWrite",
        "http://*/*",
        "https://*/*"
        ]
      }
