{
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-no-undef": 1,
        "jsx-quotes": 1
    },
    "env": {
        "browser": true,
        "node": false,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "pragma": "React",
            "version": "0.14.0"
        }
    },
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
