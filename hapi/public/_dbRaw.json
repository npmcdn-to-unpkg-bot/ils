    "2169": {
      "dePart": "Ein Feigling ist jemand, der in gefährlichen Situationen mit den Beinen denkt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2169
    },
    "2170": {
      "dePart": "Ehrgeiz ist die letzte Zuflucht des Versagers",
      "enPart": "",
      "category": "preDraft",
      "id": 2170
    },
    "2171": {
      "dePart": "Das echteste an jedem Menschen sind seine Fehler.",
      "enPart": "",
      "category": "preDraft",
      "id": 2171
    },
    "2172": {
      "dePart": "Wer das Leben nicht schätzt, der verdient es nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2172
    },
    "2173": {
      "dePart": "Es ist leichter, einer Begierde ganz zu entsagen,als in ihr Maß zu halten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2173
    },
    "2174": {
      "dePart": "Wenn man nichts tut, glaubt man, dass man für alles die Verantwortung trägt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2174
    },
    "2175": {
      "dePart": "Das Publikum beklatscht ein Feuerwerk, aber keinen Sonnenaufgang.",
      "enPart": "",
      "category": "preDraft",
      "id": 2175
    },
    "2176": {
      "dePart": "Wenn man genug Erfahrungen gesammelt hat, ist man zu alt, um sie auszunutzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2176
    },
    "2177": {
      "dePart": "Die Hauptsache ist, dass man lerne, sich selbst zu beherrschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2177
    },
    "2178": {
      "dePart": "Nur nach dem einen musst du trachten,sei würdig stets dich selbst zu achten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2178
    },
    "2179": {
      "dePart": "Die Sehnsucht ist dem Menschen oft lieber als die Erfüllung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2179
    },
    "2180": {
      "dePart": "Die Dummheit schützt vor Schande,gleichwie die Dreistigkeit vor Armut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2180
    },
    "2181": {
      "dePart": "All zu oft bringt Eile die Menschen zu Fall.",
      "enPart": "",
      "category": "preDraft",
      "id": 2181
    },
    "2182": {
      "dePart": "Das Gewissen ist eine Schwiegermutter, deren Besuch nie endet.",
      "enPart": "",
      "category": "preDraft",
      "id": 2182
    },
    "2183": {
      "dePart": "Was wir Zufall nennen, ist der Zufluchtsort der Unwissenheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2183
    },
    "2184": {
      "dePart": "Was uns beim Sterben weh tut, ist das Leben",
      "enPart": "",
      "category": "preDraft",
      "id": 2184
    },
    "2185": {
      "dePart": "Was man nicht an sich selbst erlebt, erlebtman auch nicht an anderen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2185
    },
    "2186": {
      "dePart": "Was wahr ist, ist weder gut noch böse, sondern einfach nur wahr.",
      "enPart": "",
      "category": "preDraft",
      "id": 2186
    },
    "2187": {
      "dePart": "Vorsicht ist das,was wir bei anderen Feigheit nennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2187
    },
    "2188": {
      "dePart": "Die Wahrheit ist eine scheue Geliebte, man besitzt sie niemals ganz.",
      "enPart": "",
      "category": "preDraft",
      "id": 2188
    },
    "2189": {
      "dePart": "Es gibt nichts Gefährlicheres auf der Welt als fanatische Ideen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2189
    },
    "2190": {
      "dePart": "Moral ist eine Wichtigtuerei des Menschen vor der Natur.",
      "enPart": "",
      "category": "preDraft",
      "id": 2190
    },
    "2191": {
      "dePart": "Jedes überflüssige Wort wirkt seinem Zweck gerade entgegen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2191
    },
    "2192": {
      "dePart": "Geduld zu üben ist die wirksamste Methode, unseren inneren Frieden zu wahren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2192
    },
    "2193": {
      "dePart": "Die Zeit ist die wichtigste Zutat im Rezept des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 2193
    },
    "2194": {
      "dePart": "Belehrungfindet man öfter in der Welt als Trost.",
      "enPart": "",
      "category": "preDraft",
      "id": 2194
    },
    "2195": {
      "dePart": "Geduld ist die Kunst,die Ungeduld zu verbergen",
      "enPart": "",
      "category": "preDraft",
      "id": 2195
    },
    "2196": {
      "dePart": "Wenn wir bewahren wollen, was wir haben, müssen wir vieles ändern",
      "enPart": "",
      "category": "preDraft",
      "id": 2196
    },
    "2197": {
      "dePart": "Ich liebe mein Leben,weil du die Süßigkeit meines Lebens bist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2197
    },
    "2198": {
      "dePart": "Der Utopist sieht das Paradies, der Realist das Paradies plus Schlange",
      "enPart": "",
      "category": "preDraft",
      "id": 2198
    },
    "2199": {
      "dePart": "Nimm das Leben nicht so ernst, du kommst da ja doch nicht lebend raus",
      "enPart": "",
      "category": "preDraft",
      "id": 2199
    },
    "2200": {
      "dePart": "Die Dummheit geht oft Hand in Hand mit Bosheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2200
    },
    "2201": {
      "dePart": "Das Lebenist die Suche des Nichtsnach dem Etwas",
      "enPart": "",
      "category": "preDraft",
      "id": 2201
    },
    "2202": {
      "dePart": "Überraschung und Verwunderung sind der Anfang des Begreifens",
      "enPart": "",
      "category": "preDraft",
      "id": 2202
    },
    "2203": {
      "dePart": "Sich selbst zu überraschen ist,was das Leben lebenswert macht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2203
    },
    "2204": {
      "dePart": "Meide alles, was die Menschen trennt, und tu alles, was sie eint.",
      "enPart": "",
      "category": "preDraft",
      "id": 2204
    },
    "2205": {
      "dePart": "Man glaubt nicht, was jeder Mensch glaubt, was er für ein Mensch ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2205
    },
    "2206": {
      "dePart": "Die größte Sehenswürdigkeit, die es gibtist die Welt - sieh sie dir an!",
      "enPart": "",
      "category": "preDraft",
      "id": 2206
    },
    "2207": {
      "dePart": "Das Gewissen ist eine Uhr, die bei den meisten Menschen immer nachgeht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2207
    },
    "2208": {
      "dePart": "Wer mit mir reden will, der darf nicht bloß seine eigene Meinung hören wollen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2208
    },
    "2209": {
      "dePart": "Eben, wenn man alt ist, muß man zeigen, dass man noch Lust zu leben hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2209
    },
    "2210": {
      "dePart": "Des Menschen Leben istein kurzes Blühen und ein langes Welken.",
      "enPart": "",
      "category": "preDraft",
      "id": 2210
    },
    "2211": {
      "dePart": "Es braucht zu allem ein Entschließen,selbst zum Genießen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2211
    },
    "2212": {
      "dePart": "Geduld ist die Stütze der Schwäche, Ungeduld der Ruin der Stärke.",
      "enPart": "",
      "category": "preDraft",
      "id": 2212
    },
    "2213": {
      "dePart": "Warum bekommt der Mensch die Jugend in einem Alter, in dem er nichts davon hat?",
      "enPart": "",
      "category": "preDraft",
      "id": 2213
    },
    "2214": {
      "dePart": "Es ist selten, dass ein Mensch weiß, was er eigentlich glaubt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2214
    },
    "2215": {
      "dePart": "Die Schönheit der Dinge, lebt in der Seele dessen, der sie betrachtet.",
      "enPart": "",
      "category": "preDraft",
      "id": 2215
    },
    "2216": {
      "dePart": "Unter guten, seelenvollen Menschen trägt sich die Last des Lebens leicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2216
    },
    "2217": {
      "dePart": "Es sind viele Esel, die nicht Säcke tragen",
      "enPart": "",
      "category": "preDraft",
      "id": 2217
    },
    "2218": {
      "dePart": "Der Strom der Wahrheit fließt durch Kanäle von Irrtümern",
      "enPart": "",
      "category": "preDraft",
      "id": 2218
    },
    "2219": {
      "dePart": "Was wir heute tun, entscheidet, wie die Welt morgen aussieht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2219
    },
    "2220": {
      "dePart": "Es gibt ebenso wenig hundertprozentige Wahrheit wie hundertprozentigen Alkohol.",
      "enPart": "",
      "category": "preDraft",
      "id": 2220
    },
    "2221": {
      "dePart": "Beim Menschen ist kein Ding unmöglich, im Schlimmen wie im Guten",
      "enPart": "",
      "category": "preDraft",
      "id": 2221
    },
    "2222": {
      "dePart": "Was dem Verstande endlich, ist nichtig dem Herzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2222
    },
    "2223": {
      "dePart": "Entwicklung - ein Taumel von einem Irrtum zum anderen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2223
    },
    "2224": {
      "dePart": "Wer wagen will, sich zu kennen, muß verzichten, sich zu gefallen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2224
    },
    "2225": {
      "dePart": "Wer lange genug gelebt hat, hat alles gesehen und auch das Gegenteil von allem.",
      "enPart": "",
      "category": "preDraft",
      "id": 2225
    },
    "2226": {
      "dePart": "Die Träume - sie erschaffen nicht die Wünsche, sie wecken die vorhandenen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2226
    },
    "2227": {
      "dePart": "Dem Alter, nicht der Jugend sei's geklagt, wenn uns das Alter nicht behagt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2227
    },
    "2228": {
      "dePart": "Die Menschheit ist zum Werkzeug ihrer Werkzeuge geworden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2228
    },
    "2229": {
      "dePart": "Das Leben ist eine Kette von Vergessenheiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2229
    },
    "2230": {
      "dePart": "Um Erfahrungen zu machen, bedarf es der Weisheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2230
    },
    "2231": {
      "dePart": "Erstaunlich, dass der Mensch nur hinter einer Maske ganz er selbst ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2231
    },
    "2232": {
      "dePart": "Der ist nicht einsam, der noch Schmerzen fühlt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2232
    },
    "2233": {
      "dePart": "Zwischen Sinnenglück und Seelenfriedenbleibt dem Menschen nur die bange Wahl.",
      "enPart": "",
      "category": "preDraft",
      "id": 2233
    },
    "2234": {
      "dePart": "Die Welt ist so schön und wert, dass man um sie kämpft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2234
    },
    "2235": {
      "dePart": "Kein Mensch ist so arm, dass er nichts besäße, was zu geben sich lohnt",
      "enPart": "",
      "category": "preDraft",
      "id": 2235
    },
    "2236": {
      "dePart": "Herz paßt nicht zu moderner Kleidung, es macht alt",
      "enPart": "",
      "category": "preDraft",
      "id": 2236
    },
    "2237": {
      "dePart": "Was Du erlebt, kann Dir kein Gott mehr rauben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2237
    },
    "2238": {
      "dePart": "Die meisten Menschen benützen ihre Jugend,um ihr Alter zu ruinieren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2238
    },
    "2239": {
      "dePart": "Gott zieht an einer Hand,der Teufel an beiden Beinen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2239
    },
    "2240": {
      "dePart": "Wer die Wahrheit nicht fürchtet,braucht auch die Lüge nicht zu fürchten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2240
    },
    "2241": {
      "dePart": "Man macht das Leben nicht leichter, wenn man es seichter macht",
      "enPart": "",
      "category": "preDraft",
      "id": 2241
    },
    "2242": {
      "dePart": "Fortwährendem Entbehren folgt Stumpfheit ebensowie übermässigem Genuss.",
      "enPart": "",
      "category": "preDraft",
      "id": 2242
    },
    "2243": {
      "dePart": "Wer vor sich selber flieht, wird immer eingeholt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2243
    },
    "2244": {
      "dePart": "Es ist nichts so klein und wenig,woran man sich nicht begeistern könnte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2244
    },
    "2245": {
      "dePart": "Unsichtbar wird der Wahnsinn, wenn er genügend große Ausmaße angenommen hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2245
    },
    "2246": {
      "dePart": "Gewohnheit ist oft ein Mangelan Überlegung und Erkenntnis.",
      "enPart": "",
      "category": "preDraft",
      "id": 2246
    },
    "2247": {
      "dePart": "Gefühl verankert ein Wissen nachhaltigerals tausend Worte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2247
    },
    "2248": {
      "dePart": "Der Sinn des Lebens ist ein Leben mit Sinn.",
      "enPart": "",
      "category": "preDraft",
      "id": 2248
    },
    "2249": {
      "dePart": "Wir leben nicht, um zu glauben, sondern um zu lernen",
      "enPart": "",
      "category": "preDraft",
      "id": 2249
    },
    "2250": {
      "dePart": "Das Leben ist nicht gerecht, und für die meisten von uns ist das gut so",
      "enPart": "",
      "category": "preDraft",
      "id": 2250
    },
    "2251": {
      "dePart": "Stimmung ist Schweigen in Gefühlen, die man nicht hat",
      "enPart": "",
      "category": "preDraft",
      "id": 2251
    },
    "2252": {
      "dePart": "Große Geister haben Ziele, andere haben Wünsche",
      "enPart": "",
      "category": "preDraft",
      "id": 2252
    },
    "2253": {
      "dePart": "Alle menschlichen Verfehlungen sind das Ergebnis eines Mangels an Liebe.",
      "enPart": "",
      "category": "preDraft",
      "id": 2253
    },
    "2254": {
      "dePart": "Die menschliche Natur hasst alles, was überlegen ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2254
    },
    "2255": {
      "dePart": "Es ist keine bessere Harmonie, als wenn Herz und Mund übereinstimmen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2255
    },
    "2256": {
      "dePart": "Das Menschenleben ist eine ständige Schule.",
      "enPart": "",
      "category": "preDraft",
      "id": 2256
    },
    "2257": {
      "dePart": "Schönheit des Körpers hat etwas Tierisches, wenn sie geistlos ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2257
    },
    "2258": {
      "dePart": "Mache dich nicht so wichtig - es gibt größere Zwerge als du einer bist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2258
    },
    "2259": {
      "dePart": "Ein wenig Aufrichtigkeit ist eine gefährliche Sache und viel davon ist tödlich",
      "enPart": "",
      "category": "preDraft",
      "id": 2259
    },
    "2260": {
      "dePart": "Besser noch, das Ärgernis entsteht, als dass man die Wahrheit im Stich läßt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2260
    },
    "2261": {
      "dePart": "Die Übertreibung ist eine Wahrheit, welche die Ruhe verloren hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2261
    },
    "2262": {
      "dePart": "Wer vor seiner Vergangenheit flieht, verliert immer das Rennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2262
    },
    "2263": {
      "dePart": "Die Zeit verlängert sich für alle,die sie zu nutzen verstehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2263
    },
    "2264": {
      "dePart": "Wieviel Schönheit empfängt das Herz durch die Augen!",
      "enPart": "",
      "category": "preDraft",
      "id": 2264
    },
    "2265": {
      "dePart": "Wer nicht den Wert der Weisheit wahrt,kann nicht den Weg zur Wahrheit weisen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2265
    },
    "2266": {
      "dePart": "Illusion und Weisheit zusammen bilden das Entzücken des Lebens und der Kunst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2266
    },
    "2267": {
      "dePart": "Der Mensch bedarf der Teilnahme und Anregung, wenn etwas gelingen soll.",
      "enPart": "",
      "category": "preDraft",
      "id": 2267
    },
    "2268": {
      "dePart": "Die größte Gefahr im Leben ist, dass man zu vorsichtig wird.",
      "enPart": "",
      "category": "preDraft",
      "id": 2268
    },
    "2269": {
      "dePart": "Seine Seele zeigt der Mensch nur in seinem Tun",
      "enPart": "",
      "category": "preDraft",
      "id": 2269
    },
    "2270": {
      "dePart": "Wer die Zeit anklagt, will sich nur herausreden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2270
    },
    "2271": {
      "dePart": "Man muss sich beeilen, wenn man etwas sehen will, alles verschwindet",
      "enPart": "",
      "category": "preDraft",
      "id": 2271
    },
    "2272": {
      "dePart": "Es gibt kein Omelett,ohne Eier zu zerbrechen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2272
    },
    "2273": {
      "dePart": "Beständige Treue in kleinen Dingen ist eine großartige und heroische Tugend.",
      "enPart": "",
      "category": "preDraft",
      "id": 2273
    },
    "2274": {
      "dePart": "Das größte Übel bei den Menschen ist ihre Unersättlichkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2274
    },
    "2275": {
      "dePart": "Wer tiefer irrt,der wird auch tiefer weise.",
      "enPart": "",
      "category": "preDraft",
      "id": 2275
    },
    "2276": {
      "dePart": "Sei gut zu Deinem Körper,damit Deine Seele Lust hat darin zu wohnen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2276
    },
    "2277": {
      "dePart": "Je weniger man zustande bringt, desto kürzer scheint das Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2277
    },
    "2278": {
      "dePart": "Zwischen Entweder und Oder führt manches Sträßlein",
      "enPart": "",
      "category": "preDraft",
      "id": 2278
    },
    "2279": {
      "dePart": "Die einzige Möglichkeit eine Versuchung loszuwerden, ist ihr nachzugeben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2279
    },
    "2280": {
      "dePart": "Was uns beim Sterben weh tut, ist das Leben",
      "enPart": "",
      "category": "preDraft",
      "id": 2280
    },
    "2281": {
      "dePart": "Ich bin nicht mehr jung genug, um alles zu wissen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2281
    },
    "2282": {
      "dePart": "Das Leben in der Natur gibt die Wahrheit der Dinge zu erkennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2282
    },
    "2283": {
      "dePart": "Wir wissen von unsrer Seele wenig und sind sie selbst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2283
    },
    "2284": {
      "dePart": "Wer schnell entschlossen ist, der strauchelt leicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2284
    },
    "2285": {
      "dePart": "Alle Menschen haben eine Ahnung und Idee der Wahrheit in sich.",
      "enPart": "",
      "category": "preDraft",
      "id": 2285
    },
    "2286": {
      "dePart": "Die Revolutionen sind die Lokomotiven der Geschichte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2286
    },
    "2287": {
      "dePart": "Alle Tragödien beendet der Tod,alle Komödien die Ehe",
      "enPart": "",
      "category": "preDraft",
      "id": 2287
    },
    "2288": {
      "dePart": "Nach den Jahren der Lastkommt die Last der Jahre",
      "enPart": "",
      "category": "preDraft",
      "id": 2288
    },
    "2289": {
      "dePart": "Die Klarheit seines Innernist für die Menschen das höchste Gut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2289
    },
    "2290": {
      "dePart": "Schweigen ist die unerträglichste Erwiderung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2290
    },
    "2291": {
      "dePart": "Wenn man seine Kindheit bei sich hat, wird man nie älter",
      "enPart": "",
      "category": "preDraft",
      "id": 2291
    },
    "2292": {
      "dePart": "Es fliehen nicht alle,die den Rücken kehren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2292
    },
    "2293": {
      "dePart": "Ein Dummkopf findet immer einen, der noch dümmer ist, der ihn bewundert.",
      "enPart": "",
      "category": "preDraft",
      "id": 2293
    },
    "2294": {
      "dePart": "Das Erste, was im Krieg auf der Strecke bleibt,ist die Wahrheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2294
    },
    "2295": {
      "dePart": "Wovor ich mich am meisten fürchte, ist die Furcht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2295
    },
    "2296": {
      "dePart": "Im Herzen eines Menschen ruht der Anfang und das Ende aller Dinge",
      "enPart": "",
      "category": "preDraft",
      "id": 2296
    },
    "2297": {
      "dePart": "Geschichte schreiben ist eine Art, sich das Vergangene vom Halse zu schaffen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2297
    },
    "2298": {
      "dePart": "Der Zufall ist ein Rätsel,welches das Schicksal dem Menschen aufgibt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2298
    },
    "2299": {
      "dePart": "Den Namen des Rechtes würde man nicht kennen, wenn es das Unrecht nicht gäbe.",
      "enPart": "",
      "category": "preDraft",
      "id": 2299
    },
    "2300": {
      "dePart": "Die Gerechtigkeit wächst nicht auf einem mit Lastern gedüngten Boden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2300
    },
    "2301": {
      "dePart": "Ein Mensch ist seiner Beschaffenheit nach ein religiöses Tier.",
      "enPart": "",
      "category": "preDraft",
      "id": 2301
    },
    "2302": {
      "dePart": "Ein schönes Weib war immer rar, das nicht auch widerspenstig war",
      "enPart": "",
      "category": "preDraft",
      "id": 2302
    },
    "2303": {
      "dePart": "In dieser Welt gibt's immer Gefahren für die, die sich fürchten",
      "enPart": "",
      "category": "preDraft",
      "id": 2303
    },
    "2304": {
      "dePart": "Sehnsucht nach Licht ist des Lebens Gebot",
      "enPart": "",
      "category": "preDraft",
      "id": 2304
    },
    "2305": {
      "dePart": "Man hört in der Welt leichter ein Echo als eine Antwort",
      "enPart": "",
      "category": "preDraft",
      "id": 2305
    },
    "2306": {
      "dePart": "Verehrung ist Liebe zu jemandem,den wir bewundern",
      "enPart": "",
      "category": "preDraft",
      "id": 2306
    },
    "2307": {
      "dePart": "Körper, Seele und Geist sind die Elemente der Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2307
    },
    "2308": {
      "dePart": "Heutzutage hat jeder vor sich selbst Angst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2308
    },
    "2309": {
      "dePart": "Was das Gesetz nicht verbietet, das verbietet der Anstand.",
      "enPart": "",
      "category": "preDraft",
      "id": 2309
    },
    "2310": {
      "dePart": "Was glänzt, ist für den Augenblick geboren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2310
    },
    "2311": {
      "dePart": "Es gibt keine Beschimpfung, die man nicht verzeiht,wenn man sich gerächt hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2311
    },
    "2312": {
      "dePart": "Wer Unverhofftes nicht erhofft, kann es nicht finden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2312
    },
    "2313": {
      "dePart": "Der Geist der Frauen stärkt eher ihren Leichtsinn als ihre Vernunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2313
    },
    "2314": {
      "dePart": "Geiz ist die letzte und tyrannischste unserer Leidenschaften.",
      "enPart": "",
      "category": "preDraft",
      "id": 2314
    },
    "2315": {
      "dePart": "Drei sind, die da herrschen auf Erden: die Weisheit, der Schein und die Gewalt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2315
    },
    "2316": {
      "dePart": "Die Jahre lehren viel, was die Tage nicht wissen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2316
    },
    "2317": {
      "dePart": "Das Jahrhundert ist vorgerückt; jeder einzelne aber fängt doch von vorne an.",
      "enPart": "",
      "category": "preDraft",
      "id": 2317
    },
    "2318": {
      "dePart": "Die Vielen können nichts, der Einzelne kann alles.",
      "enPart": "",
      "category": "preDraft",
      "id": 2318
    },
    "2319": {
      "dePart": "Denn was von mir ein Esel spricht,das acht' ich nicht!",
      "enPart": "",
      "category": "preDraft",
      "id": 2319
    },
    "2320": {
      "dePart": "Das Vorurteil ist das Kind der Unwissenheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2320
    },
    "2321": {
      "dePart": "Schlecht weht der Wind,der keinen Vorteil bringt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2321
    },
    "2322": {
      "dePart": "Gar oft verbirgt sich in groben Hosen ein in sie gefallenes Herz",
      "enPart": "",
      "category": "preDraft",
      "id": 2322
    },
    "2323": {
      "dePart": "Das Beste im Menschen sindseine jungen Gefühle und seine alten Gedanken.",
      "enPart": "",
      "category": "preDraft",
      "id": 2323
    },
    "2324": {
      "dePart": "Macht ist eine Mahlzeit, die wachsenden Appetit verursacht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2324
    },
    "2325": {
      "dePart": "Der Mensch kommt fast nie aus Vernunft zur Vernunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2325
    },
    "2326": {
      "dePart": "Das Herz gibt allem, was der Mensch sieht, hört und weiß, die Farbe.",
      "enPart": "",
      "category": "preDraft",
      "id": 2326
    },
    "2327": {
      "dePart": "Man ist nie so sehr bei sich, als wenn man sich verliert.",
      "enPart": "",
      "category": "preDraft",
      "id": 2327
    },
    "2328": {
      "dePart": "Ich höre mich gern reden - es ist so unterhaltend, sich zuzuhören.",
      "enPart": "",
      "category": "preDraft",
      "id": 2328
    },
    "2329": {
      "dePart": "Die Wahrheit ist unterwegs und nichts kann sie aufhalten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2329
    },
    "2330": {
      "dePart": "Selten tritt dem Weisen das Schicksal in den Weg",
      "enPart": "",
      "category": "preDraft",
      "id": 2330
    },
    "2331": {
      "dePart": "Mit dem Alter nimmt Urteilskraft zu und Genie ab.",
      "enPart": "",
      "category": "preDraft",
      "id": 2331
    },
    "2332": {
      "dePart": "Alles, was wir mit Wärme und Enthusiasmus ergreifen, ist eine Art der Liebe.",
      "enPart": "",
      "category": "preDraft",
      "id": 2332
    },
    "2333": {
      "dePart": "Recht häuft sich nicht, wohl aber Unrecht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2333
    },
    "2334": {
      "dePart": "Zaghaftigkeit in der Ausführung macht tollkühne Unternehmungen zunichte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2334
    },
    "2335": {
      "dePart": "Wir leben in einer Zeit vollkommener Mittel und verworrener Ziele.",
      "enPart": "",
      "category": "preDraft",
      "id": 2335
    },
    "2336": {
      "dePart": "Tu, was du kannst, mit dem, was du hast, wo immer du bist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2336
    },
    "2337": {
      "dePart": "Der größte Lump im ganzen Land, das ist und bleibt der Denunziant.",
      "enPart": "",
      "category": "preDraft",
      "id": 2337
    },
    "2338": {
      "dePart": "Der Mensch - ein Exempel der beispiellosen Geduld der Natur.",
      "enPart": "",
      "category": "preDraft",
      "id": 2338
    },
    "2339": {
      "dePart": "Das nenne ich würdig, fürwahr, sich im Alter zu beschäftigen!",
      "enPart": "",
      "category": "preDraft",
      "id": 2339
    },
    "2340": {
      "dePart": "Menschlich gesprochen hat auch der Tod sein Gutes: Er setzt dem Alter ein Ziel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2340
    },
    "2341": {
      "dePart": "Was in der Welt dir nicht gefällt,musst du dir gelassen gefallen lassen",
      "enPart": "",
      "category": "preDraft",
      "id": 2341
    },
    "2342": {
      "dePart": "Gleichmütigkeit ist das Selbstgefühleiner gesunden Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 2342
    },
    "2343": {
      "dePart": "Kräfte lassen sich nicht mitteilen, sondern nur wecken.",
      "enPart": "",
      "category": "preDraft",
      "id": 2343
    },
    "2344": {
      "dePart": "Von einem bestimmten Alter an ist jeder Mensch für sein Gesicht verantwortlich",
      "enPart": "",
      "category": "preDraft",
      "id": 2344
    },
    "2345": {
      "dePart": "Träume verhauchen - und nackte, geschundene Menschen erwachen",
      "enPart": "",
      "category": "preDraft",
      "id": 2345
    },
    "2346": {
      "dePart": "Wieviel Freude schläft in uns - und wir wecken sie nicht!",
      "enPart": "",
      "category": "preDraft",
      "id": 2346
    },
    "2347": {
      "dePart": "Was man verachten will, das muss man kennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2347
    },
    "2348": {
      "dePart": "Unrecht gewinnt oft Rechtscharakter einfach dadurch,dass es häufig vorkommt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2348
    },
    "2349": {
      "dePart": "Tradition ist Bewahrung des Feuers und nicht Anbetung der Asche.",
      "enPart": "",
      "category": "preDraft",
      "id": 2349
    },
    "2350": {
      "dePart": "Der Mensch kann nicht anders als menschlich urteilen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2350
    },
    "2351": {
      "dePart": "Die Zeit bekehrt mehr Menschen als die Vernunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2351
    },
    "2352": {
      "dePart": "Anmut ist ein Ausströmen der inneren Harmonie.",
      "enPart": "",
      "category": "preDraft",
      "id": 2352
    },
    "2353": {
      "dePart": "Der Mensch ist das Raubtier mit den Händen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2353
    },
    "2354": {
      "dePart": "Die meisten Dinge, die uns Vergnügen bereiten,sind unvernünftige Dinge.",
      "enPart": "",
      "category": "preDraft",
      "id": 2354
    },
    "2355": {
      "dePart": "Die Vorsehung wirkt ihre höchsten Ziele durch scheinbare Zufälle.",
      "enPart": "",
      "category": "preDraft",
      "id": 2355
    },
    "2356": {
      "dePart": "Auch das Gewissen der Menschen unterliegt der Mode.",
      "enPart": "",
      "category": "preDraft",
      "id": 2356
    },
    "2357": {
      "dePart": "Je boshafter eine Lüge ist, für desto wahrscheinlicher hält man sie.",
      "enPart": "",
      "category": "preDraft",
      "id": 2357
    },
    "2358": {
      "dePart": "Was gestern absurd schien, wird heute allgemein geglaubt und ist morgen banal",
      "enPart": "",
      "category": "preDraft",
      "id": 2358
    },
    "2359": {
      "dePart": "Das Gesetz ist lückenhaft, das Recht ist lückenlos.",
      "enPart": "",
      "category": "preDraft",
      "id": 2359
    },
    "2360": {
      "dePart": "Nichts tun vermehrt den Frieden der Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2360
    },
    "2361": {
      "dePart": "Der Mensch vergißt Schmerz viel rascher als Freude.",
      "enPart": "",
      "category": "preDraft",
      "id": 2361
    },
    "2362": {
      "dePart": "Die Angst vor einer Gefahr kann manchmal die größte der Gefahren sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2362
    },
    "2363": {
      "dePart": "Ein einz'ger Augenblick kann alles umgestalten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2363
    },
    "2364": {
      "dePart": "Wie viele Lichter verdanken bloß Ihrem Leuchter, dass man sie sieht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2364
    },
    "2365": {
      "dePart": "Für den Bau von Luftschlössern gibt es keine architektonischen Regeln.",
      "enPart": "",
      "category": "preDraft",
      "id": 2365
    },
    "2366": {
      "dePart": "Die Ewigkeit gleicht einem Rad,das weder Anfang noch Ende hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2366
    },
    "2367": {
      "dePart": "Verbrecher werden immer von Verbrechern eingesperrt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2367
    },
    "2368": {
      "dePart": "Ich besitze nicht, damit ich nicht besessen werde.",
      "enPart": "",
      "category": "preDraft",
      "id": 2368
    },
    "2369": {
      "dePart": "Die Furcht schützt die Tempel besser als alle Mauern.",
      "enPart": "",
      "category": "preDraft",
      "id": 2369
    },
    "2370": {
      "dePart": "Ein Mensch, der Angst hat, wird zum reißenden Tier.",
      "enPart": "",
      "category": "preDraft",
      "id": 2370
    },
    "2371": {
      "dePart": "Was der Feind uns nicht antun konnte, das tun wir uns selber an.",
      "enPart": "",
      "category": "preDraft",
      "id": 2371
    },
    "2372": {
      "dePart": "Ein Mord aus Rache, ein Mord aus Hass ist kein Beweis für Kraft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2372
    },
    "2373": {
      "dePart": "Manche Laster lassen im Alter nach, andere werden ärger.",
      "enPart": "",
      "category": "preDraft",
      "id": 2373
    },
    "2374": {
      "dePart": "Zyniker: ein Mensch, der, wenn er Blumen sieht, nach dem Sarg Ausschau hält.",
      "enPart": "",
      "category": "preDraft",
      "id": 2374
    },
    "2375": {
      "dePart": "Die Welt ist ein schöner Platzund wert, dass man um sie kämpft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2375
    },
    "2376": {
      "dePart": "Es kennzeichnet den Unerfahrenen, dass er nicht an glückliche Zufälle glaubt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2376
    },
    "2377": {
      "dePart": "Der Veränderung die Tür schließen hieße,das Leben selber aussperren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2377
    },
    "2378": {
      "dePart": "Zeige dich, wie du bist, und sei, wie du dich zeigst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2378
    },
    "2379": {
      "dePart": "Ich lerne, wenn ich gescheitert bin und nicht, wenn ich Erfolg hatte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2379
    },
    "2380": {
      "dePart": "Auf dem Herzen steht, wie auf gewissen Medizinen: Vor Gebrauch schütteln.",
      "enPart": "",
      "category": "preDraft",
      "id": 2380
    },
    "2381": {
      "dePart": "Gewinnsucht und Eitelkeit sind die Werbeoffiziere der Schlechtigkeit",
      "enPart": "",
      "category": "preDraft",
      "id": 2381
    },
    "2382": {
      "dePart": "Ungeduld sieht nur das Ziel,nicht die Wege dorthin.",
      "enPart": "",
      "category": "preDraft",
      "id": 2382
    },
    "2383": {
      "dePart": "Wer darauf wartet,für wichtige Dinge Zeit zu haben,dem läuft sie davon.",
      "enPart": "",
      "category": "preDraft",
      "id": 2383
    },
    "2384": {
      "dePart": "Entscheidungen, Entschlüsse brauchen Zeit —vor oder nach dem Handeln.",
      "enPart": "",
      "category": "preDraft",
      "id": 2384
    },
    "2385": {
      "dePart": "Mit offenen Augen und wachen Sinnenlassen sich Wunder entdecken.",
      "enPart": "",
      "category": "preDraft",
      "id": 2385
    },
    "2386": {
      "dePart": "Wir liegen alle in der Gosse, aber einige von uns betrachten die Sterne.",
      "enPart": "",
      "category": "preDraft",
      "id": 2386
    },
    "2387": {
      "dePart": "Es bedürfte zu vieler Verdienste, um das zu sein,was man von sich hält.",
      "enPart": "",
      "category": "preDraft",
      "id": 2387
    },
    "2388": {
      "dePart": "Tapferkeit ist die Fähigkeit, von der eigenen Furcht keine Notiz zu nehmen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2388
    },
    "2389": {
      "dePart": "Überschreitet man das Maß, so wird das Angenehme zum Unangenehmen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2389
    },
    "2390": {
      "dePart": "Die das Dunkel nicht fühlen, werden sich nie nach dem Lichte umsehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2390
    },
    "2391": {
      "dePart": "Viele Menschen halten ihre Phantasie für ihr Gedächtnis.",
      "enPart": "",
      "category": "preDraft",
      "id": 2391
    },
    "2392": {
      "dePart": "Wandlung ist notwendig,wie die Erneuerung der Blätter im Frühling.",
      "enPart": "",
      "category": "preDraft",
      "id": 2392
    },
    "2393": {
      "dePart": "Die Moral steckt in kurzen Sprüchen besser, als in langen Reden und Predigten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2393
    },
    "2394": {
      "dePart": "Von Wahrheit will ich nimmer weichen, das soll kein Mann bei mir erreichen",
      "enPart": "",
      "category": "preDraft",
      "id": 2394
    },
    "2395": {
      "dePart": "Alle Dinge werden zu einer Quelle der Lust, wenn man sie liebt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2395
    },
    "2396": {
      "dePart": "Sobald eine Mode allgemein geworden ist, hat sie sich überlebt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2396
    },
    "2397": {
      "dePart": "Die Lust ist nur dann ein Gut, wenn sie keine Reue nach sich zieht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2397
    },
    "2398": {
      "dePart": "Das Gefühl versteht, was der Verstand nicht begreift.",
      "enPart": "",
      "category": "preDraft",
      "id": 2398
    },
    "2399": {
      "dePart": "Gewohnheiten mögen nicht so weise sein wie Gesetze, sind aber immer beliebter.",
      "enPart": "",
      "category": "preDraft",
      "id": 2399
    },
    "2400": {
      "dePart": "Wer seine Grenzen kennt, ist schon ein halber Weiser.",
      "enPart": "",
      "category": "preDraft",
      "id": 2400
    },
    "2401": {
      "dePart": "Die Vorsehung gibt uns nicht zweimal die Hand.",
      "enPart": "",
      "category": "preDraft",
      "id": 2401
    },
    "2402": {
      "dePart": "Man trägt oft am schwersten an dem, was man weggeworfen hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2402
    },
    "2403": {
      "dePart": "Es ist besser, für etwas zu kämpfen, als gegen etwas.",
      "enPart": "",
      "category": "preDraft",
      "id": 2403
    },
    "2404": {
      "dePart": "Es ist unglaublich, wieviel Kraft die Seele dem Körper zu leihen vermag.",
      "enPart": "",
      "category": "preDraft",
      "id": 2404
    },
    "2405": {
      "dePart": "Vielleicht kann ich die Wahrheit finden, indem ich die Lügen vergleiche.",
      "enPart": "",
      "category": "preDraft",
      "id": 2405
    },
    "2406": {
      "dePart": "Der erste Seufzer der Liebe ist der letzte der Zurechnungsfähigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2406
    },
    "2407": {
      "dePart": "Zwang ist ein Übel; aber es besteht kein Zwang, unter Zwang zu leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2407
    },
    "2408": {
      "dePart": "Der Traum und die Phantasiesind das eigenste Eigentum",
      "enPart": "",
      "category": "preDraft",
      "id": 2408
    },
    "2409": {
      "dePart": "Das Leben wäre unerträglich,wenn wir nie träumten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2409
    },
    "2410": {
      "dePart": "Tatsachen hören nicht auf zu existieren, nur weil sie ignoriert werden",
      "enPart": "",
      "category": "preDraft",
      "id": 2410
    },
    "2411": {
      "dePart": "Nur in der Träne des Schmerzes spiegeltsich der Regenbogen einer besseren Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2411
    },
    "2412": {
      "dePart": "In der Bilanz eines Lebenszählen manche Momente mehr als viele Jahre",
      "enPart": "",
      "category": "preDraft",
      "id": 2412
    },
    "2413": {
      "dePart": "Die Leidenschaften vernichten das Beste und die Besten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2413
    },
    "2414": {
      "dePart": "Der Glaube an Vorurteile gilt in der Welt als gesunder Menschenverstand.",
      "enPart": "",
      "category": "preDraft",
      "id": 2414
    },
    "2415": {
      "dePart": "An Wundern ist niemals Mangel in dieser Welt,sondern nur am Sich-Wundern-Können",
      "enPart": "",
      "category": "preDraft",
      "id": 2415
    },
    "2416": {
      "dePart": "Etwas vergessen lernen, ist ebenso wichtig, wie etwas behalten lernen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2416
    },
    "2417": {
      "dePart": "Sehnsucht involviert immer Ungeduld und bedeutet Leiden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2417
    },
    "2418": {
      "dePart": "Für seine Prinzipien kann man leichter kämpfen, als nach ihnen leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2418
    },
    "2419": {
      "dePart": "Ein angenehmes Laster ist einer langweiligen Tugend bei weitem vorzuziehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2419
    },
    "2420": {
      "dePart": "Das Gewissen spricht, aber der Eigennutz schreit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2420
    },
    "2421": {
      "dePart": "Um die Seele zu füllen, muss sie erst entleert werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2421
    },
    "2422": {
      "dePart": "So viele Jahre auch vergehen mögen,die Natur des Menschen ändert sich nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2422
    },
    "2423": {
      "dePart": "Der am meisten beschäftigte Mensch hat am meisten Zeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2423
    },
    "2424": {
      "dePart": "Nichts ist schöner als seine Pflicht zu tun, wenn man der Herr ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2424
    },
    "2425": {
      "dePart": "Der Reisende verändert nur den Himmel über sich - nie die Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 2425
    },
    "2426": {
      "dePart": "Nicht alles,was zwei Backen hat,ist ein Gesicht",
      "enPart": "",
      "category": "preDraft",
      "id": 2426
    },
    "2427": {
      "dePart": "Was für ein Los wir fürchten müssen,sagt uns das eigene Gewissen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2427
    },
    "2428": {
      "dePart": "Jeder ist ein Trottel, nur auf jeweils auf einem anderen Gebiet.",
      "enPart": "",
      "category": "preDraft",
      "id": 2428
    },
    "2429": {
      "dePart": "Leben ist ja doch des Lebens höchstes Ziel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2429
    },
    "2430": {
      "dePart": "Es gibt eine Grenze, wo Zurückhaltung aufhört, eine Tugend zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2430
    },
    "2431": {
      "dePart": "Ein Teil des Lebens geht zu Grunde,so oft eine Stunde verschleudert wird.",
      "enPart": "",
      "category": "preDraft",
      "id": 2431
    },
    "2432": {
      "dePart": "Dem Armen ist nicht mehr gegeben als gute Hoffnung, übles Leben",
      "enPart": "",
      "category": "preDraft",
      "id": 2432
    },
    "2433": {
      "dePart": "Der Rost frisst Stahl und Eisen, die Sorge frisst den Weisen",
      "enPart": "",
      "category": "preDraft",
      "id": 2433
    },
    "2434": {
      "dePart": "Die Hoffnung freuet manchen Mann,der Herzensfreude nie gewann.",
      "enPart": "",
      "category": "preDraft",
      "id": 2434
    },
    "2435": {
      "dePart": "Wer zwei Dinge auf einmal tut,die geraten selten gut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2435
    },
    "2436": {
      "dePart": "Alle Narrheit erschöpfen - so gelangt man zum Boden der Weisheit",
      "enPart": "",
      "category": "preDraft",
      "id": 2436
    },
    "2437": {
      "dePart": "Wenn alles auf Erden vernünftig wäre, würde nichts geschehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2437
    },
    "2438": {
      "dePart": "Wir sollten durch unser Lebendie Erde zum Himmel machen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2438
    },
    "2439": {
      "dePart": "Wer die Welt nicht leicht nimmt,dem macht sie Schwierigkeiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2439
    },
    "2440": {
      "dePart": "Die Freude finden wir nicht in den Dingen, sondern in der Tiefe unserer Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 2440
    },
    "2441": {
      "dePart": "Alle Erfahrung ist ein Gewölbe, auf das man aufbauen kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 2441
    },
    "2442": {
      "dePart": "In Wahrheit spiegelt die Kunst den Zuschauer, nicht das Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2442
    },
    "2443": {
      "dePart": "Was für ein Los wir fürchten müssen,sagt uns das eigene Gewissen",
      "enPart": "",
      "category": "preDraft",
      "id": 2443
    },
    "2444": {
      "dePart": "Als ich des Suchens müde war, erlernte ich das Finden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2444
    },
    "2445": {
      "dePart": "Der beste Teil der Schönheit ist der, den ein Bild nicht wiedergeben kann",
      "enPart": "",
      "category": "preDraft",
      "id": 2445
    },
    "2446": {
      "dePart": "Man kann mit einer Wohnung einen Menschen genauso töten wie mit einer Axt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2446
    },
    "2447": {
      "dePart": "Was Grazie dem Körper ist Klarheit dem Geist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2447
    },
    "2448": {
      "dePart": "Man gibt Ratschläge, aber die Ausführung bringt man keinem bei.",
      "enPart": "",
      "category": "preDraft",
      "id": 2448
    },
    "2449": {
      "dePart": "Erdachtes mag zu denken geben, doch nur Erlebtes wird beleben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2449
    },
    "2450": {
      "dePart": "Auf der Suche nach der Wahrheit darfst du nicht die Lüge nach dem Weg fragen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2450
    },
    "2451": {
      "dePart": "Geizhälse sind gewöhnlich auf nicht allzu viel stolz.",
      "enPart": "",
      "category": "preDraft",
      "id": 2451
    },
    "2452": {
      "dePart": "Alle Wesen erstreben das Gute, doch nicht alle erkennen das Wahre.",
      "enPart": "",
      "category": "preDraft",
      "id": 2452
    },
    "2453": {
      "dePart": "Die Berge die es zu versetzen gilt, sind in unserem Bewustsein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2453
    },
    "2454": {
      "dePart": "Nur dem, der bereut, wird verziehen im Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2454
    },
    "2455": {
      "dePart": "Pünktlichkeit stiehlt uns die beste Zeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2455
    },
    "2456": {
      "dePart": "Bei gleicher Umgebung lebt doch jeder in einer anderen Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2456
    },
    "2457": {
      "dePart": "Man muss, solange man lebt, lernen, wie man leben soll.",
      "enPart": "",
      "category": "preDraft",
      "id": 2457
    },
    "2458": {
      "dePart": "Die Gleichgültigkeit ist eine Lähmung der Seele,ein vorzeitiger Tod.",
      "enPart": "",
      "category": "preDraft",
      "id": 2458
    },
    "2459": {
      "dePart": "Die Selbstsucht stirbt erst eine halbe Stunde nach unserem Tod",
      "enPart": "",
      "category": "preDraft",
      "id": 2459
    },
    "2460": {
      "dePart": "Der Amerikaner, der den Kolumbus zuerst entdeckte, machte eine böse Entdeckung",
      "enPart": "",
      "category": "preDraft",
      "id": 2460
    },
    "2461": {
      "dePart": "Jener Tag, vor dem du zurückschreckst, ist der Geburtstag der Ewigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2461
    },
    "2462": {
      "dePart": "Der Unverstand ist die unbesiegbarste Macht auf Erden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2462
    },
    "2463": {
      "dePart": "Wir sollten das Leben verlassen wie ein Bankett: weder durstig noch betrunken.",
      "enPart": "",
      "category": "preDraft",
      "id": 2463
    },
    "2464": {
      "dePart": "Niemand ist nutzlos in dieser Welt, der einem anderen die Bürde leichter macht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2464
    },
    "2465": {
      "dePart": "Der Verstand kann ein Held sein, die Klugheit ist meistens ein Feigling.",
      "enPart": "",
      "category": "preDraft",
      "id": 2465
    },
    "2466": {
      "dePart": "Jeder lebt hinter einem Gitter, das er mit sich herumträgt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2466
    },
    "2467": {
      "dePart": "Manchmal zahlt man den höchsten Preis für Dinge, die man umsonst bekommt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2467
    },
    "2468": {
      "dePart": "Die Schönheit vergeht, die Blödheit ist ewig.",
      "enPart": "",
      "category": "preDraft",
      "id": 2468
    },
    "2469": {
      "dePart": "Wenn Du die Wahrheit sagst, gibt es nichts, was Du im Kopf behalten müßtest",
      "enPart": "",
      "category": "preDraft",
      "id": 2469
    },
    "2470": {
      "dePart": "Und wenn Ihr Euch nur selbst vertraut, vertrauen Euch die anderen Seelen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2470
    },
    "2471": {
      "dePart": "Hab Mut! Jedoch nicht, um ihn zu beweisen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2471
    },
    "2472": {
      "dePart": "Spielen ist Experimentieren mit dem Zufall.",
      "enPart": "",
      "category": "preDraft",
      "id": 2472
    },
    "2473": {
      "dePart": "Der Friede ist ein Baum,der eines langen Wachstums bedarf.",
      "enPart": "",
      "category": "preDraft",
      "id": 2473
    },
    "2474": {
      "dePart": "Es gibt auch Spiegel, in denen man erkennen kann, was einem fehlt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2474
    },
    "2475": {
      "dePart": "Am bittersten bereuen wir die Fehler, die wir am leichtesten vermieden hätten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2475
    },
    "2476": {
      "dePart": "Alles, was dir begegnen wird, ist leider nicht zu vermeiden",
      "enPart": "",
      "category": "preDraft",
      "id": 2476
    },
    "2477": {
      "dePart": "Wer sich einen Dienst erweisen lässt, steht unter dem, der ihn erweist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2477
    },
    "2478": {
      "dePart": "Rat erbitten: sich den eingeschlagenen Weg von einem anderen bestätigen lassen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2478
    },
    "2479": {
      "dePart": "Jede Handlung setzt ein Motiv voraus, aus dem sie mit Notwendigkeit erfolgt",
      "enPart": "",
      "category": "preDraft",
      "id": 2479
    },
    "2480": {
      "dePart": "Auf dem höchsten Thron der Welt sitzen wir doch nur auf unserem Hintern.",
      "enPart": "",
      "category": "preDraft",
      "id": 2480
    },
    "2481": {
      "dePart": "Das Unverständlichste am Universum ist im Grunde, dass wir es verstehen können.",
      "enPart": "",
      "category": "preDraft",
      "id": 2481
    },
    "2482": {
      "dePart": "Denn wohl ist die Schule das höchste, aber nur dann, wenn sie das Leben ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2482
    },
    "2483": {
      "dePart": "Die Hoffnungslosigkeit ist schon die vorweggenommene Niederlage.",
      "enPart": "",
      "category": "preDraft",
      "id": 2483
    },
    "2484": {
      "dePart": "Gesellschaftlich ist kaum etwas so erfolgreichwie Dummheit mit guten Manieren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2484
    },
    "2485": {
      "dePart": "Hass ist Sache des Herzens, Verachtung des Kopfes.",
      "enPart": "",
      "category": "preDraft",
      "id": 2485
    },
    "2486": {
      "dePart": "Stil ist richtiges Weglassen von Unwichtigem.",
      "enPart": "",
      "category": "preDraft",
      "id": 2486
    },
    "2487": {
      "dePart": "Das glänzendste Geschäft in dieser Welt ist die Moral.",
      "enPart": "",
      "category": "preDraft",
      "id": 2487
    },
    "2488": {
      "dePart": "Zivilisation ist die unablässige Vermehrung unnötiger Notwendigkeiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2488
    },
    "2489": {
      "dePart": "Das menschliche Lebenbeginnt jenseits der Verzweiflung!",
      "enPart": "",
      "category": "preDraft",
      "id": 2489
    },
    "2490": {
      "dePart": "Müde macht uns die Arbeit, die wir liegenlassen, nicht die, die wir tun.",
      "enPart": "",
      "category": "preDraft",
      "id": 2490
    },
    "2491": {
      "dePart": "Während man's aufschiebt, geht das Leben vorüber",
      "enPart": "",
      "category": "preDraft",
      "id": 2491
    },
    "2492": {
      "dePart": "Die Vernünftigen halten bloß durch,die Leidenschaftlichen leben",
      "enPart": "",
      "category": "preDraft",
      "id": 2492
    },
    "2493": {
      "dePart": "Die Weisheit führt uns zur Kindheit zurück.",
      "enPart": "",
      "category": "preDraft",
      "id": 2493
    },
    "2494": {
      "dePart": "Alles Reden ist sinnlos, wenn das Vertrauen fehlt",
      "enPart": "",
      "category": "preDraft",
      "id": 2494
    },
    "2495": {
      "dePart": "Die Güter der Weltgleiten uns durch die Fingerwie der Sand der Dünen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2495
    },
    "2496": {
      "dePart": "Das Schicksal wird im Leben oft \"Zufall\" genannt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2496
    },
    "2497": {
      "dePart": "Es gibt Momente, wo ich finde, dass die Sprache noch gar nichts ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2497
    },
    "2498": {
      "dePart": "Allein sein zu müssen ist schwer - allein sein zu können, ist schön",
      "enPart": "",
      "category": "preDraft",
      "id": 2498
    },
    "2499": {
      "dePart": "Mit einem Menschen, der nur Trümpfe hat, kann man nicht Karten spielen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2499
    },
    "2500": {
      "dePart": "Jede Erscheinung beweist ihre Notwendigkeit durch ihr Dasein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2500
    },
    "2501": {
      "dePart": "Die Sinne betrügen nicht, weil sie gar nicht beurteilen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2501
    },
    "2502": {
      "dePart": "Das Leben ist nicht gerecht, und für die meisten von uns ist das gut so",
      "enPart": "",
      "category": "preDraft",
      "id": 2502
    },
    "2503": {
      "dePart": "Die Größte Sehenswürdigkeit, die es gibt, ist die Welt - sieh sie dir an.",
      "enPart": "",
      "category": "preDraft",
      "id": 2503
    },
    "2504": {
      "dePart": "Trau lieber deiner Kraft als deinem Glück",
      "enPart": "",
      "category": "preDraft",
      "id": 2504
    },
    "2505": {
      "dePart": "Ruhig und vernünftig zu betrachten,ist zu keiner Zeit schädlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 2505
    },
    "2506": {
      "dePart": "Phantasie ist die Gabe,unsichtbare Dinge zu sehen",
      "enPart": "",
      "category": "preDraft",
      "id": 2506
    },
    "2507": {
      "dePart": "Jeder Mensch trägt einen Zauber im Gesicht; irgendeinem gefällt er.",
      "enPart": "",
      "category": "preDraft",
      "id": 2507
    },
    "2508": {
      "dePart": "Für ein gutes Kompliment verkaufe ich weite Teile meiner Verwandtschaft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2508
    },
    "2509": {
      "dePart": "Laß uns das Leben genießen, solange wir es nicht begreifen",
      "enPart": "",
      "category": "preDraft",
      "id": 2509
    },
    "2510": {
      "dePart": "Man soll die Welt nicht belachen, nicht beweinen,sondern begreifen",
      "enPart": "",
      "category": "preDraft",
      "id": 2510
    },
    "2511": {
      "dePart": "Im Kampf gegen die Wirklichkeit hat der Mensch nur eine Waffe: die Fantasie.",
      "enPart": "",
      "category": "preDraft",
      "id": 2511
    },
    "2512": {
      "dePart": "Die Neider sterben, nimmer stirbt der Neid",
      "enPart": "",
      "category": "preDraft",
      "id": 2512
    },
    "2513": {
      "dePart": "Die Gleichgültigkeit ist wie das Eis an den Polen:Sie tötet alles.",
      "enPart": "",
      "category": "preDraft",
      "id": 2513
    },
    "2514": {
      "dePart": "Wer Phantasie hat aber kein Wissen,der hat Flügel aber keine Beine.",
      "enPart": "",
      "category": "preDraft",
      "id": 2514
    },
    "2515": {
      "dePart": "Selbst eine schwere Türhat nur einen kleinen Schlüssel nötig.",
      "enPart": "",
      "category": "preDraft",
      "id": 2515
    },
    "2516": {
      "dePart": "Phantasie ist die Fähigkeit, in Bildern zu denken.",
      "enPart": "",
      "category": "preDraft",
      "id": 2516
    },
    "2517": {
      "dePart": "Das einzige, was die Armut beseitigen kann, ist miteinander zu teilen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2517
    },
    "2518": {
      "dePart": "Es gibt Leute, deren Geist immer Ferien hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2518
    },
    "2519": {
      "dePart": "Wer alle Brücken abbricht, muss schwimmen können.",
      "enPart": "",
      "category": "preDraft",
      "id": 2519
    },
    "2520": {
      "dePart": "Armut und Reichtum wohnen nicht im Hause, sondern im Herzen der Menschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2520
    },
    "2521": {
      "dePart": "Ich habe so selten einmal Zeit zum Träumen und doch so viele Träume",
      "enPart": "",
      "category": "preDraft",
      "id": 2521
    },
    "2522": {
      "dePart": "Jeder Augenblick im Leben ist ein Schritt zum Tode hin.",
      "enPart": "",
      "category": "preDraft",
      "id": 2522
    },
    "2523": {
      "dePart": "Alle Reisen haben eine bestimmte Bestimmung,die der Reisende nicht kennt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2523
    },
    "2524": {
      "dePart": "Laßt uns immer in den großen Traum des Lebens kleine bunte Träume weben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2524
    },
    "2525": {
      "dePart": "Auch Besiegte fasst Wut bisweilen und Tapferkeit",
      "enPart": "",
      "category": "preDraft",
      "id": 2525
    },
    "2526": {
      "dePart": "Schweigen ist der beste Ausweg für den, der seiner Sache nicht sicher ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2526
    },
    "2527": {
      "dePart": "Die Faulen sind stets aufgelegt, irgendetwas zu tun.",
      "enPart": "",
      "category": "preDraft",
      "id": 2527
    },
    "2528": {
      "dePart": "Das Gegenteil von gutist nicht böse,sondern gut gemeint",
      "enPart": "",
      "category": "preDraft",
      "id": 2528
    },
    "2529": {
      "dePart": "Das Schlimmste in allen Dingen, das ist die Unentschlossenheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2529
    },
    "2530": {
      "dePart": "Es gibt nur zwei Todsünden:Die Bosheit und den Egoismus",
      "enPart": "",
      "category": "preDraft",
      "id": 2530
    },
    "2531": {
      "dePart": "Nur die Toten haben das Ende des Krieges gesehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2531
    },
    "2532": {
      "dePart": "Wer nur die Welt umarmt,erfriert an ihrer Gleichgültigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2532
    },
    "2533": {
      "dePart": "Geduld ist ein langsamer, aber verlässlicher Baumeister.",
      "enPart": "",
      "category": "preDraft",
      "id": 2533
    },
    "2534": {
      "dePart": "Man soll nie zuschauen,man soll Zeuge sein und mittunund Verantwortung tragen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2534
    },
    "2535": {
      "dePart": "Das entschieden Charakteristische dieser Welt ist ihre Vergänglichkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2535
    },
    "2536": {
      "dePart": "Lasst uns das Leben geniessen, solange wir es noch nicht begreifen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2536
    },
    "2537": {
      "dePart": "Was immer du tust, irgendwann wirst du es bereuen",
      "enPart": "",
      "category": "preDraft",
      "id": 2537
    },
    "2538": {
      "dePart": "Wie wenig Lärm machen die wirklichen Wunder.",
      "enPart": "",
      "category": "preDraft",
      "id": 2538
    },
    "2539": {
      "dePart": "Die Gerechtigkeit wohnt in einer Etage,zu der die Justiz keinen Zugang hat",
      "enPart": "",
      "category": "preDraft",
      "id": 2539
    },
    "2540": {
      "dePart": "Die liebenswürdigste der Frauen wird immer auch die schönste sein",
      "enPart": "",
      "category": "preDraft",
      "id": 2540
    },
    "2541": {
      "dePart": "Für die Eitelkeit ist selbst die Pfütze ein wohlgefälliger Spiegel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2541
    },
    "2542": {
      "dePart": "Die Illusion, die uns verherrlicht ist uns lieber als zehntausend Wahrheiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2542
    },
    "2543": {
      "dePart": "Oh welcher Zauber liegt in diesem Wort: Daheim!",
      "enPart": "",
      "category": "preDraft",
      "id": 2543
    },
    "2544": {
      "dePart": "Es gibt Menschen, die jeden Tag in einer neuen Auflage erscheinen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2544
    },
    "2545": {
      "dePart": "Das Aussortieren des Unwesentlichenist der Kern aller Lebensweisheit",
      "enPart": "",
      "category": "preDraft",
      "id": 2545
    },
    "2546": {
      "dePart": "Heimat ist da, wo wir verstehen und verstanden werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2546
    },
    "2547": {
      "dePart": "Es ist keine Kunst, geistreich zu sein, wenn man vor nichts Respekt hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2547
    },
    "2548": {
      "dePart": "Melancholie ist das Vergnügen, traurig zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2548
    },
    "2549": {
      "dePart": "Man stirbt nur einmal, und dann für sehr lange Zeit",
      "enPart": "",
      "category": "preDraft",
      "id": 2549
    },
    "2550": {
      "dePart": "Das wunderbarste Märchenist das Leben selbst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2550
    },
    "2551": {
      "dePart": "Die Schwärmerei für die Natur kommt von der Unbewohnbarkeit der Städte",
      "enPart": "",
      "category": "preDraft",
      "id": 2551
    },
    "2552": {
      "dePart": "Die Ungleichheit ist die Ursache aller örtlichen Bewegungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2552
    },
    "2553": {
      "dePart": "Übereifer grenzt an Torheitund führt zu Enttäuschungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2553
    },
    "2554": {
      "dePart": "Ein schlechtes Gewissen ist der schmerzlichste Beweis für ein gutes Gedächtnis",
      "enPart": "",
      "category": "preDraft",
      "id": 2554
    },
    "2555": {
      "dePart": "Der Aufrichtige lacht mit den Augen mehr als mit dem Mund",
      "enPart": "",
      "category": "preDraft",
      "id": 2555
    },
    "2556": {
      "dePart": "Dein Geist gleicht einer Mauer, an der wechselnde Wolken vorüberziehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2556
    },
    "2557": {
      "dePart": "Gewisse Dinge greift man so vergeblich mit Worten an wie Geister mit Waffen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2557
    },
    "2558": {
      "dePart": "Ich hoffe, weiterhin fähig zu bleiben, immer wieder neue Täume zu finden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2558
    },
    "2559": {
      "dePart": "Oft ist ein guter Tod der beste Lebenslauf.",
      "enPart": "",
      "category": "preDraft",
      "id": 2559
    },
    "2560": {
      "dePart": "Je intensiver wir leben,umso mehr strahlen wir Lebensfreude aus.",
      "enPart": "",
      "category": "preDraft",
      "id": 2560
    },
    "2561": {
      "dePart": "Die Wahrheit ist die Tochter der Zeit, nicht der Autorität.",
      "enPart": "",
      "category": "preDraft",
      "id": 2561
    },
    "2562": {
      "dePart": "Du kannst dein Leben nicht verlängern, noch verbreitern, nur vertiefen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2562
    },
    "2563": {
      "dePart": "Ich ärgere mich nicht!Ärger ist ungesund und unwissenschaftlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 2563
    },
    "2564": {
      "dePart": "Wenn deine Grundsätze dich traurig machen,verlass dich drauf: Sie sind falsch.",
      "enPart": "",
      "category": "preDraft",
      "id": 2564
    },
    "2565": {
      "dePart": "Die Notwendigkeit zu entscheiden reicht weiter als die Möglichkeit zu erkennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2565
    },
    "2566": {
      "dePart": "Oft ist der Mensch sich selbst sein größter Feind.",
      "enPart": "",
      "category": "preDraft",
      "id": 2566
    },
    "2567": {
      "dePart": "Es gibt keine reine Wahrheit, aber ebensowenig einen reinen Irrtum.",
      "enPart": "",
      "category": "preDraft",
      "id": 2567
    },
    "2568": {
      "dePart": "Ereignisse sind nur die Schalen der Ideen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2568
    },
    "2569": {
      "dePart": "Das Gesicht ist das Abbild des Hirns, die Augen sein Berichterstatter.",
      "enPart": "",
      "category": "preDraft",
      "id": 2569
    },
    "2570": {
      "dePart": "Verbrecher verlangen Geld oder Leben, Frauen beides.",
      "enPart": "",
      "category": "preDraft",
      "id": 2570
    },
    "2571": {
      "dePart": "An der Maske die wir uns vorbinden, erkennt man uns.",
      "enPart": "",
      "category": "preDraft",
      "id": 2571
    },
    "2572": {
      "dePart": "Erfahrung ist die einzige Schule, in der auch Dummköpfe etwas lernen können.",
      "enPart": "",
      "category": "preDraft",
      "id": 2572
    },
    "2573": {
      "dePart": "Die Mittelmäßigkeit wiegt immer richtig, nur ist ihre Waage falsch.",
      "enPart": "",
      "category": "preDraft",
      "id": 2573
    },
    "2574": {
      "dePart": "Die Anhänger der Entwicklung haben oft eine zu geringe Meinung von Bestehendem.",
      "enPart": "",
      "category": "preDraft",
      "id": 2574
    },
    "2575": {
      "dePart": "Ein Charakter ist ein vollständig gebildeter Wille.",
      "enPart": "",
      "category": "preDraft",
      "id": 2575
    },
    "2576": {
      "dePart": "Von Ruhm und Ehre wird das Herz durchaus nicht satt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2576
    },
    "2577": {
      "dePart": "Ganz dumm ist nicht so schlimm als nur halb aufgeklärt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2577
    },
    "2578": {
      "dePart": "Mit vollen Segeln lief ich in das Meer des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 2578
    },
    "2579": {
      "dePart": "Kein gutes Buch oder irgend etwas gutes zeigt seine gute Seite zuerst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2579
    },
    "2580": {
      "dePart": "Rede sanft, aber trage einen großen Stock bei dir.",
      "enPart": "",
      "category": "preDraft",
      "id": 2580
    },
    "2581": {
      "dePart": "Vertrauen wird dadurch erschöpft, dass es in Anspruch genommen wird.",
      "enPart": "",
      "category": "preDraft",
      "id": 2581
    },
    "2582": {
      "dePart": "Verachtung ist schlechtgelaunter Egoismus.",
      "enPart": "",
      "category": "preDraft",
      "id": 2582
    },
    "2583": {
      "dePart": "Flüchtiger als Wind und Welle flieht die Zeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2583
    },
    "2584": {
      "dePart": "Nichts kommt einem doch teurer in der Welt zu stehen als die Humanität.",
      "enPart": "",
      "category": "preDraft",
      "id": 2584
    },
    "2585": {
      "dePart": "Die schönsten Träume von Freiheit werden im Kerker geträumt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2585
    },
    "2586": {
      "dePart": "Das Leben ist nur ein Moment, der Tod ist auch nur einer.",
      "enPart": "",
      "category": "preDraft",
      "id": 2586
    },
    "2587": {
      "dePart": "Wo der Verstand anfängt, hört der Mensch auf, Mensch zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2587
    },
    "2588": {
      "dePart": "Die Gefahr verschont denjenigen, der ihr die Stirn bietet.",
      "enPart": "",
      "category": "preDraft",
      "id": 2588
    },
    "2589": {
      "dePart": "Alles, was lediglich wahrscheinlich ist, ist wahrscheinlich falsch.",
      "enPart": "",
      "category": "preDraft",
      "id": 2589
    },
    "2590": {
      "dePart": "Tapferkeit heißt, der einzige zu sein der weiß, dass man Angst hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2590
    },
    "2591": {
      "dePart": "Die Leidenschaften sind die Pforten der Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 2591
    },
    "2592": {
      "dePart": "Um der Zukunft willen soll der Mensch die Vergangenheit hochhalten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2592
    },
    "2593": {
      "dePart": "Der Mensch ist am phantasievollsten in seinen Rechtfertigungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2593
    },
    "2594": {
      "dePart": "Sich immer treu bleiben ist eine beliebte Devise für Gewohnheitsmenschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2594
    },
    "2595": {
      "dePart": "Wahres Leben ist Lauschen des Herzensauf die Klänge der Ewigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2595
    },
    "2596": {
      "dePart": "Meinung wird letztlich durch Gefühle und nicht durch den Intellekt bestimmt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2596
    },
    "2597": {
      "dePart": "Ohr und Geist können müde werden, dasselbe zu hören, das Herz nie.",
      "enPart": "",
      "category": "preDraft",
      "id": 2597
    },
    "2598": {
      "dePart": "Das Gewissen ist die Wunde, die nie heilt und an der keiner stirbt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2598
    },
    "2599": {
      "dePart": "Zufall ist ein Wort ohne Sinn – nichts kann ohne Ursache existieren",
      "enPart": "",
      "category": "preDraft",
      "id": 2599
    },
    "2600": {
      "dePart": "Es gibt Wichtigeres im Leben, als sein Tempo zu beschleunigen",
      "enPart": "",
      "category": "preDraft",
      "id": 2600
    },
    "2601": {
      "dePart": "Je verbreiteter die Geselligkeit, desto frostiger die Herzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2601
    },
    "2602": {
      "dePart": "Alle Kraft, die wir fortgeben, kommt erfahren und verwandelt wieder über uns.",
      "enPart": "",
      "category": "preDraft",
      "id": 2602
    },
    "2603": {
      "dePart": "Der Körper ist der Übersetzer der Seele ins Sichtbare.",
      "enPart": "",
      "category": "preDraft",
      "id": 2603
    },
    "2604": {
      "dePart": "Hüte dich vor dem Entschluß, zu dem du nicht lächeln kannst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2604
    },
    "2605": {
      "dePart": "Der Mensch ist ein Blinder,der vom Sehen träumt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2605
    },
    "2606": {
      "dePart": "Niemand liebt das Leben so wie einer, der alt ist",
      "enPart": "",
      "category": "preDraft",
      "id": 2606
    },
    "2607": {
      "dePart": "Armut ist in der Stadt groß, doch die Faulheit noch viel größer.",
      "enPart": "",
      "category": "preDraft",
      "id": 2607
    },
    "2608": {
      "dePart": "Erst die Fremde lehrt uns, was wir an der Heimat besitzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2608
    },
    "2609": {
      "dePart": "Jede Ordnung ist der erste Schritt auf dem Weg in neuerliches Chaos.",
      "enPart": "",
      "category": "preDraft",
      "id": 2609
    },
    "2610": {
      "dePart": "Es gibt nichts so Grauenvolles wie die Fremdheit derer, die sich kennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2610
    },
    "2611": {
      "dePart": "Das Gewissen ist unser bester und zuverlässigster Wegweiser",
      "enPart": "",
      "category": "preDraft",
      "id": 2611
    },
    "2612": {
      "dePart": "Je länger man vor der Tür zögert, desto fremder wird man.",
      "enPart": "",
      "category": "preDraft",
      "id": 2612
    },
    "2613": {
      "dePart": "Das ganze Leben ist ein ewiges Wiederanfangen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2613
    },
    "2614": {
      "dePart": "Der Zufall ist der einzig legitime Herrscher des Universums.",
      "enPart": "",
      "category": "preDraft",
      "id": 2614
    },
    "2615": {
      "dePart": "Wer ein WOFÜR im Leben hat, der kann fast jedes WIE ertragen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2615
    },
    "2616": {
      "dePart": "Wer die Dummköpfe gegen sich hat, verdient Vertrauen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2616
    },
    "2617": {
      "dePart": "Je mehr ein Mensch sich schämt, desto anständiger ist er.",
      "enPart": "",
      "category": "preDraft",
      "id": 2617
    },
    "2618": {
      "dePart": "Nur große Seelen lieben die Einsamkeit und können sie vertragen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2618
    },
    "2619": {
      "dePart": "Das Gegenteil von Spiel ist nicht Ernst, sondern Wirklichkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2619
    },
    "2620": {
      "dePart": "Die Seele nährt sich von dem, worüber sie sich freut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2620
    },
    "2621": {
      "dePart": "Das Leben ist wie ein Buch,und wer nicht reist,liest nur ein wenig davon.",
      "enPart": "",
      "category": "preDraft",
      "id": 2621
    },
    "2622": {
      "dePart": "Hoffnung ist ein gutes Frühstück,aber ein schlechtes Abendbrot.",
      "enPart": "",
      "category": "preDraft",
      "id": 2622
    },
    "2623": {
      "dePart": "Tugend ist wie ein kostbarer Stein - am besten in einfacher Fassung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2623
    },
    "2624": {
      "dePart": "Man soll das Feuer in seiner Seele nie ausgehen lassen, sondern es schüren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2624
    },
    "2625": {
      "dePart": "Um sich selbst zu erkennen, muss man handeln",
      "enPart": "",
      "category": "preDraft",
      "id": 2625
    },
    "2626": {
      "dePart": "Leben heißt träumen; weise sein heißt angenehm träumen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2626
    },
    "2627": {
      "dePart": "Versuchungen bekämpft man am besten mit Geldmangel und Rheumatismus",
      "enPart": "",
      "category": "preDraft",
      "id": 2627
    },
    "2628": {
      "dePart": "Träumen muss nicht heissen,die Augen vor der Wirklichkeit zu verschließen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2628
    },
    "2629": {
      "dePart": "Wird Stolz sich seiner bewußt, so ist er Eitelkeit",
      "enPart": "",
      "category": "preDraft",
      "id": 2629
    },
    "2630": {
      "dePart": "Um manche Delikte zu begreifen, genügt es, die Opfer zu kennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2630
    },
    "2631": {
      "dePart": "Die Frau, die sich ertappen läßt, verdient ihr Schicksal.",
      "enPart": "",
      "category": "preDraft",
      "id": 2631
    },
    "2632": {
      "dePart": "Ein Mensch, der für nichts zu Sterben gewillt ist, verdient es nicht zu leben",
      "enPart": "",
      "category": "preDraft",
      "id": 2632
    },
    "2633": {
      "dePart": "Schönheit ist Ewigkeit,die sich in einem Spiegel anblickt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2633
    },
    "2634": {
      "dePart": "Unsichtbar wird die Dummheit,wenn sie genügend grosse Ausmasse angenommen hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2634
    },
    "2635": {
      "dePart": "Auch die Augen haben ihr tägliches Brot: den Himmel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2635
    },
    "2636": {
      "dePart": "Manche Menschen leben bloß für eine gute Grabinschrift.",
      "enPart": "",
      "category": "preDraft",
      "id": 2636
    },
    "2637": {
      "dePart": "Das große unzerstörbare Wunder ist der Menschenglaube an Wunder.",
      "enPart": "",
      "category": "preDraft",
      "id": 2637
    },
    "2638": {
      "dePart": "Kraft ist die Moral der Menschen, die sich vor anderen auszeichnen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2638
    },
    "2639": {
      "dePart": "Die Heimat des Abenteuers ist die Fremde.",
      "enPart": "",
      "category": "preDraft",
      "id": 2639
    },
    "2640": {
      "dePart": "Freue dich mit mir!Es ist so traurig, sich allein zu freuen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2640
    },
    "2641": {
      "dePart": "Es gibt ein Ziel, aber keinen Weg, das was wir Weg nennen ist Zögern.",
      "enPart": "",
      "category": "preDraft",
      "id": 2641
    },
    "2642": {
      "dePart": "Gewohnheit, Sitte und Brauch sind stärker als die Wahrheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2642
    },
    "2643": {
      "dePart": "Trauet Euren Träumen, denn das Tor der Ewigkeit ist darin verborgen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2643
    },
    "2644": {
      "dePart": "Gedächtnis ist ein Tagebuch, das wir immer mit uns herumtragen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2644
    },
    "2645": {
      "dePart": "Es sind die Begegnungen mit Menschen, die das Leben lebenswert machen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2645
    },
    "2646": {
      "dePart": "Mich reuen die Sünden, die ich nicht beging.",
      "enPart": "",
      "category": "preDraft",
      "id": 2646
    },
    "2647": {
      "dePart": "Überall geht ein früheres Ahnen dem späteren Wissen voraus.",
      "enPart": "",
      "category": "preDraft",
      "id": 2647
    },
    "2648": {
      "dePart": "Etwas muß er sein Eigen nennen, oder der Mensch wird morden und brennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2648
    },
    "2649": {
      "dePart": "Wer die Wahrheit nicht weiß, ist ein Dummkopf.",
      "enPart": "",
      "category": "preDraft",
      "id": 2649
    },
    "2650": {
      "dePart": "Wer das Licht der Welt erblickt,wird das Dunkel schon noch kennen lernen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2650
    },
    "2651": {
      "dePart": "In der Fremde erfährt man, was die Heimat wert ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2651
    },
    "2652": {
      "dePart": "Früher litten wir an Verbrechen, heute an Gesetzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2652
    },
    "2653": {
      "dePart": "Gewisse Dinge greift man so vergeblich mit Worten an, wie Geister mit Waffen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2653
    },
    "2654": {
      "dePart": "Nur ein mittelmäßiger Mensch ist immer in Hochform.",
      "enPart": "",
      "category": "preDraft",
      "id": 2654
    },
    "2655": {
      "dePart": "Der Montag ist in christlichen Ländern der Tag nach dem Fussballspiel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2655
    },
    "2656": {
      "dePart": "Den Willigen führt das Schicksal, den Unwilligen reißt es hinfort.",
      "enPart": "",
      "category": "preDraft",
      "id": 2656
    },
    "2657": {
      "dePart": "Es ist besser, ein junger Spatz zu sein als ein alter Paradiesvogel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2657
    },
    "2658": {
      "dePart": "Wenn auf der Erde die Liebe herrschte, wären alle Gesetze entbehrlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 2658
    },
    "2659": {
      "dePart": "Unsere Zeit krankt daran, dass es zu wenige Menschen gibt und zuviele Leute.",
      "enPart": "",
      "category": "preDraft",
      "id": 2659
    },
    "2660": {
      "dePart": "In Fragen der Moral hilft Schulbildung nicht viel.",
      "enPart": "",
      "category": "preDraft",
      "id": 2660
    },
    "2661": {
      "dePart": "Wer zu geschäftig Gutes tut, hat nicht die Zeit, gut zu sein",
      "enPart": "",
      "category": "preDraft",
      "id": 2661
    },
    "2662": {
      "dePart": "Nur wer von Herzen negativ denkt, kann positiv überrascht werden",
      "enPart": "",
      "category": "preDraft",
      "id": 2662
    },
    "2663": {
      "dePart": "Umändern kann sich niemand, bessern jeder.",
      "enPart": "",
      "category": "preDraft",
      "id": 2663
    },
    "2664": {
      "dePart": "Unsere Wirrnisse sind seit jeher Teil unserer Reichtümer gewesen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2664
    },
    "2665": {
      "dePart": "Unser Schicksal hängt nicht von den Sternen ab,sondern von unserem Handeln.",
      "enPart": "",
      "category": "preDraft",
      "id": 2665
    },
    "2666": {
      "dePart": "Einen Unscheinbaren glaubt man keine Kraft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2666
    },
    "2667": {
      "dePart": "Der Schmerz ist ein Eigentum,wie das Glück und die Freude.",
      "enPart": "",
      "category": "preDraft",
      "id": 2667
    },
    "2668": {
      "dePart": "Der entscheidende Augenblick der menschlichen Entwicklung ist immerwährend.",
      "enPart": "",
      "category": "preDraft",
      "id": 2668
    },
    "2669": {
      "dePart": "Ich liebe den Tanz,denn er befreit den Menschenvon der Schwere der Dinge.",
      "enPart": "",
      "category": "preDraft",
      "id": 2669
    },
    "2670": {
      "dePart": "Hass ist die Rache des Feiglings dafür, dass er eingeschüchtert ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2670
    },
    "2671": {
      "dePart": "Nur auf dem Pfad der Nacht erreicht man die Morgenröte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2671
    },
    "2672": {
      "dePart": "Das einzig Konsequente an mir ist meine ewige Inkonsequenz.",
      "enPart": "",
      "category": "preDraft",
      "id": 2672
    },
    "2673": {
      "dePart": "Eine große Stadt bedeutet große Einsamkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2673
    },
    "2674": {
      "dePart": "Der Mensch ist nur ganz Mensch, wo er spielt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2674
    },
    "2675": {
      "dePart": "Das schönste, was wir erleben können, ist das Geheimnisvolle.",
      "enPart": "",
      "category": "preDraft",
      "id": 2675
    },
    "2676": {
      "dePart": "Vergiss nie, was ein Mensch zu dir sagt, wenn er wütend ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2676
    },
    "2677": {
      "dePart": "Nicht vom Geben, sondern vom Behalten werden wir krank.",
      "enPart": "",
      "category": "preDraft",
      "id": 2677
    },
    "2678": {
      "dePart": "Das, was dem Leben Sinn verleiht,gibt auch dem Tod Sinn.",
      "enPart": "",
      "category": "preDraft",
      "id": 2678
    },
    "2679": {
      "dePart": "Wer einen Menschen bessern will,muss ihn erst einmal respektieren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2679
    },
    "2680": {
      "dePart": "Man kann mit dem Leben mehr anfangen,als es nur immer schneller zu leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2680
    },
    "2681": {
      "dePart": "Unwahrheit zerstört die Seele;Wahrheit stärkt sie.",
      "enPart": "",
      "category": "preDraft",
      "id": 2681
    },
    "2682": {
      "dePart": "Diese Welt ist ständig durch zwei Dinge bedroht, Durch Ordnung und Unordnung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2682
    },
    "2683": {
      "dePart": "Gar sehr verzwickt ist diese Welt mich wundert's dass sie wem gefällt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2683
    },
    "2684": {
      "dePart": "Die Erinnerung ist ein Friedhof, worauf mehr Kreuze als Blumen stehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2684
    },
    "2685": {
      "dePart": "Wer einen Unterschied zwischen Leib und Seele macht, besitzt keines von beiden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2685
    },
    "2686": {
      "dePart": "Man ist niemals so, wie man es mit allen Fasern seines Herzens sein möchte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2686
    },
    "2687": {
      "dePart": "Man verachtet nur dort,wo man nicht stark genug ist, zu lachen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2687
    },
    "2688": {
      "dePart": "Gewissen ist das Bewußtsein eines inneren Gerichtshofes im Menschen",
      "enPart": "",
      "category": "preDraft",
      "id": 2688
    },
    "2689": {
      "dePart": "Wer in sich selbst beruhigt ist,der beunruhigt auch den anderen nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2689
    },
    "2690": {
      "dePart": "Keine Reue ist so schmerzlich wie die vergebliche.",
      "enPart": "",
      "category": "preDraft",
      "id": 2690
    },
    "2691": {
      "dePart": "Die größte Sünde gegen den menschlichen Geist ist, Dinge ohne Beweis zu glauben",
      "enPart": "",
      "category": "preDraft",
      "id": 2691
    },
    "2692": {
      "dePart": "Schändlich ist es, wenn deine Seele ermüdet, bevor der Leib müde ist",
      "enPart": "",
      "category": "preDraft",
      "id": 2692
    },
    "2693": {
      "dePart": "In einem aufgeräumten Zimmer ist auch die Seele aufgeräumt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2693
    },
    "2694": {
      "dePart": "Auch aus einem bescheidenen Winkelkann man in den Himmel springen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2694
    },
    "2695": {
      "dePart": "Das Allerschönste am ganzen Leben sind seine Illusionen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2695
    },
    "2696": {
      "dePart": "Wenn weise Männer nicht irrten, müssten Narren verzweifeln",
      "enPart": "",
      "category": "preDraft",
      "id": 2696
    },
    "2697": {
      "dePart": "Man versehe mich mit Luxus, auf alles Notwendige kann ich verzichten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2697
    },
    "2698": {
      "dePart": "Sage nicht, du kennst einen Menschen,bevor du ein Erbe mit ihm geteilt hast.",
      "enPart": "",
      "category": "preDraft",
      "id": 2698
    },
    "2699": {
      "dePart": "Nur Feigen, die sich selbst verlassen, legt die Verzweiflung Fesseln an.",
      "enPart": "",
      "category": "preDraft",
      "id": 2699
    },
    "2700": {
      "dePart": "Wenn wir alt werden, verkriecht sich die Schönheit nach innen",
      "enPart": "",
      "category": "preDraft",
      "id": 2700
    },
    "2701": {
      "dePart": "Ein Spiegel ist besser als eine ganze Reihe von Ahnenbildern",
      "enPart": "",
      "category": "preDraft",
      "id": 2701
    },
    "2702": {
      "dePart": "Das Leben zu Ende leben ist kein Kinderspiel",
      "enPart": "",
      "category": "preDraft",
      "id": 2702
    },
    "2703": {
      "dePart": "Freuden sind unsere Flügel,Schmerzen unsere Sporen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2703
    },
    "2704": {
      "dePart": "Alle Untugenden, die gerade modern sind, gelten als Tugenden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2704
    },
    "2705": {
      "dePart": "Kein Vormarsch ist so schwer wie der zurück zur Vernunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2705
    },
    "2706": {
      "dePart": "Das Schicksal nimmt nichts, was es nicht auch gegeben hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2706
    },
    "2707": {
      "dePart": "Wo man schreit, ist keine klare Erkenntnis.",
      "enPart": "",
      "category": "preDraft",
      "id": 2707
    },
    "2708": {
      "dePart": "Verzweiflung ist die Schlussfolgerung der Narren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2708
    },
    "2709": {
      "dePart": "Ein zur Unzeit gesprochenes Wortkann ein ganzes Leben umstürzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2709
    },
    "2710": {
      "dePart": "Jeder sehnt sich nun einmal nach dem, was nicht für ihn bestimmt ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2710
    },
    "2711": {
      "dePart": "Manche können nur fremde Meinungen, nicht ihre eigenen berichtigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2711
    },
    "2712": {
      "dePart": "Der größte Verlust für das Lebenist das Hinausschieben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2712
    },
    "2713": {
      "dePart": "Das Beste, was man in der Welt haben kann,ist daheim zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2713
    },
    "2714": {
      "dePart": "Ohne ein bißchen Unordnung ist das Leben langeweilig.",
      "enPart": "",
      "category": "preDraft",
      "id": 2714
    },
    "2715": {
      "dePart": "Nicht was der Mensch ist, nur was er tut, ist sein unverlierbares Eigentum.",
      "enPart": "",
      "category": "preDraft",
      "id": 2715
    },
    "2716": {
      "dePart": "Der Kompromiß ist ein guter Regenschirm, aber kein Dach.",
      "enPart": "",
      "category": "preDraft",
      "id": 2716
    },
    "2717": {
      "dePart": "Wahrheit ist das Erhabenste, drum kann man's nicht jedem auf die Nase binden",
      "enPart": "",
      "category": "preDraft",
      "id": 2717
    },
    "2718": {
      "dePart": "Wenn Gott auf der 5-Tage-Woche bestanden hätte, gäbe es heute keine Menschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2718
    },
    "2719": {
      "dePart": "Kultur ist nur ein dünnes Apfelhäutchenüber einem glühenden Chaos",
      "enPart": "",
      "category": "preDraft",
      "id": 2719
    },
    "2720": {
      "dePart": "Der Mensch weiß wohl um das Gute, auch wenn er es nicht tut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2720
    },
    "2721": {
      "dePart": "Man muss seine Überlegenheit mit ständiger Wachsamkeit erkaufen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2721
    },
    "2722": {
      "dePart": "Der Mensch würde nicht verzweifeln, wenn er nicht hoffen würde.",
      "enPart": "",
      "category": "preDraft",
      "id": 2722
    },
    "2723": {
      "dePart": "Die Hälfte dessen, was man schreibt, ist schädlich, die andere Hälfte unnütz.",
      "enPart": "",
      "category": "preDraft",
      "id": 2723
    },
    "2724": {
      "dePart": "Wer Freiheit zugunsten von Sicherheit aufgibt,hat keines von beiden verdient.",
      "enPart": "",
      "category": "preDraft",
      "id": 2724
    },
    "2725": {
      "dePart": "Man kann für eine Empfindung nie genug bezahlen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2725
    },
    "2726": {
      "dePart": "Erfahrung nennt man die Summe aller unserer Irrtümer.",
      "enPart": "",
      "category": "preDraft",
      "id": 2726
    },
    "2727": {
      "dePart": "Schweigen ist eine der großen Künste der Konversation.",
      "enPart": "",
      "category": "preDraft",
      "id": 2727
    },
    "2728": {
      "dePart": "Man kann eine Idee durch eine andere verdrängen, ur die der Freiheit nicht",
      "enPart": "",
      "category": "preDraft",
      "id": 2728
    },
    "2729": {
      "dePart": "Der grösste Feind des Rechtes ist das Vorrecht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2729
    },
    "2730": {
      "dePart": "Der Neid ist unversöhnlicher als der Hass.",
      "enPart": "",
      "category": "preDraft",
      "id": 2730
    },
    "2731": {
      "dePart": "Leiden, Irrtum und Widerstandskrafthalten das Leben lebendig.",
      "enPart": "",
      "category": "preDraft",
      "id": 2731
    },
    "2732": {
      "dePart": "Mensch, was du liebst, in das wirst du verwandelt werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2732
    },
    "2733": {
      "dePart": "Frühere Zeiten hält man immer für die besseren.",
      "enPart": "",
      "category": "preDraft",
      "id": 2733
    },
    "2734": {
      "dePart": "Viele Menschen verstehen unter Denken nur die Umgruppierung ihrer Vorurteile.",
      "enPart": "",
      "category": "preDraft",
      "id": 2734
    },
    "2735": {
      "dePart": "Aus vielen Worten entspringt ebensoviel Gelegenheit zum Mißverständnis.",
      "enPart": "",
      "category": "preDraft",
      "id": 2735
    },
    "2736": {
      "dePart": "Ehrgeiz ist Habsucht auf Stelzen und maskiert.",
      "enPart": "",
      "category": "preDraft",
      "id": 2736
    },
    "2737": {
      "dePart": "Worte sind die mächtigste Droge, welche die Menschheit benutzt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2737
    },
    "2738": {
      "dePart": "Der bessere Teil der Tapferkeitist Vorsicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2738
    },
    "2739": {
      "dePart": "Nachts erst ist's schön, ans Licht zu glauben",
      "enPart": "",
      "category": "preDraft",
      "id": 2739
    },
    "2740": {
      "dePart": "Die Welt gehört dem, der in ihr mit Heiterkeit und nach hohen Zielen wandert.",
      "enPart": "",
      "category": "preDraft",
      "id": 2740
    },
    "2741": {
      "dePart": "Dem Blöden fährt bei jedem sinnvollen Wort der Schrecken in die Glieder.",
      "enPart": "",
      "category": "preDraft",
      "id": 2741
    },
    "2742": {
      "dePart": "Gerechtigkeit ist schwerer als Hingebung und Liebe",
      "enPart": "",
      "category": "preDraft",
      "id": 2742
    },
    "2743": {
      "dePart": "Schönen Mädchen ist es erlaubt, auf ihre Gabe stolz zu sein",
      "enPart": "",
      "category": "preDraft",
      "id": 2743
    },
    "2744": {
      "dePart": "Der Charakter ist weiter nichts als eine langwierige Gewohnheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2744
    },
    "2745": {
      "dePart": "Die ganze gegenwärtige Zeit ist ein Punkt der Ewigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2745
    },
    "2746": {
      "dePart": "Ein Mensch ohne Kenntnis, eine Welt im Finstern.",
      "enPart": "",
      "category": "preDraft",
      "id": 2746
    },
    "2747": {
      "dePart": "Begeist'rung ist keine Heringsware,die man einpökelt auf einige Jahre.",
      "enPart": "",
      "category": "preDraft",
      "id": 2747
    },
    "2748": {
      "dePart": "Das Gesetz der Geschichte lautet: Steh auf, damit ich mich setzen kann. -",
      "enPart": "",
      "category": "preDraft",
      "id": 2748
    },
    "2749": {
      "dePart": "Wer einst den Blitz zu zünden hat,muss lange Wolke sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2749
    },
    "2750": {
      "dePart": "Nur dem Fröhlichen blüht der Baum des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 2750
    },
    "2751": {
      "dePart": "Nur Dummköpfe und Spinnen produzieren aus sich selber heraus.",
      "enPart": "",
      "category": "preDraft",
      "id": 2751
    },
    "2752": {
      "dePart": "Wer viel redet, glaubt am Ende, was er sagt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2752
    },
    "2753": {
      "dePart": "Erfahrungen sind Wegweiser - keine Lagerplätze",
      "enPart": "",
      "category": "preDraft",
      "id": 2753
    },
    "2754": {
      "dePart": "Ein ehrlicher Tod ist besser als ein schändliches Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2754
    },
    "2755": {
      "dePart": "Unwahrheit ist so einfach, Wahrheit so schwierig.",
      "enPart": "",
      "category": "preDraft",
      "id": 2755
    },
    "2756": {
      "dePart": "Es gibt bestimmte Fehler,welche - gut dargestellt -besser glänzen als Tugenden!",
      "enPart": "",
      "category": "preDraft",
      "id": 2756
    },
    "2757": {
      "dePart": "Der faule Mensch ist egoistisch, auch mit gutem Herzen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2757
    },
    "2758": {
      "dePart": "Furcht, Sehnsucht, Hoffnung drängen uns in die Zukunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2758
    },
    "2759": {
      "dePart": "Auf die Frage \"Was ist Freiheit?\" antwortet ein Weiser: \"Ein gutes Gewissen\".",
      "enPart": "",
      "category": "preDraft",
      "id": 2759
    },
    "2760": {
      "dePart": "Der Idealismus wächst mit der Entfernung vom Problem",
      "enPart": "",
      "category": "preDraft",
      "id": 2760
    },
    "2761": {
      "dePart": "Jede Stunde, die wir leben, ist eine Sterbestunde für irgendwen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2761
    },
    "2762": {
      "dePart": "Das Leben der Gefühlvollen hat drei Epochen: Erwartung, Vergessen, Erinnerung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2762
    },
    "2763": {
      "dePart": "Über seine Verhältnisse kann man nur leben, wenn man welche hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2763
    },
    "2764": {
      "dePart": "Ehrgeiz und Lust sprechen oft dieselbe Sprache.",
      "enPart": "",
      "category": "preDraft",
      "id": 2764
    },
    "2765": {
      "dePart": "Betrachte den Traum wie das Gebetdeiner dir selbst entflohenen Seele",
      "enPart": "",
      "category": "preDraft",
      "id": 2765
    },
    "2766": {
      "dePart": "Erfahrung ist fast immer eine Parodie auf die Idee",
      "enPart": "",
      "category": "preDraft",
      "id": 2766
    },
    "2767": {
      "dePart": "Auch wenn man nur zum Fenster hinaus schaut, sieht man die Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2767
    },
    "2768": {
      "dePart": "Vernunft verhält sich zum Verstande wie ein Kochbuch zu einer Pastete",
      "enPart": "",
      "category": "preDraft",
      "id": 2768
    },
    "2769": {
      "dePart": "Oft nennt man Güte, was nur ein Mangel an Geistesgegenwart ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2769
    },
    "2770": {
      "dePart": "Vom Wahrsagen lässt sich wohl leben in der Welt, aber nicht vom Wahrheit sagen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2770
    },
    "2771": {
      "dePart": "Selbsterkenntnis ist der erste Schritt zur Heuchelei.",
      "enPart": "",
      "category": "preDraft",
      "id": 2771
    },
    "2772": {
      "dePart": "Zum Mitleiden gab die Natur vielen ein Talent,zur Mitfreude nur wenigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2772
    },
    "2773": {
      "dePart": "Schönheit ist der Sinn der Welt.Schönheit genießen heißt: die Welt verstehen",
      "enPart": "",
      "category": "preDraft",
      "id": 2773
    },
    "2774": {
      "dePart": "Wie mutig man ist,weiß man immer erst nachher",
      "enPart": "",
      "category": "preDraft",
      "id": 2774
    },
    "2775": {
      "dePart": "Die Sprache ist ein Gewölke,an dem jede Phantasie ein anderes Gebilde erblickt",
      "enPart": "",
      "category": "preDraft",
      "id": 2775
    },
    "2776": {
      "dePart": "Der Wunsch ist ein Wille, der sich selbst nicht ganz ernst nimmt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2776
    },
    "2777": {
      "dePart": "Dem Verschwender fehlt viel,dem Geizigen alles.",
      "enPart": "",
      "category": "preDraft",
      "id": 2777
    },
    "2778": {
      "dePart": "Der Mensch muss das Gute und Große wollen, das Übrige hängt vom Schicksal ab.",
      "enPart": "",
      "category": "preDraft",
      "id": 2778
    },
    "2779": {
      "dePart": "Das Gestern ist fort, das Morgen nicht da. Leb' also heute!",
      "enPart": "",
      "category": "preDraft",
      "id": 2779
    },
    "2780": {
      "dePart": "In dieser Welt hat nur die Unbeständigkeit Bestand.",
      "enPart": "",
      "category": "preDraft",
      "id": 2780
    },
    "2781": {
      "dePart": "Es gibt Dinge, die jeder leicht haben kann, er muß sich nur danach bücken",
      "enPart": "",
      "category": "preDraft",
      "id": 2781
    },
    "2782": {
      "dePart": "Phlegmatische Naturen sind nur so zu begeistern, dass man sie fanatisiert.",
      "enPart": "",
      "category": "preDraft",
      "id": 2782
    },
    "2783": {
      "dePart": "Alles, was menschlich ist, muß rückwärts gehen,wenn es nicht vorwärts geht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2783
    },
    "2784": {
      "dePart": "Jedes Schreckbild verschwindet, wenn man es fest ins Auge faßt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2784
    },
    "2785": {
      "dePart": "Die Alten sind die einzigen Alten, die nie alt werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2785
    },
    "2786": {
      "dePart": "Ordnung führt zu allen Tugenden. Was aber führt zur Ordnung?",
      "enPart": "",
      "category": "preDraft",
      "id": 2786
    },
    "2787": {
      "dePart": "Je jünger, einfacher und frömmer die Völker, desto mehr Tierliebe.",
      "enPart": "",
      "category": "preDraft",
      "id": 2787
    },
    "2788": {
      "dePart": "Wer ohne Murren sich dem Schicksal unterwirft, ist bei uns weise.",
      "enPart": "",
      "category": "preDraft",
      "id": 2788
    },
    "2789": {
      "dePart": "Der Hass ist ein fruchtbares, der Neid ein steriles Laster.",
      "enPart": "",
      "category": "preDraft",
      "id": 2789
    },
    "2790": {
      "dePart": "Wer Vertrauen hat,erlebt jeden Tag Wunder.",
      "enPart": "",
      "category": "preDraft",
      "id": 2790
    },
    "2791": {
      "dePart": "Was mit Gewalt gewonnen wird, hat keine Dauer",
      "enPart": "",
      "category": "preDraft",
      "id": 2791
    },
    "2792": {
      "dePart": "Kalte Umschläge sind besser als warme Ratschläge.",
      "enPart": "",
      "category": "preDraft",
      "id": 2792
    },
    "2793": {
      "dePart": "Man nimmt die unerklärte dunkle Sache wichtiger als die erklärte helle.",
      "enPart": "",
      "category": "preDraft",
      "id": 2793
    },
    "2794": {
      "dePart": "Das beste aller Hausmittel ist eine gute Hausfrau.",
      "enPart": "",
      "category": "preDraft",
      "id": 2794
    },
    "2795": {
      "dePart": "Gemeinsame Erinnerungen sind manchmal die besten Friedensstifter.",
      "enPart": "",
      "category": "preDraft",
      "id": 2795
    },
    "2796": {
      "dePart": "Wenn man viel hineinzustecken hat, so hat ein Tag hundert Taschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2796
    },
    "2797": {
      "dePart": "Man kann nicht für seinen Mut einstehen, wenn man nie in Gefahr gewesen ist",
      "enPart": "",
      "category": "preDraft",
      "id": 2797
    },
    "2798": {
      "dePart": "Wie viele Male schaut der Wille durch's Fenster,ehe die Tat durch's Tor geht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2798
    },
    "2799": {
      "dePart": "Eine Seele ohne Phantasieist wie eine Sternwarte ohne Teleskop.",
      "enPart": "",
      "category": "preDraft",
      "id": 2799
    },
    "2800": {
      "dePart": "Alle Dinge müssen; der Mensch allein ist das Wesen, welches will.",
      "enPart": "",
      "category": "preDraft",
      "id": 2800
    },
    "2801": {
      "dePart": "Wer still und unbeobachtet gelebt hat, der hat gut gelebt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2801
    },
    "2802": {
      "dePart": "Nüchtern sein und zweifeln, das ist der Kern der Weisheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2802
    },
    "2803": {
      "dePart": "Das Leben ist eine Brücke von Seufzern über einen Strom von Tränen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2803
    },
    "2804": {
      "dePart": "Überall wo Grenzen sind, wird geschmuggelt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2804
    },
    "2805": {
      "dePart": "Die Mode ist eine charmante Tyrannei von kurzer Dauer.",
      "enPart": "",
      "category": "preDraft",
      "id": 2805
    },
    "2806": {
      "dePart": "Rechtzeitige und vorsorgliche Angst ist die Mutter der Sicherheit",
      "enPart": "",
      "category": "preDraft",
      "id": 2806
    },
    "2807": {
      "dePart": "Für trügende Hoffnungen entschädigen oft trügende Erinnerungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2807
    },
    "2808": {
      "dePart": "Jedes Ding wird mit mehr Genuss erjagt als genossen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2808
    },
    "2809": {
      "dePart": "In der Mitte wirst du am sichersten gehen",
      "enPart": "",
      "category": "preDraft",
      "id": 2809
    },
    "2810": {
      "dePart": "Drei Schritte tue nach innen, dann den nach außen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2810
    },
    "2811": {
      "dePart": "Die Sprache ist dem Menschen gegeben,um seine Gedanken zu verbergen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2811
    },
    "2812": {
      "dePart": "Das was jemand von sich aus denkt, bestimmt sein Schicksal",
      "enPart": "",
      "category": "preDraft",
      "id": 2812
    },
    "2813": {
      "dePart": "Wer immer die Wahrheit sagt,wird kein Gedächtniskünstler",
      "enPart": "",
      "category": "preDraft",
      "id": 2813
    },
    "2814": {
      "dePart": "Wenn wir einmal nicht grausam sind, dann glauben wir gleich, wir seien gut",
      "enPart": "",
      "category": "preDraft",
      "id": 2814
    },
    "2815": {
      "dePart": "Lebenskünstler leben nicht länger,aber mehr.",
      "enPart": "",
      "category": "preDraft",
      "id": 2815
    },
    "2816": {
      "dePart": "Alles Bedeutende im Strom des Lebensist durch Sieg und Niederlage entstanden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2816
    },
    "2817": {
      "dePart": "Die Menschen fürchten den Tod, wie Kinder sich fürchten, im Dunkeln zu gehen",
      "enPart": "",
      "category": "preDraft",
      "id": 2817
    },
    "2818": {
      "dePart": "Wer recht erkennen will,muss zuvor in richtiger Weise gezweifelt haben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2818
    },
    "2819": {
      "dePart": "Ironie ist keine Waffe, sondern eher ein Trost der Ohnmächtigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2819
    },
    "2820": {
      "dePart": "In jedem Wunsch schlummert die Enttäuschung seiner Erfüllung",
      "enPart": "",
      "category": "preDraft",
      "id": 2820
    },
    "2821": {
      "dePart": "Wer die Wahrheit geigt, dem schlägt man die Fiedel um den Kopf.",
      "enPart": "",
      "category": "preDraft",
      "id": 2821
    },
    "2822": {
      "dePart": "Deutschland ist da,wo starke Herzen sind.",
      "enPart": "",
      "category": "preDraft",
      "id": 2822
    },
    "2823": {
      "dePart": "Die Menschheit ist die Unsterblichkeit der sterblichen Menschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2823
    },
    "2824": {
      "dePart": "Wer verborgen bleibt, hat ein schönes Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2824
    },
    "2825": {
      "dePart": "Wer sich nicht selbst helfen will,dem kann niemand helfen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2825
    },
    "2826": {
      "dePart": "Das Denken gehört zu den größten Vergnügungen der menschlichen Rasse.",
      "enPart": "",
      "category": "preDraft",
      "id": 2826
    },
    "2827": {
      "dePart": "Wir müssen Deiche des Mutes bauen gegen die Flut der Furcht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2827
    },
    "2828": {
      "dePart": "Nicht die Erkenntnis gehört zum Wesen der Dinge,sondern der Irrtum.",
      "enPart": "",
      "category": "preDraft",
      "id": 2828
    },
    "2829": {
      "dePart": "Man ist in dem Maße zur Freiheit reif, als man zur Selbstkritik fähig ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2829
    },
    "2830": {
      "dePart": "Der Phantast verbirgt die Wahrheit vor sich selbst,der Lügner vor allen",
      "enPart": "",
      "category": "preDraft",
      "id": 2830
    },
    "2831": {
      "dePart": "Immer allein leben: eine kostbare Regel, die ich mich bemühen will einzuhalten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2831
    },
    "2832": {
      "dePart": "Viele Menschen sehen die Tugend mehr im Bereuen der Fehler als im Vermeiden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2832
    },
    "2833": {
      "dePart": "Wenn wir bedenken, dass wir alle verrückt sind, ist das Leben erklärt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2833
    },
    "2834": {
      "dePart": "Jeder hat soviel Recht, wie er Macht hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2834
    },
    "2835": {
      "dePart": "Das Menschenherz ist ein Kirchhof begrabener Hoffnungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2835
    },
    "2836": {
      "dePart": "Alter ist etwas, was keine Rolle spielt, es sei denn, man ist ein Käse.",
      "enPart": "",
      "category": "preDraft",
      "id": 2836
    },
    "2837": {
      "dePart": "Der Tod selbst ist für den, der nachdenkt, nichts so Ernstes wie die Ehe.",
      "enPart": "",
      "category": "preDraft",
      "id": 2837
    },
    "2838": {
      "dePart": "Gerechtigkeit ist vorübergehend, aber das Gewissen ist ewig.",
      "enPart": "",
      "category": "preDraft",
      "id": 2838
    },
    "2839": {
      "dePart": "Das Gewissen ist der einzige Spiegel,der weder betrügt noch schmeichelt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2839
    },
    "2840": {
      "dePart": "Mancher findet sein Herz nicht eher, als bis er seinen Kopf verliert.",
      "enPart": "",
      "category": "preDraft",
      "id": 2840
    },
    "2841": {
      "dePart": "Misstrauen ist eine schlechte Rüstung, die mehr hindern als schirmen kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 2841
    },
    "2842": {
      "dePart": "Ein Mann wird durch Erfahrungen klüger, eine Frau wird durch Erfahrungen älter",
      "enPart": "",
      "category": "preDraft",
      "id": 2842
    },
    "2843": {
      "dePart": "Großer Schmerz und große Lust bilden Menschen am besten",
      "enPart": "",
      "category": "preDraft",
      "id": 2843
    },
    "2844": {
      "dePart": "Eine Wahrheit kann erst wirken, wenn der Empfänger für sie reif ist",
      "enPart": "",
      "category": "preDraft",
      "id": 2844
    },
    "2845": {
      "dePart": "Das Recht des Stärkeren ist das stärkste Unrecht",
      "enPart": "",
      "category": "preDraft",
      "id": 2845
    },
    "2846": {
      "dePart": "Die Wahrheit ist eine Arznei,die angreift.",
      "enPart": "",
      "category": "preDraft",
      "id": 2846
    },
    "2847": {
      "dePart": "Der unsichere Geist fürchtet, obwohl er zu wissen begehrt",
      "enPart": "",
      "category": "preDraft",
      "id": 2847
    },
    "2848": {
      "dePart": "Alle leeren Seelen neigen zu extremen Ansichten",
      "enPart": "",
      "category": "preDraft",
      "id": 2848
    },
    "2849": {
      "dePart": "Wenn du kein Kompliment bekommen kannst - mach dir selbst eines!",
      "enPart": "",
      "category": "preDraft",
      "id": 2849
    },
    "2850": {
      "dePart": "Gewisse Stubenhocker sind gewohnt, alles zu bezweifeln, weil sie nichts sehen",
      "enPart": "",
      "category": "preDraft",
      "id": 2850
    },
    "2851": {
      "dePart": "Man muss auf seinen eigenen Wert stolz sein und den der anderen achten können.",
      "enPart": "",
      "category": "preDraft",
      "id": 2851
    },
    "2852": {
      "dePart": "Kraft macht keinen Lärm, sie ist da und wirkt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2852
    },
    "2853": {
      "dePart": "Wie viel geschieht nicht,was geschehen könnte!",
      "enPart": "",
      "category": "preDraft",
      "id": 2853
    },
    "2854": {
      "dePart": "Das Leben ist die Idee in ihrem unmittelbaren Dasein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2854
    },
    "2855": {
      "dePart": "Man ist nur schön, wenn man auch zugleich menschlich ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2855
    },
    "2856": {
      "dePart": "Wer sich mit Liebe wappnet, überwindet Zorn,Elend, Übermacht und Missgeschick.",
      "enPart": "",
      "category": "preDraft",
      "id": 2856
    },
    "2857": {
      "dePart": "Sämtliche Spiegel sind nutzlos,wenn man zu spät hinein schaut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2857
    },
    "2858": {
      "dePart": "Gutsein ist ein weit gewaltigeres Abenteuer als eine Weltumsegelung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2858
    },
    "2859": {
      "dePart": "Jede Mode ist zweimal lächerlich: am Anfang und am Ende.",
      "enPart": "",
      "category": "preDraft",
      "id": 2859
    },
    "2860": {
      "dePart": "Mein Herz im Traume Wunder sieht, was nie geschah und nie geschieht",
      "enPart": "",
      "category": "preDraft",
      "id": 2860
    },
    "2861": {
      "dePart": "Du mußt selbst wachsen, egal wie groß Dein Großvater war.",
      "enPart": "",
      "category": "preDraft",
      "id": 2861
    },
    "2862": {
      "dePart": "Es gibt ein Ziel, aber keinen Weg. Das, was wir Weg nennen, ist Zögern",
      "enPart": "",
      "category": "preDraft",
      "id": 2862
    },
    "2863": {
      "dePart": "Viel Gutes kann der Mensch vollbringen,ohne sich ein Opfer zumuten zu müssen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2863
    },
    "2864": {
      "dePart": "Wir müssen aus dem Schlafe erwachen und unsere Verantwortung sehen",
      "enPart": "",
      "category": "preDraft",
      "id": 2864
    },
    "2865": {
      "dePart": "Das ganze Leben ist ein Versuch.Je mehr Versuche du durchführst, desto besser",
      "enPart": "",
      "category": "preDraft",
      "id": 2865
    },
    "2866": {
      "dePart": "Die Häßlichen und die Dummen haben es am besten auf der Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2866
    },
    "2867": {
      "dePart": "Da wird es hell in unseren Leben, wo man für das Kleinste danken lernt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2867
    },
    "2868": {
      "dePart": "Die Wahrheit triumphiert nie, ihre Gegner sterben nur aus.",
      "enPart": "",
      "category": "preDraft",
      "id": 2868
    },
    "2869": {
      "dePart": "Die Gegenwart ist die einzige Zeit, die uns wirklich gehört.",
      "enPart": "",
      "category": "preDraft",
      "id": 2869
    },
    "2870": {
      "dePart": "Die Gewohnheit ist eine zweite Schwerkraft",
      "enPart": "",
      "category": "preDraft",
      "id": 2870
    },
    "2871": {
      "dePart": "Wissen und Erkennensind die Freude und die Berechtigung der Menschheit",
      "enPart": "",
      "category": "preDraft",
      "id": 2871
    },
    "2872": {
      "dePart": "Das Leben ist kurz und seine Zeit zu verlieren ist eine Sünde.",
      "enPart": "",
      "category": "preDraft",
      "id": 2872
    },
    "2873": {
      "dePart": "Die Zukunft zeigt sich in uns – lange bevor sie eintritt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2873
    },
    "2874": {
      "dePart": "Man steht wohl einmal am Ende des Lebens, immer aber am Anfang des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 2874
    },
    "2875": {
      "dePart": "Schenken ist ein Brückenschlag über den Abgrund deiner Einsamkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2875
    },
    "2876": {
      "dePart": "Die beste Definition für Mensch lautet: undankbarer Zweibeiner.",
      "enPart": "",
      "category": "preDraft",
      "id": 2876
    },
    "2877": {
      "dePart": "Die Seele legt, so wie der Magnet den Feinstaub, das Gesicht um sich herum",
      "enPart": "",
      "category": "preDraft",
      "id": 2877
    },
    "2878": {
      "dePart": "Jahre runzeln die Haut, aber den Enthusiasmus aufgeben runzelt die Seele",
      "enPart": "",
      "category": "preDraft",
      "id": 2878
    },
    "2879": {
      "dePart": "Willst du das Leben leicht haben, so bleibe immer bei der Herde.",
      "enPart": "",
      "category": "preDraft",
      "id": 2879
    },
    "2880": {
      "dePart": "Wirklich unersetzlich in der Geschichtewaren nur Adam und Eva",
      "enPart": "",
      "category": "preDraft",
      "id": 2880
    },
    "2881": {
      "dePart": "Nur durch Mut kann man sein Leben in Ordnung bringen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2881
    },
    "2882": {
      "dePart": "Freude ist das Leben durch einen Sonnenstrahl gesehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2882
    },
    "2883": {
      "dePart": "Alt und jung, wir alle befinden uns auf unserer letzten Reise.",
      "enPart": "",
      "category": "preDraft",
      "id": 2883
    },
    "2884": {
      "dePart": "Man muss immer etwas haben, worauf man sich freut.",
      "enPart": "",
      "category": "preDraft",
      "id": 2884
    },
    "2885": {
      "dePart": "Wenn du das Leben liebst, liebt es dich auch.",
      "enPart": "",
      "category": "preDraft",
      "id": 2885
    },
    "2886": {
      "dePart": "Das wahrste Wort der Menschen-Sprache heißt: Vielleicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2886
    },
    "2887": {
      "dePart": "Gleich wichtig ist es, sich bei der Freude wie beim Schmerz zu mäßigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2887
    },
    "2888": {
      "dePart": "Der wahre Heldenmut besteht darin, über das Elend des Lebens erhaben zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2888
    },
    "2889": {
      "dePart": "Wo ich auch war und was mir das Leben auch gab, immer hatte ich Heimweh.",
      "enPart": "",
      "category": "preDraft",
      "id": 2889
    },
    "2890": {
      "dePart": "Das Leben bedeutet eine fast lückenlose Reihe persönlicher Entdeckungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2890
    },
    "2891": {
      "dePart": "Hochmut reitet zu Pferde aus und kehrt zu Fuß heim.",
      "enPart": "",
      "category": "preDraft",
      "id": 2891
    },
    "2892": {
      "dePart": "Kränkungen haben ihr Gutes: Sie setzen einen ins Recht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2892
    },
    "2893": {
      "dePart": "Sprich über das Moderne ohne Verachtung und über das Alte ohne Vergötterung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2893
    },
    "2894": {
      "dePart": "Wir wünschen Alter alle Tage,und kommt es, hört man nichts als Klage.",
      "enPart": "",
      "category": "preDraft",
      "id": 2894
    },
    "2895": {
      "dePart": "Lass das lange Vorbereiten, fang dein Leben an beizeiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2895
    },
    "2896": {
      "dePart": "Geselligkeit ist die Kunst, den Umgang mit sich selbst zu verlernen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2896
    },
    "2897": {
      "dePart": "Nichts ist erbärmlicherals die Resignation, die zu früh kommt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2897
    },
    "2898": {
      "dePart": "Die meisten jagen so sehr dem Genusse nach, dass sie an ihm vorbeilaufen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2898
    },
    "2899": {
      "dePart": "Ein glückliches Leben besteht in erster Linie aus Freiheit von Sorgen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2899
    },
    "2900": {
      "dePart": "An seinen Idealen zu Grunde gehen können heißt lebensfähig sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2900
    },
    "2901": {
      "dePart": "Die Wahrheit gehört dem Menschen, der Irrtum der Zeit an.",
      "enPart": "",
      "category": "preDraft",
      "id": 2901
    },
    "2902": {
      "dePart": "Ein Volk, das keine Vergangenheit haben will,verdient auch keine Zukunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2902
    },
    "2903": {
      "dePart": "Bei den meisten Menschen altert das Herz mit dem Körper.",
      "enPart": "",
      "category": "preDraft",
      "id": 2903
    },
    "2904": {
      "dePart": "Die Furcht, als Feigling zu gelten, macht oft zum Feigling.",
      "enPart": "",
      "category": "preDraft",
      "id": 2904
    },
    "2905": {
      "dePart": "Ist nicht im Innern Sonnenschein,von außen kommt er nicht hinein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2905
    },
    "2906": {
      "dePart": "Der Sarg, das sind die Bretter, welche die andere Welt bedeuten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2906
    },
    "2907": {
      "dePart": "Je mehr es abwärts geht, je reicher wird das Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2907
    },
    "2908": {
      "dePart": "Gewalt kann wohl den Richter beugen,doch niemals beugt Gewalt das Recht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2908
    },
    "2909": {
      "dePart": "Gute Vorsätze sind grüne Früchte, die abfallen, ehe sie reif sind.",
      "enPart": "",
      "category": "preDraft",
      "id": 2909
    },
    "2910": {
      "dePart": "Die Bosheit und Missgunst kann sich den Anschein von Freiheit geben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2910
    },
    "2911": {
      "dePart": "Zu große Güte ist ein Stück von Dummheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2911
    },
    "2912": {
      "dePart": "Der Schiffbrüchige fürchtet auch die ruhigen Gewässer.",
      "enPart": "",
      "category": "preDraft",
      "id": 2912
    },
    "2913": {
      "dePart": "Reue ist die Ebbe nach der Flut der Leidenschaft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2913
    },
    "2914": {
      "dePart": "Die Zukunft ist als Raum der Möglichkeiten der Raum unserer Freiheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2914
    },
    "2915": {
      "dePart": "Wenn du die Wahl hast zwischen zwei Übeln, wähle keines von beiden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2915
    },
    "2916": {
      "dePart": "Erziehung ist die billigste Verteidigung der Nationen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2916
    },
    "2917": {
      "dePart": "Wir haben vage Hoffnungen, aber klare Befürchtungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2917
    },
    "2918": {
      "dePart": "Keine Zeit ist so schlimm,dass man nicht ehrlich sein könnte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2918
    },
    "2919": {
      "dePart": "Am schwersten sind Mißgeschicke zu ertragen, zu denen es nie gekommen ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 2919
    },
    "2920": {
      "dePart": "Will man Tiefes sagen,so gewöhne man sich zunächst,nichts Falsches zu sagen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2920
    },
    "2921": {
      "dePart": "Irgendwann hört auch das Altern auf; man verwittert dann nur noch.",
      "enPart": "",
      "category": "preDraft",
      "id": 2921
    },
    "2922": {
      "dePart": "Der Sinn des Lebens ist mehrals das Leben selbst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2922
    },
    "2923": {
      "dePart": "Was Du erhältst, nimm ohne Stolz an! Was Du verlierst, gib ohne Trauer auf!",
      "enPart": "",
      "category": "preDraft",
      "id": 2923
    },
    "2924": {
      "dePart": "Im Kampf zwischen dir und der Welt, stelle dich immer auf die Seite der Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2924
    },
    "2925": {
      "dePart": "Wo sind die Tränen von gestern Abend? Wo ist der Schnee vom vergangenen Jahr?",
      "enPart": "",
      "category": "preDraft",
      "id": 2925
    },
    "2926": {
      "dePart": "Der Haß ist die Liebe,die gescheitert ist",
      "enPart": "",
      "category": "preDraft",
      "id": 2926
    },
    "2927": {
      "dePart": "Wer Sünde weiß zu scheuen, der darf sie nicht bereuen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2927
    },
    "2928": {
      "dePart": "Wir loben die gute alte Zeit, leben aber gern in er Gegenwart",
      "enPart": "",
      "category": "preDraft",
      "id": 2928
    },
    "2929": {
      "dePart": "Als Gott den Menschen erschuf, war er bereits müde, das erklärt manches.",
      "enPart": "",
      "category": "preDraft",
      "id": 2929
    },
    "2930": {
      "dePart": "Gutes Aussehen ist eine stumme Empfehlung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2930
    },
    "2931": {
      "dePart": "Der Lorbeer und der Hochmut sind gefährlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 2931
    },
    "2932": {
      "dePart": "Das Schicksal mischt die Karten und wir spielen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2932
    },
    "2933": {
      "dePart": "Ich danke Gott, dass ich an Schwierigkeiten gewöhnt bin.",
      "enPart": "",
      "category": "preDraft",
      "id": 2933
    },
    "2934": {
      "dePart": "Wünsche nie etwas, was durch Mauern oder Vorhänge verborgen werden müsste.",
      "enPart": "",
      "category": "preDraft",
      "id": 2934
    },
    "2935": {
      "dePart": "Auf Grabschriften stehen auch die größten Lügen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2935
    },
    "2936": {
      "dePart": "Hunger und Liebe sind die Triebkräftealler menschlichen Handlungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2936
    },
    "2937": {
      "dePart": "Wenn das Leben ein Traum ist, was passiert dann - wenn ich aufwache?",
      "enPart": "",
      "category": "preDraft",
      "id": 2937
    },
    "2938": {
      "dePart": "Den seelischen Wert einer Frau erkennst du daran, wie sie zu altern versteht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2938
    },
    "2939": {
      "dePart": "Die Schönheit ist nach drei Tagen genauso langweilig wie die Tugend",
      "enPart": "",
      "category": "preDraft",
      "id": 2939
    },
    "2940": {
      "dePart": "Alles in der Welt lässt sich ertragen,nur nicht eine Reihe von schönen Tagen",
      "enPart": "",
      "category": "preDraft",
      "id": 2940
    },
    "2941": {
      "dePart": "Alles Schöne ist ein selbst erleuchtetes,vollendetes Individuum.",
      "enPart": "",
      "category": "preDraft",
      "id": 2941
    },
    "2942": {
      "dePart": "Das größte Gut, das uns geschenkt ist,ist die Sehnsucht unserer Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 2942
    },
    "2943": {
      "dePart": "Die Welt schuldet uns nichts - sie war vor uns da!",
      "enPart": "",
      "category": "preDraft",
      "id": 2943
    },
    "2944": {
      "dePart": "Eigentlich müßte man sich fürchten, aus dem Haus zu treten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2944
    },
    "2945": {
      "dePart": "Lerne von jedem, der kommt,so erfreust du jeden, der geht",
      "enPart": "",
      "category": "preDraft",
      "id": 2945
    },
    "2946": {
      "dePart": "Eher wird ein Unrecht verziehen als eine Beleidigung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2946
    },
    "2947": {
      "dePart": "Nichts auf der Welt trägt so viele Masken wie die Eifersucht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2947
    },
    "2948": {
      "dePart": "Gesundheit und Verstand sind die zwei großen Gaben des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 2948
    },
    "2949": {
      "dePart": "Das einzige Mittel, den Irrtum zu vermeiden, ist die Unwissenheit",
      "enPart": "",
      "category": "preDraft",
      "id": 2949
    },
    "2950": {
      "dePart": "Passbilder sind die Rache der Photographen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2950
    },
    "2951": {
      "dePart": "Enttäuschungen töten nicht und Hoffnungen lassen leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2951
    },
    "2952": {
      "dePart": "Eine kränkende Wahrheit ist mehr wert als eine reizende Schmeichelei.",
      "enPart": "",
      "category": "preDraft",
      "id": 2952
    },
    "2953": {
      "dePart": "Die Macht des Wunders ist nichts anderes als die Macht der Einbildungskraft.",
      "enPart": "",
      "category": "preDraft",
      "id": 2953
    },
    "2954": {
      "dePart": "Einen Wahn verlieren macht weiser als eine Wahrheit finden.",
      "enPart": "",
      "category": "preDraft",
      "id": 2954
    },
    "2955": {
      "dePart": "Revolutionensind die Krankheiten,die das Wachstum der Menschheit bezeichnen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2955
    },
    "2956": {
      "dePart": "Selbst ein kurzes Leben ist immer noch zu lang",
      "enPart": "",
      "category": "preDraft",
      "id": 2956
    },
    "2957": {
      "dePart": "Tugend ist die Mitte zwischen den Fehlern.",
      "enPart": "",
      "category": "preDraft",
      "id": 2957
    },
    "2958": {
      "dePart": "Das Leben leicht tragen und tief genießen,ist ja doch die Summe aller Weisheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2958
    },
    "2959": {
      "dePart": "Alles, was das Leben ist, sind die Träume nach Rache.",
      "enPart": "",
      "category": "preDraft",
      "id": 2959
    },
    "2960": {
      "dePart": "Der Mensch hat zwei Berufungen in sich:eine ruft zum Leben, die andere zum Tod.",
      "enPart": "",
      "category": "preDraft",
      "id": 2960
    },
    "2961": {
      "dePart": "Der kälteste Winter meines Lebens war ein Sommer in San Francisco.",
      "enPart": "",
      "category": "preDraft",
      "id": 2961
    },
    "2962": {
      "dePart": "Es sei gleich morgen oder heut, Sterben müssen alle Leut'",
      "enPart": "",
      "category": "preDraft",
      "id": 2962
    },
    "2963": {
      "dePart": "Reisen heißt, an ein Ziel kommen, Wandern heißt, unterwegs sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 2963
    },
    "2964": {
      "dePart": "Der Feige stirbt schon viele male vor seinem Tode.",
      "enPart": "",
      "category": "preDraft",
      "id": 2964
    },
    "2965": {
      "dePart": "Das Leben ist eine Quarantäne für das Paradies.",
      "enPart": "",
      "category": "preDraft",
      "id": 2965
    },
    "2966": {
      "dePart": "Ein graues Haar steckt all die andern an!",
      "enPart": "",
      "category": "preDraft",
      "id": 2966
    },
    "2967": {
      "dePart": "Wer mit der Sünde spielt, mit dem spielt die Sünde.",
      "enPart": "",
      "category": "preDraft",
      "id": 2967
    },
    "2968": {
      "dePart": "Ein guter Rat in spaßiger Form ist besser als eine ernste Belehrung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2968
    },
    "2969": {
      "dePart": "Der wahre Stolz ergreift für sich nicht selbst das Wort.",
      "enPart": "",
      "category": "preDraft",
      "id": 2969
    },
    "2970": {
      "dePart": "Schicksal ist nie eine Frage der Chance sondern eine Frage der Wahl.",
      "enPart": "",
      "category": "preDraft",
      "id": 2970
    },
    "2971": {
      "dePart": "Die Wirklichkeit ist nur ein Teil des Möglichen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2971
    },
    "2972": {
      "dePart": "Das Leben ist ein ewiges Wechselspiel aus Nehmen und Behalten.",
      "enPart": "",
      "category": "preDraft",
      "id": 2972
    },
    "2973": {
      "dePart": "Das Geheimnis, die größte Freude am Leben zu finden, ist gefährlich zu leben!",
      "enPart": "",
      "category": "preDraft",
      "id": 2973
    },
    "2974": {
      "dePart": "Die Welt ist rund und muss sich drehn, was oben war, muss unten stehn.",
      "enPart": "",
      "category": "preDraft",
      "id": 2974
    },
    "2975": {
      "dePart": "Das Leben ist ein Hospital, in dem jeder sein Bett wechseln möchte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2975
    },
    "2976": {
      "dePart": "Wer zu lange gegen Drachen kämpft, wird selbst zum Drachen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2976
    },
    "2977": {
      "dePart": "Bitter ist es, das heute zu müssen, was man gestern noch wollen konnte.",
      "enPart": "",
      "category": "preDraft",
      "id": 2977
    },
    "2978": {
      "dePart": "Alle kräftigen Menschen lieben das Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2978
    },
    "2979": {
      "dePart": "Gewohnheit ist der große Führer im Menschenleben.",
      "enPart": "",
      "category": "preDraft",
      "id": 2979
    },
    "2980": {
      "dePart": "Dunkel sind die Wege, die das Schicksal geht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2980
    },
    "2981": {
      "dePart": "Kein Toter ist so begraben wie eine erloschene Leidenschaft",
      "enPart": "",
      "category": "preDraft",
      "id": 2981
    },
    "2982": {
      "dePart": "Man müßte das Leben so einrichten, dass jeder Moment bedeutungsvoll ist",
      "enPart": "",
      "category": "preDraft",
      "id": 2982
    },
    "2983": {
      "dePart": "Intuitionen sind Träume,derer man sich erinnert",
      "enPart": "",
      "category": "preDraft",
      "id": 2983
    },
    "2984": {
      "dePart": "Träumend plant der Geist seine eigene Wirklichkeit",
      "enPart": "",
      "category": "preDraft",
      "id": 2984
    },
    "2985": {
      "dePart": "Die wichtigste Fähigkeit ist die,welche alle anderen ordnet.",
      "enPart": "",
      "category": "preDraft",
      "id": 2985
    },
    "2986": {
      "dePart": "Alles altert, selbst die Achtung, wenn man sich nicht in Acht nimmt.",
      "enPart": "",
      "category": "preDraft",
      "id": 2986
    },
    "2987": {
      "dePart": "Alle Künste sind gut, ausgenommen die langweilige Kunst.",
      "enPart": "",
      "category": "preDraft",
      "id": 2987
    },
    "2988": {
      "dePart": "Manche meinen, wenn sie ein Echo hören, der Ton stamme von Ihnen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2988
    },
    "2989": {
      "dePart": "Je mehr ein Mensch weiß, desto weniger ahnt er.",
      "enPart": "",
      "category": "preDraft",
      "id": 2989
    },
    "2990": {
      "dePart": "Die Schönheit ist die größte menschliche Macht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2990
    },
    "2991": {
      "dePart": "Das Schicksal macht nie einen König matt, ehe es ihm Schach geboten hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 2991
    },
    "2992": {
      "dePart": "Das einzige Paradies ist das verlorene Paradies.",
      "enPart": "",
      "category": "preDraft",
      "id": 2992
    },
    "2993": {
      "dePart": "Die Welt kann man nicht verändern, aber gewisse Sauereien kann man abstellen",
      "enPart": "",
      "category": "preDraft",
      "id": 2993
    },
    "2994": {
      "dePart": "Alle Menschen schieben aufund bereuen den Aufschub.",
      "enPart": "",
      "category": "preDraft",
      "id": 2994
    },
    "2995": {
      "dePart": "Die Augen reden mächtiger als die Lippen.",
      "enPart": "",
      "category": "preDraft",
      "id": 2995
    },
    "2996": {
      "dePart": "Kaum gedacht, kaum gedacht,war der Lust ein End' gemacht.",
      "enPart": "",
      "category": "preDraft",
      "id": 2996
    },
    "2997": {
      "dePart": "Der Mensch wird wie der Stahl hart - durch öfteres Abkühlen nach Erhitzung.",
      "enPart": "",
      "category": "preDraft",
      "id": 2997
    },
    "2998": {
      "dePart": "Die Empörung ist der Zorn der Gerechtigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 2998
    },
    "2999": {
      "dePart": "Aber alles, was kommen wird, ist unsicher:Lebe für den Augenblick!",
      "enPart": "",
      "category": "preDraft",
      "id": 2999
    },
    "3000": {
      "dePart": "Die Seele, das Wesen des Menschen,entzieht sich der Logik.",
      "enPart": "",
      "category": "preDraft",
      "id": 3000
    },
    "3001": {
      "dePart": "Das Leben wird schließlich mit dem Tode bestraft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3001
    },
    "3002": {
      "dePart": "\"Pietät\" ist unter Umständen die raffinierteste Art des Egoismus.",
      "enPart": "",
      "category": "preDraft",
      "id": 3002
    },
    "3003": {
      "dePart": "Vielleicht war meine Gleichgültigkeit nur ein Übermaß an Begierde.",
      "enPart": "",
      "category": "preDraft",
      "id": 3003
    },
    "3004": {
      "dePart": "Die Wahrheit verletzt tiefer als jede Beleidigung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3004
    },
    "3005": {
      "dePart": "Bei einer liebenden Frau hat die Vernunft nur wenig zu sagen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3005
    },
    "3006": {
      "dePart": "Die, die von Sterben reden, leben am längsten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3006
    },
    "3007": {
      "dePart": "Im Fortschritt der Geschichte nimmt die Phantasie ab und das Denken zu.",
      "enPart": "",
      "category": "preDraft",
      "id": 3007
    },
    "3008": {
      "dePart": "Es ist gar viel leichter, ein Ding zu tadeln als es selbst zu erfinden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3008
    },
    "3009": {
      "dePart": "Das Schicksal hält es immer mit den Kühnen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3009
    },
    "3010": {
      "dePart": "Besser ein freier Teufel, als ein gebundener Engel.",
      "enPart": "",
      "category": "preDraft",
      "id": 3010
    },
    "3011": {
      "dePart": "Besser ein kleines Feuer, das dich wärmt,als ein großes, das dich verbrennt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3011
    },
    "3012": {
      "dePart": "Die Einsamkeit ist der vertraute Umgang mit sich selbst.",
      "enPart": "",
      "category": "preDraft",
      "id": 3012
    },
    "3013": {
      "dePart": "Träume sind wahr, solange wir sie träumen und leben wir nicht immer im Traum?",
      "enPart": "",
      "category": "preDraft",
      "id": 3013
    },
    "3014": {
      "dePart": "Küsse sind das, was von der Sprache des Paradieses übrig geblieben ist",
      "enPart": "",
      "category": "preDraft",
      "id": 3014
    },
    "3015": {
      "dePart": "Das Grämen vor der Zeithalte ich für ein sehr unnützes Geschäft",
      "enPart": "",
      "category": "preDraft",
      "id": 3015
    },
    "3016": {
      "dePart": "Das Gelobte Land liegt immer jenseits der Berge.",
      "enPart": "",
      "category": "preDraft",
      "id": 3016
    },
    "3017": {
      "dePart": "Unser Leib ist so: Je mehr wir ihn pflegen, desto mehr meint er nötig zu haben",
      "enPart": "",
      "category": "preDraft",
      "id": 3017
    },
    "3018": {
      "dePart": "Gewalt vernichtet was das Leben lebenswert macht",
      "enPart": "",
      "category": "preDraft",
      "id": 3018
    },
    "3019": {
      "dePart": "Frauen sind die Holzwolle der Glaskiste des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 3019
    },
    "3020": {
      "dePart": "Freiheit ist Hingabe - Hingabe an eine selbstgewählte Idee.",
      "enPart": "",
      "category": "preDraft",
      "id": 3020
    },
    "3021": {
      "dePart": "Was Schicksal ist, läßt sich nicht definieren, nur sehend erleben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3021
    },
    "3022": {
      "dePart": "Wir leben immer in einer Welt,die wir uns selbst einbilden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3022
    },
    "3023": {
      "dePart": "Zwei Wahrheiten können sich nie widersprechen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3023
    },
    "3024": {
      "dePart": "Wer irregegangen ist, der kann andern desto besser den Weg zeigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3024
    },
    "3025": {
      "dePart": "Erfahrung ist der Name, den die Menschen ihren Torheiten und Sorgen geben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3025
    },
    "3026": {
      "dePart": "Es ist wichtiger,die Möglichkeiten des Lebens im Auge zu habenals die Gefahren.",
      "enPart": "",
      "category": "preDraft",
      "id": 3026
    },
    "3027": {
      "dePart": "Sehnsucht und Ahnung liegen ineinander,eins treibt das andre hervor",
      "enPart": "",
      "category": "preDraft",
      "id": 3027
    },
    "3028": {
      "dePart": "Die gefährlichsten Unwahrheiten, sind Wahrheiten, mäßig entstellt",
      "enPart": "",
      "category": "preDraft",
      "id": 3028
    },
    "3029": {
      "dePart": "Ein kluger Mann lebt weder geistig noch finanziell über seine Verhältnisse.",
      "enPart": "",
      "category": "preDraft",
      "id": 3029
    },
    "3030": {
      "dePart": "Ruhige, stille Hochachtung ist mehr wert als Anbetung, Verehrung, Entzückung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3030
    },
    "3031": {
      "dePart": "Beim Genie heißt es: Laß dich gehen!Beim Talent: Nimm dich zusammen!",
      "enPart": "",
      "category": "preDraft",
      "id": 3031
    },
    "3032": {
      "dePart": "Worte sind die einzigen Dinge, die nie vergehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3032
    },
    "3033": {
      "dePart": "Die Mode ist weiblichen Geschlechts, hat folglich ihre Launen",
      "enPart": "",
      "category": "preDraft",
      "id": 3033
    },
    "3034": {
      "dePart": "Das Leben ist eine Krankheit, der Schlaf ein Palliativ, der Tod die Radikalkur.",
      "enPart": "",
      "category": "preDraft",
      "id": 3034
    },
    "3035": {
      "dePart": "Es gibt keinen Frieden für die Furchtsamen und Uneinigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3035
    },
    "3036": {
      "dePart": "Man kann seiner eigenen Zeit nicht böse sein, ohne selbst Schaden zu nehmen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3036
    },
    "3037": {
      "dePart": "Ein Kritiker ist jemand, der zum Nörgeln neigt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3037
    },
    "3038": {
      "dePart": "Die Zeit ist schlecht? Wohlan.Du bist da, sie besser zu machen",
      "enPart": "",
      "category": "preDraft",
      "id": 3038
    },
    "3039": {
      "dePart": "Die Gewohnheit ist ein lästiges Ding,selbst an Verhasstes fesselt sie",
      "enPart": "",
      "category": "preDraft",
      "id": 3039
    },
    "3040": {
      "dePart": "Ein Geheimnis haben, macht einen schrecklich einsam.",
      "enPart": "",
      "category": "preDraft",
      "id": 3040
    },
    "3041": {
      "dePart": "Schenkt Worten keinen Glauben, wenn nicht Werke dahinter stehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3041
    },
    "3042": {
      "dePart": "Nachdenken enthält eine unerschöpfliche Quellevon Trost und Beruhigung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3042
    },
    "3043": {
      "dePart": "Zerstörung ist absolut keine Garantiefür den Aufbau eines besseren Lebens",
      "enPart": "",
      "category": "preDraft",
      "id": 3043
    },
    "3044": {
      "dePart": "So genannte Jahrhundertwerkeschafft der Zahn der Zeit in ein paar Jahren.",
      "enPart": "",
      "category": "preDraft",
      "id": 3044
    },
    "3045": {
      "dePart": "Die Stunden, nicht die Tage, sind die Stützpunkte unserer Erinnerung",
      "enPart": "",
      "category": "preDraft",
      "id": 3045
    },
    "3046": {
      "dePart": "Illusionen sind Krankheiten wovon man leider immer geheilt wird.",
      "enPart": "",
      "category": "preDraft",
      "id": 3046
    },
    "3047": {
      "dePart": "Alle Verallgemeinerungen sind gefährlich, sogar diese.",
      "enPart": "",
      "category": "preDraft",
      "id": 3047
    },
    "3048": {
      "dePart": "Wenn der Mensch nur erst so weit wäre,seinem eigenen Genie zu trauen!",
      "enPart": "",
      "category": "preDraft",
      "id": 3048
    },
    "3049": {
      "dePart": "Der wahre Mensch ist immer traurig; seine Freuden sind Blitze in der Nacht",
      "enPart": "",
      "category": "preDraft",
      "id": 3049
    },
    "3050": {
      "dePart": "Moral ist ein Maulkorb für den Willen, Logik ein Steigriemen für den Geist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3050
    },
    "3051": {
      "dePart": "Für Geheimnis und anvertrautes Gut gelten die gleichen Regeln.",
      "enPart": "",
      "category": "preDraft",
      "id": 3051
    },
    "3052": {
      "dePart": "Höflichkeit ist Klugheit, folglich ist Unhöflichkeit Dummheit",
      "enPart": "",
      "category": "preDraft",
      "id": 3052
    },
    "3053": {
      "dePart": "Wo ich auch war und was mir das Leben auch gab, immer hatte ich Heimweh.",
      "enPart": "",
      "category": "preDraft",
      "id": 3053
    },
    "3054": {
      "dePart": "Der Mensch ist eine Episode, ein Augenblick im Weltschicksal.",
      "enPart": "",
      "category": "preDraft",
      "id": 3054
    },
    "3055": {
      "dePart": "Die eigene Erfahrung hat den Vorteil völliger Gewissheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3055
    },
    "3056": {
      "dePart": "Der Spott endet, wo das Verständnis beginnt",
      "enPart": "",
      "category": "preDraft",
      "id": 3056
    },
    "3057": {
      "dePart": "Freiheitsliebe ist Nächstenliebe, Machtliebe ist Eigenliebe.",
      "enPart": "",
      "category": "preDraft",
      "id": 3057
    },
    "3058": {
      "dePart": "Schmerz und Freude liegt in einer Schale; ihre Mischung ist der Menschen Los.",
      "enPart": "",
      "category": "preDraft",
      "id": 3058
    },
    "3059": {
      "dePart": "Nie drohe man zuerst mit der Faust und dann mit dem Finger.",
      "enPart": "",
      "category": "preDraft",
      "id": 3059
    },
    "3060": {
      "dePart": "Man muss den Punkt kennen, bis zu dem man zurückweichen kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 3060
    },
    "3061": {
      "dePart": "Am Ende des längsten Lebens sind wir immer noch Anfänger.",
      "enPart": "",
      "category": "preDraft",
      "id": 3061
    },
    "3062": {
      "dePart": "Hoffnung ist der krankhafte Glaube an den Eintritt des Unmöglichen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3062
    },
    "3063": {
      "dePart": "Was ist Reue ?Eine grosse Trauer darüber,dass wir sind, wie wir sind.",
      "enPart": "",
      "category": "preDraft",
      "id": 3063
    },
    "3064": {
      "dePart": "Nach meinem Tod will ich es Rosen regnen lassen",
      "enPart": "",
      "category": "preDraft",
      "id": 3064
    },
    "3065": {
      "dePart": "Wer die Kehrseite der Menschen gesehen hat, wie kann der noch stolz sein?",
      "enPart": "",
      "category": "preDraft",
      "id": 3065
    },
    "3066": {
      "dePart": "Was man von der Minute ausgeschlagen,gibt keine Ewigkeit zurück.",
      "enPart": "",
      "category": "preDraft",
      "id": 3066
    },
    "3067": {
      "dePart": "Kein Abschied auf der Welt fällt schwererals der Abschied von der Macht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3067
    },
    "3068": {
      "dePart": "Die Ungeduld des Alters bleibt der Jugend ewig unverständlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3068
    },
    "3069": {
      "dePart": "Dürfen darf man alles, man muss es nur können.",
      "enPart": "",
      "category": "preDraft",
      "id": 3069
    },
    "3070": {
      "dePart": "Lerne den Ton der Gesellschaft anzunehmen, in der du dich befindest.",
      "enPart": "",
      "category": "preDraft",
      "id": 3070
    },
    "3071": {
      "dePart": "Nichts ist so verzichtbar wie die Sitten und Gebäuche anderer Völker.",
      "enPart": "",
      "category": "preDraft",
      "id": 3071
    },
    "3072": {
      "dePart": "Ich liebe den Verrat, aber ich hasse Verräter.",
      "enPart": "",
      "category": "preDraft",
      "id": 3072
    },
    "3073": {
      "dePart": "Die zwei größten Tyrannen der Erde: der Zufall und die Zeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3073
    },
    "3074": {
      "dePart": "Wenn man mit sich selbst einig ist, ist man es auch mit anderen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3074
    },
    "3075": {
      "dePart": "Aufgeblasenheit ist der zuverlässigste Schwimmgürtel im Strom des Lebens",
      "enPart": "",
      "category": "preDraft",
      "id": 3075
    },
    "3076": {
      "dePart": "Zweifel ist keine angenehme Voraussetzung, aber Gewissheit ist eine absurde.",
      "enPart": "",
      "category": "preDraft",
      "id": 3076
    },
    "3077": {
      "dePart": "Man kann nicht ohne Furcht sein,wenn man furchterregend aussieht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3077
    },
    "3078": {
      "dePart": "Auch der Anblick des Schlechten kann eine Schulung für das Gute sein",
      "enPart": "",
      "category": "preDraft",
      "id": 3078
    },
    "3079": {
      "dePart": "Die Wahrheit ist zu schlau, um gefangen zu werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3079
    },
    "3080": {
      "dePart": "Verpflichtungen nehmen einem nur Kraft und machen wenig Vergnügen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3080
    },
    "3081": {
      "dePart": "Für mich ist Denkmal ein lebenslanger Imperativ,der aus zwei Wörtern besteht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3081
    },
    "3082": {
      "dePart": "Welchen Sinn hat das Leben eines Menschen, der keine Zeit hat?",
      "enPart": "",
      "category": "preDraft",
      "id": 3082
    },
    "3083": {
      "dePart": "Die Wahrheit ist eine Braut ohne Aussteuer.",
      "enPart": "",
      "category": "preDraft",
      "id": 3083
    },
    "3084": {
      "dePart": "Frauen verwechseln ihre Halluzinationen so leicht mit der Wírklichkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3084
    },
    "3085": {
      "dePart": "Der Mensch ist der Faktor, der nie ganz aufgeht",
      "enPart": "",
      "category": "preDraft",
      "id": 3085
    },
    "3086": {
      "dePart": "Wer nicht zuweilen zu viel empfindet, der empfindet immer zu wenig",
      "enPart": "",
      "category": "preDraft",
      "id": 3086
    },
    "3087": {
      "dePart": "Die Tochter des Neides ist die Verleumdung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3087
    },
    "3088": {
      "dePart": "Vieles Gewaltige lebt, und nichts ist gewaltiger als der Mensch.",
      "enPart": "",
      "category": "preDraft",
      "id": 3088
    },
    "3089": {
      "dePart": "Es gibt kein Wunder für den,der sich nicht wundern kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 3089
    },
    "3090": {
      "dePart": "Unsere Zeit ist eine Parodie aller vorhergehenden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3090
    },
    "3091": {
      "dePart": "Zwischen dem Rande der Lippe und des Bechers kann viel sich ereignen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3091
    },
    "3092": {
      "dePart": "Stärke des Charakters ist oft nichts anderes als eine Schwäche des Gefühls.",
      "enPart": "",
      "category": "preDraft",
      "id": 3092
    },
    "3093": {
      "dePart": "Das Gemüt hat mehr Mängel als der Verstand.",
      "enPart": "",
      "category": "preDraft",
      "id": 3093
    },
    "3094": {
      "dePart": "Das Leben ist seinem inneren Wesen nachein ständiger Schiffbruch.",
      "enPart": "",
      "category": "preDraft",
      "id": 3094
    },
    "3095": {
      "dePart": "Abhängigkeit ist das Los der Frauen; Macht ist, wo die Bärte sind.",
      "enPart": "",
      "category": "preDraft",
      "id": 3095
    },
    "3096": {
      "dePart": "Wir haben viele Sündenböcke, aber der populärste ist die Vorsehung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3096
    },
    "3097": {
      "dePart": "Der Mensch lebt nicht vom Brot allein, sondern hauptsächlich von Schlagworten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3097
    },
    "3098": {
      "dePart": "Leichter trägt, was er trägt,wer Geduld zur Bürde legt",
      "enPart": "",
      "category": "preDraft",
      "id": 3098
    },
    "3099": {
      "dePart": "Nichts ist so fürchterlich als die Macht der Dummheit in den Klugen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3099
    },
    "3100": {
      "dePart": "Der Süden ist immer da, wo man noch nicht hingekommen ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3100
    },
    "3101": {
      "dePart": "Nicht alle lebendigen Dinge sind leicht zu begreifen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3101
    },
    "3102": {
      "dePart": "Wer sich selbst beherrschen kann, beherrscht andere.",
      "enPart": "",
      "category": "preDraft",
      "id": 3102
    },
    "3103": {
      "dePart": "Es gibt viele Dinge, die nicht vernünftig und doch ganz natürlich sind.",
      "enPart": "",
      "category": "preDraft",
      "id": 3103
    },
    "3104": {
      "dePart": "Wenn ich aufhören werde, mich zu entrüsten, werde ich zu altern begonnen haben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3104
    },
    "3105": {
      "dePart": "Der Mutmensch kennt den Zorn, der Furchtmensch die Wut und den Ärger",
      "enPart": "",
      "category": "preDraft",
      "id": 3105
    },
    "3106": {
      "dePart": "Im Dunkeln bleibt hell auch der gute Name",
      "enPart": "",
      "category": "preDraft",
      "id": 3106
    },
    "3107": {
      "dePart": "Unsere Sehnsüchte sind unsere Möglichkeiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3107
    },
    "3108": {
      "dePart": "Es ist schmerzlich, einem Menschen seine Grenzen anzusehen",
      "enPart": "",
      "category": "preDraft",
      "id": 3108
    },
    "3109": {
      "dePart": "Wir leben immer für die Zukunft: Ewiges Stimmen und nie beginnt das Konzert",
      "enPart": "",
      "category": "preDraft",
      "id": 3109
    },
    "3110": {
      "dePart": "Das Alter gräbt uns mehr Falten in den Geist,als in das Gesicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3110
    },
    "3111": {
      "dePart": "Gleichgültigkeit ist die mildeste Form der Intoleranz.",
      "enPart": "",
      "category": "preDraft",
      "id": 3111
    },
    "3112": {
      "dePart": "Die Menschen und die Pyramiden Sind nicht gemacht, um auf dem Kopf zu stehn.",
      "enPart": "",
      "category": "preDraft",
      "id": 3112
    },
    "3113": {
      "dePart": "Die ganze Weisheit junger Toren ist keinen Tag Erfahrung wert.",
      "enPart": "",
      "category": "preDraft",
      "id": 3113
    },
    "3114": {
      "dePart": "Abenteuer: Eine von der richtigen Seite betrachtete Strapaze.",
      "enPart": "",
      "category": "preDraft",
      "id": 3114
    },
    "3115": {
      "dePart": "Fremder Gleichmut ist eines der besten Mittelgegen die Fieber des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 3115
    },
    "3116": {
      "dePart": "Das ist der Toren Torheit: Sie leben ohne Herzeleid.",
      "enPart": "",
      "category": "preDraft",
      "id": 3116
    },
    "3117": {
      "dePart": "Das Leben besteht aus kleinen Handlungen und die Tugend aus kleinen Siegen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3117
    },
    "3118": {
      "dePart": "Die Schnellentschlossenen straucheln leicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3118
    },
    "3119": {
      "dePart": "Kein Ding schwächt die Vernunft mehr als Unlauterkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3119
    },
    "3120": {
      "dePart": "Die Träne ist die Sprache der Seele und die Stimme des Gefühls.",
      "enPart": "",
      "category": "preDraft",
      "id": 3120
    },
    "3121": {
      "dePart": "Der Mensch flieht vor der Zeit,die Zeit vor dem Menschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3121
    },
    "3122": {
      "dePart": "Übertriebene Bescheidenheit ist auch Eitelkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3122
    },
    "3123": {
      "dePart": "Es ist kein Winkerl im Haus, in dem man nicht Gutes tun kann,wenn man nur will.",
      "enPart": "",
      "category": "preDraft",
      "id": 3123
    },
    "3124": {
      "dePart": "Besser in der Hölle herrschen, als im Himmel dienen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3124
    },
    "3125": {
      "dePart": "Die lächerlichsten aller Träume sind die Träumereien der Weltverbesserer.",
      "enPart": "",
      "category": "preDraft",
      "id": 3125
    },
    "3126": {
      "dePart": "Jugend ist beständige Trunkenheit: sie ist das Fieber der Vernunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3126
    },
    "3127": {
      "dePart": "Die Geschichte ist meistens die Schande des Menschengeschlechts.",
      "enPart": "",
      "category": "preDraft",
      "id": 3127
    },
    "3128": {
      "dePart": "Der Ruhm eines Hochmütigen wird schnell zur Schande.",
      "enPart": "",
      "category": "preDraft",
      "id": 3128
    },
    "3129": {
      "dePart": "Jedem Genusse folgt sein Schatten, die Unlust.",
      "enPart": "",
      "category": "preDraft",
      "id": 3129
    },
    "3130": {
      "dePart": "Eigentlich ist jeder Tag wie eine große Schlacht mit vielen Lichtblicken",
      "enPart": "",
      "category": "preDraft",
      "id": 3130
    },
    "3131": {
      "dePart": "Das Geheimnis der Tugend ist die Gewohnheit",
      "enPart": "",
      "category": "preDraft",
      "id": 3131
    },
    "3132": {
      "dePart": "Der Zufall ist die in Schleier gehüllte Notwendigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3132
    },
    "3133": {
      "dePart": "Der Reaktionär ist ein Nachtwandler, der rückwärts geht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3133
    },
    "3134": {
      "dePart": "Wer wagt, durch das Reich der Träume zu schreiten,gelangt zur Wahrheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3134
    },
    "3135": {
      "dePart": "Was hilft Gesetz, was helfen Strafen, wenn Obrigkeit und Fürsten schlafen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3135
    },
    "3136": {
      "dePart": "Wer die Vergangenheit liebt,liebt eigentlich das Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3136
    },
    "3137": {
      "dePart": "Der Mensch ist dazu bestimmt, den Sturz seiner Götter zu überleben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3137
    },
    "3138": {
      "dePart": "Mancher meint er kenne jedermann,der sich doch selbst nicht kennen kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 3138
    },
    "3139": {
      "dePart": "Erst der Ernst macht den Mann, erst der Fleiß das Genie.",
      "enPart": "",
      "category": "preDraft",
      "id": 3139
    },
    "3140": {
      "dePart": "Die Menschen verkommen, wenn sie kein Festtagskleid anziehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3140
    },
    "3141": {
      "dePart": "Wie soll man in der Abgeschiedenheit seine Fehler kennenlernen?",
      "enPart": "",
      "category": "preDraft",
      "id": 3141
    },
    "3142": {
      "dePart": "Die Ehre hat gesetzlichen Anspruch auf Feierabend und Pension.",
      "enPart": "",
      "category": "preDraft",
      "id": 3142
    },
    "3143": {
      "dePart": "Ein freundliches Wort findet immer guten Boden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3143
    },
    "3144": {
      "dePart": "Das Schicksal ist das, was sich nicht umgehen läßt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3144
    },
    "3145": {
      "dePart": "Alles in der Weltendet durch Zerfall oder Ermüdung",
      "enPart": "",
      "category": "preDraft",
      "id": 3145
    },
    "3146": {
      "dePart": "Die Welt wird altund wird wieder jung,doch der Mensch hofft immer Verbesserung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3146
    },
    "3147": {
      "dePart": "Was für eine Frau das Parfüm, ist für den Mann der Zigarrenduft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3147
    },
    "3148": {
      "dePart": "Der Mittelweg ist oft doppelt gefährlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3148
    },
    "3149": {
      "dePart": "Um allein zu leben,muß man sehr einem Gotte oder ganz einem Tier ähnlich sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3149
    },
    "3150": {
      "dePart": "Bleiben wird von uns nur, was wir dem Allgemeinen geweiht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3150
    },
    "3151": {
      "dePart": "Das Schwache ist ein Charakterzug unseres Jahrhunderts.",
      "enPart": "",
      "category": "preDraft",
      "id": 3151
    },
    "3152": {
      "dePart": "Das einzige, was ich an der Freiheit liebe, ist der Kampf um sie.",
      "enPart": "",
      "category": "preDraft",
      "id": 3152
    },
    "3153": {
      "dePart": "Anmut umgibt die Eleganz und kleidet sie.",
      "enPart": "",
      "category": "preDraft",
      "id": 3153
    },
    "3154": {
      "dePart": "Stil ist der äußere Ausdruck einer inneren Harmonie der Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 3154
    },
    "3155": {
      "dePart": "Das Leben wird gegen Abend, wie die Träume gegen Morgen, immer klarer.",
      "enPart": "",
      "category": "preDraft",
      "id": 3155
    },
    "3156": {
      "dePart": "Wer das beste will, muss of das bitterste kosten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3156
    },
    "3157": {
      "dePart": "Schneller Gang ist unser Leben, lasst uns Rosen auf ihn streun.",
      "enPart": "",
      "category": "preDraft",
      "id": 3157
    },
    "3158": {
      "dePart": "Ach, wie bald schwindet Schönheit und Gestalt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3158
    },
    "3159": {
      "dePart": "Willst Du nicht Dein Herz öffnen, zu hören,was Regenbogen und Sonne lehren?",
      "enPart": "",
      "category": "preDraft",
      "id": 3159
    },
    "3160": {
      "dePart": "Das Leben erzieht die großen Menschen und lässt die kleinen laufen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3160
    },
    "3161": {
      "dePart": "Jedes Ding kann nur von einer äußeren Ursache zerstört werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3161
    },
    "3162": {
      "dePart": "Alle Dinge, die über Maß und Zeit gehen, sind von kurzer Dauer.",
      "enPart": "",
      "category": "preDraft",
      "id": 3162
    },
    "3163": {
      "dePart": "Der Abend des Lebens bringt seine Lampe mit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3163
    },
    "3164": {
      "dePart": "Die Vorzüge von gestern sind oft die Fehler von morgen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3164
    },
    "3165": {
      "dePart": "Individualität ist überall zu schonen und zu ehren als Wurzel jedes Guten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3165
    },
    "3166": {
      "dePart": "Nur die Ruhe in der Bewegunghält die Welt und macht den Mann.",
      "enPart": "",
      "category": "preDraft",
      "id": 3166
    },
    "3167": {
      "dePart": "Nicht die Armseligkeit macht den Menschen selig, sondern die Barmherzigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3167
    },
    "3168": {
      "dePart": "Wer die Welt vernünftig ansieht, den sieht auch sie vernünftig an.",
      "enPart": "",
      "category": "preDraft",
      "id": 3168
    },
    "3169": {
      "dePart": "Selbsterkenntnis ist der erste Schritt zur Heuchelei.",
      "enPart": "",
      "category": "preDraft",
      "id": 3169
    },
    "3170": {
      "dePart": "Sei weder der erste, Neuem nachzujagen,noch der letzte, Altem zu entsagen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3170
    },
    "3171": {
      "dePart": "Die Eitelkeit ist das letzte Kleid,das der Mensch auszieht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3171
    },
    "3172": {
      "dePart": "Das Leben eines jeden Menschenist ein von Gotteshand geschriebenes Märchen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3172
    },
    "3173": {
      "dePart": "Furcht macht verächtlich, und Verachtung ist gefahrvoller als Hass.",
      "enPart": "",
      "category": "preDraft",
      "id": 3173
    },
    "3174": {
      "dePart": "Die Jahre lehren viel,was die Tage niemals wissen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3174
    },
    "3175": {
      "dePart": "Eine kleine Reise tut gut, um die Welt zu erneuern.",
      "enPart": "",
      "category": "preDraft",
      "id": 3175
    },
    "3176": {
      "dePart": "Man soll nur dies fürchten, zuviel Furcht zu haben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3176
    },
    "3177": {
      "dePart": "Ernte heute und vertraue so wenig auf morgen wie nur möglich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3177
    },
    "3178": {
      "dePart": "Wer Aufopferung als Lebensinhalt akzeptiert,wird ein Opfer seiner selbst.",
      "enPart": "",
      "category": "preDraft",
      "id": 3178
    },
    "3179": {
      "dePart": "Ein Blick ins Glas gibt mehr Sehschärfe als das beste Scherenfernrohr",
      "enPart": "",
      "category": "preDraft",
      "id": 3179
    },
    "3180": {
      "dePart": "Freude treibt zum Genuß, Seligkeit zur Arbeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3180
    },
    "3181": {
      "dePart": "Der Verstand ist das einzige, was mancher verliert, ohne es besessen zu haben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3181
    },
    "3182": {
      "dePart": "Man muss im Leben wählenzwischen Langeweile und Leiden",
      "enPart": "",
      "category": "preDraft",
      "id": 3182
    },
    "3183": {
      "dePart": "In den Gerichtshöfen sollen die Gesetze sprechen und der Herrscher schweigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3183
    },
    "3184": {
      "dePart": "Modern zu sein ist das einzige, was heutzutage das Leben lebenswert macht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3184
    },
    "3185": {
      "dePart": "Verschwendete Zeit ist Dasein; gebrauchte Zeit ist Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3185
    },
    "3186": {
      "dePart": "Die Sentimentalität ist das Alibi der Hartherzigen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3186
    },
    "3187": {
      "dePart": "Es ist gut, in Bedrängnis zu leben. Das wirkt wie eine gespannte Feder",
      "enPart": "",
      "category": "preDraft",
      "id": 3187
    },
    "3188": {
      "dePart": "Welcher Raum in der Welt ist nicht schon voll von unserem Mühen?",
      "enPart": "",
      "category": "preDraft",
      "id": 3188
    },
    "3189": {
      "dePart": "Wenn nur die Zeit nicht so verginge,aber sie vergeht so wahnsinnig.",
      "enPart": "",
      "category": "preDraft",
      "id": 3189
    },
    "3190": {
      "dePart": "Die Welt verstehen nenne ich der Welt gewachsen sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3190
    },
    "3191": {
      "dePart": "Älter werde ich stets, niemals doch lerne ich aus.",
      "enPart": "",
      "category": "preDraft",
      "id": 3191
    },
    "3192": {
      "dePart": "Die Schönheit ist Lebensnahrung der Seele.",
      "enPart": "",
      "category": "preDraft",
      "id": 3192
    },
    "3193": {
      "dePart": "Tiefe Seelen brauchen keinen Weltraum, Welteroberer brauchen keine Seelentiefe.",
      "enPart": "",
      "category": "preDraft",
      "id": 3193
    },
    "3194": {
      "dePart": "Die Welt ist wahrhaft zu ernsthaft, um damit zu spaßen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3194
    },
    "3195": {
      "dePart": "Was der Mensch fliehen will, das bringt er mit an den Ort, an den er flieht",
      "enPart": "",
      "category": "preDraft",
      "id": 3195
    },
    "3196": {
      "dePart": "Etwas wagen muß das Herz und früh auf sein, wenn es leben will.",
      "enPart": "",
      "category": "preDraft",
      "id": 3196
    },
    "3197": {
      "dePart": "Wo ein Begeisterter steht,ist der Gipfel der Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3197
    },
    "3198": {
      "dePart": "Für des Menschen wilde Brust ist keine Heimat möglich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3198
    },
    "3199": {
      "dePart": "Die Entsagungen sind vergänglich, ihr Lohn aber bleibt beständig.",
      "enPart": "",
      "category": "preDraft",
      "id": 3199
    },
    "3200": {
      "dePart": "Die reichste Empfindung des Lebens liegt im Atmen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3200
    },
    "3201": {
      "dePart": "Auch der Zufall ist nicht unergründlich - er hat seine Regelmäßigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3201
    },
    "3202": {
      "dePart": "Unerfahrenheit führt zu Selbstvertrauen, Bildung zu Scheu.",
      "enPart": "",
      "category": "preDraft",
      "id": 3202
    },
    "3203": {
      "dePart": "Nur die Liebe zur Wahrheit schafft Wunder.",
      "enPart": "",
      "category": "preDraft",
      "id": 3203
    },
    "3204": {
      "dePart": "Tadeln können zwar die Toren, aber klüger handeln nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3204
    },
    "3205": {
      "dePart": "Eine schöne Jungfrau trägt ihr Heiratsgut im Angesicht",
      "enPart": "",
      "category": "preDraft",
      "id": 3205
    },
    "3206": {
      "dePart": "Es ist ein grundsätzlicher Irrtum,Heftigkeit und Starrheit Stärke zu nennen",
      "enPart": "",
      "category": "preDraft",
      "id": 3206
    },
    "3207": {
      "dePart": "Nur wer im Wohlstand lebt, schimpft auf ihn.",
      "enPart": "",
      "category": "preDraft",
      "id": 3207
    },
    "3208": {
      "dePart": "Nichts ist dem Menschen schwerer zu tragen als eine Last von guten Tagen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3208
    },
    "3209": {
      "dePart": "Eine vollkommene Ordnung wäre der Ruin allen Fortschritts und Vergnügens.",
      "enPart": "",
      "category": "preDraft",
      "id": 3209
    },
    "3210": {
      "dePart": "Trachte nicht nach dem ewigen Leben, sondern schöpfe das Mögliche aus!",
      "enPart": "",
      "category": "preDraft",
      "id": 3210
    },
    "3211": {
      "dePart": "Das Alter wägt und misst es, die Jugend spricht: So ist es.",
      "enPart": "",
      "category": "preDraft",
      "id": 3211
    },
    "3212": {
      "dePart": "Die Bekanntschaft mit einem einzigen guten Buchkann ein Leben ändern.",
      "enPart": "",
      "category": "preDraft",
      "id": 3212
    },
    "3213": {
      "dePart": "Die leere Form ist nicht Schönheit, sie ist das Gewand der Schönheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3213
    },
    "3214": {
      "dePart": "Fanatiker sind zu allem fähig, sonst aber zu nichts.",
      "enPart": "",
      "category": "preDraft",
      "id": 3214
    },
    "3215": {
      "dePart": "Wir wollen die Welt ein wenig besser verlassen, als wir sie vorgefunden haben",
      "enPart": "",
      "category": "preDraft",
      "id": 3215
    },
    "3216": {
      "dePart": "Lehren und nicht tun, das ist klein, lehren und tun, das ist groß.",
      "enPart": "",
      "category": "preDraft",
      "id": 3216
    },
    "3217": {
      "dePart": "Es ist nicht gut, dass alles geschieht, was die Menschen wünschen",
      "enPart": "",
      "category": "preDraft",
      "id": 3217
    },
    "3218": {
      "dePart": "Versuche stets, ein Stückchen Himmel über deinem Leben freizuhalten",
      "enPart": "",
      "category": "preDraft",
      "id": 3218
    },
    "3219": {
      "dePart": "Ich bin zu alt, um nur zu spielen, zu jung, um ohne Wunsch zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3219
    },
    "3220": {
      "dePart": "Das Wichtigste im Verkehr mit Menschen:ihnen ihre Ausreden wegräumen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3220
    },
    "3221": {
      "dePart": "Im Leben einer Frau zählt nur die Jugend.",
      "enPart": "",
      "category": "preDraft",
      "id": 3221
    },
    "3222": {
      "dePart": "Es kommt darauf an, dass sich ein Kapital bildet,was nie ausgeht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3222
    },
    "3223": {
      "dePart": "Der Irrtum ist, Charakter sei keinen Widerspruch in sich selbst zu dulden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3223
    },
    "3224": {
      "dePart": "Wir reden wenig, wenn wir nicht über uns reden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3224
    },
    "3225": {
      "dePart": "Jeder Luxusverdirbt die Sitten oder den Geschmack",
      "enPart": "",
      "category": "preDraft",
      "id": 3225
    },
    "3226": {
      "dePart": "Freiheit kann man einem Menschen zwar lassen,aber nicht geben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3226
    },
    "3227": {
      "dePart": "Niemand ist härter als die Sanftmütigen aus Berechnung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3227
    },
    "3228": {
      "dePart": "In der Aufstellung unserer Grundsätze sind wir strenger als in ihrer Befolgung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3228
    },
    "3229": {
      "dePart": "Der Mensch ist vor allen anderen Geschöpfenein auf Hoffnung gestelltes Wesen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3229
    },
    "3230": {
      "dePart": "Man muß die Zukunft im Sinn haben und die Vergangenheit in den Akten",
      "enPart": "",
      "category": "preDraft",
      "id": 3230
    },
    "3231": {
      "dePart": "Ein angenehmes Leben und heiteres Leben kommt nicht von äußeren Dingen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3231
    },
    "3232": {
      "dePart": "Der eigentliche Redner der Volksversammlung ist die Leidenschaft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3232
    },
    "3233": {
      "dePart": "Das Geheimnis der kleinsten natürlichen Freuden geht über die Vernunft hinaus.",
      "enPart": "",
      "category": "preDraft",
      "id": 3233
    },
    "3234": {
      "dePart": "Zu sagen, was ist, ist und bleibt die revolutionärste Tat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3234
    },
    "3235": {
      "dePart": "Wahres und Gutes wird sich versöhnen, wenn sich beide vermählen im Schönen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3235
    },
    "3236": {
      "dePart": "Wahre Menschlichkeit ist köstlicher als alle Schönheit der Erde.",
      "enPart": "",
      "category": "preDraft",
      "id": 3236
    },
    "3237": {
      "dePart": "Die Schönheit strahlt nur aus dem innern Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3237
    },
    "3238": {
      "dePart": "Wer über den See geht, wechselt den Himmel, doch nicht den Charakter.",
      "enPart": "",
      "category": "preDraft",
      "id": 3238
    },
    "3239": {
      "dePart": "Die Einfachheit des Geistes ist Dummheit, die des Herzens Unschuld.",
      "enPart": "",
      "category": "preDraft",
      "id": 3239
    },
    "3240": {
      "dePart": "Der Mittelpunkt alles Menschenverderbens ist Verhärtung des Herzens.",
      "enPart": "",
      "category": "preDraft",
      "id": 3240
    },
    "3241": {
      "dePart": "Das Alte stürzt,es ändert sich die Zeit,und neues Leben blüht aus den Ruinen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3241
    },
    "3242": {
      "dePart": "Die Freiheit der Meinung setzt voraus,dass man eine hat",
      "enPart": "",
      "category": "preDraft",
      "id": 3242
    },
    "3243": {
      "dePart": "Jeder will Freiheit haben, und niemand will sie geben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3243
    },
    "3244": {
      "dePart": "In der vollkommenen Stille hört man die ganze Welt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3244
    },
    "3245": {
      "dePart": "Die Strafe, die züchtigt ohne zu verhüten, heißt Rache.",
      "enPart": "",
      "category": "preDraft",
      "id": 3245
    },
    "3246": {
      "dePart": "Die Freiheit ist ein Gut, das alle anderen Güter zu genießen erlaubt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3246
    },
    "3247": {
      "dePart": "Der Geist läßt uns zu Göttern werden, das Fleisch zu Tieren.",
      "enPart": "",
      "category": "preDraft",
      "id": 3247
    },
    "3248": {
      "dePart": "Unbestechlichkeit ist nicht an Regeln gebunden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3248
    },
    "3249": {
      "dePart": "Das Leben gab den Sterblichen nichts ohne große Arbeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3249
    },
    "3250": {
      "dePart": "Die Jahre lehren viel, wovon der Tag keine Ahnung hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3250
    },
    "3251": {
      "dePart": "Die Hölle ist mit guten Vorsätzen gepflastert.",
      "enPart": "",
      "category": "preDraft",
      "id": 3251
    },
    "3252": {
      "dePart": "Das Alte wird nie alt, es wird nur alt das Neue.",
      "enPart": "",
      "category": "preDraft",
      "id": 3252
    },
    "3253": {
      "dePart": "Jeder Segen der Zivilisation ist entweder ein Fluch oder überflüssig.",
      "enPart": "",
      "category": "preDraft",
      "id": 3253
    },
    "3254": {
      "dePart": "Geschichte ist nichts anderes als die Unfallchronik der Menschheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3254
    },
    "3255": {
      "dePart": "Dein tägliches Quantum Sonnenscheinmußt du dir täglich selbst verdienen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3255
    },
    "3256": {
      "dePart": "In deiner Zelle erlangst du wieder,was du draußen häufig verlierst",
      "enPart": "",
      "category": "preDraft",
      "id": 3256
    },
    "3257": {
      "dePart": "Die Waage gleicht der großen Welt:Das Leichte steigt, das Schwere fällt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3257
    },
    "3258": {
      "dePart": "Eine Sache, die niemand glaubt, kann nicht oft genug bewiesen werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3258
    },
    "3259": {
      "dePart": "Wir verdanken den Leidenschaften vielleicht die größten Vorzüge des Verstandes",
      "enPart": "",
      "category": "preDraft",
      "id": 3259
    },
    "3260": {
      "dePart": "Verstanden zu werden bedeutet heutzutage, ertappt zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3260
    },
    "3261": {
      "dePart": "Das einzige, was der Illusionslose respektiert, ist die Hoffnung",
      "enPart": "",
      "category": "preDraft",
      "id": 3261
    },
    "3262": {
      "dePart": "Niemand tadelt uns so scharf,wie wir uns oft selbst verurteilen",
      "enPart": "",
      "category": "preDraft",
      "id": 3262
    },
    "3263": {
      "dePart": "Wenn man die Verteidigung nicht widerlegen kann, tadelt man die Art derselben",
      "enPart": "",
      "category": "preDraft",
      "id": 3263
    },
    "3264": {
      "dePart": "Umsonst fragst du die Schrift um Auskunft, frage lieber die Erfahrung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3264
    },
    "3265": {
      "dePart": "Wer nicht weiß, dass er eine Maske trägt,trägt sie am vollkommensten",
      "enPart": "",
      "category": "preDraft",
      "id": 3265
    },
    "3266": {
      "dePart": "Die nicht zivilisierte, unverdorbene Welt ist das beste Verjüngungsmittel.",
      "enPart": "",
      "category": "preDraft",
      "id": 3266
    },
    "3267": {
      "dePart": "Ich bin immer töricht gewesen, an das Gute im Menschen zu glauben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3267
    },
    "3268": {
      "dePart": "Die schlechte Absicht ist immer auf den Beinen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3268
    },
    "3269": {
      "dePart": "Menschlich und edel ist das Gute, göttlich und unsterblich aber das Schöne.",
      "enPart": "",
      "category": "preDraft",
      "id": 3269
    },
    "3270": {
      "dePart": "Die Wahrheit liegt zwischen zwei Extremen, aber nicht in der Mitte.",
      "enPart": "",
      "category": "preDraft",
      "id": 3270
    },
    "3271": {
      "dePart": "Der Traum ist unser Versucher und unser Richter.",
      "enPart": "",
      "category": "preDraft",
      "id": 3271
    },
    "3272": {
      "dePart": "Nichts lernt sich so leicht wie der Luxus, auch der des Geistes.",
      "enPart": "",
      "category": "preDraft",
      "id": 3272
    },
    "3273": {
      "dePart": "Eigensinn ist die Parodie des Charakters.",
      "enPart": "",
      "category": "preDraft",
      "id": 3273
    },
    "3274": {
      "dePart": "Die Erfahrung verfehlt ihren Lebenszweck,wo kein Wunsch zu lernen besteht",
      "enPart": "",
      "category": "preDraft",
      "id": 3274
    },
    "3275": {
      "dePart": "Österreich ist eine kleine Welt,in der die große ihre Probe hält.",
      "enPart": "",
      "category": "preDraft",
      "id": 3275
    },
    "3276": {
      "dePart": "Der Mensch muss sich stets auf neue Überraschungen gefasst machen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3276
    },
    "3277": {
      "dePart": "Die Schienbeine und die Hoffnungensoll man nicht zu weit hinausstrecken.",
      "enPart": "",
      "category": "preDraft",
      "id": 3277
    },
    "3278": {
      "dePart": "Worte sind wie Laub - wo sie im Übermaß sind, findet man keine Früchte darunter",
      "enPart": "",
      "category": "preDraft",
      "id": 3278
    },
    "3279": {
      "dePart": "Gerade das Gegenteil tun ist auch eine Nachahmung",
      "enPart": "",
      "category": "preDraft",
      "id": 3279
    },
    "3280": {
      "dePart": "Man soll nie das Wohl, man soll das Heil eines Menschen im Auge haben",
      "enPart": "",
      "category": "preDraft",
      "id": 3280
    },
    "3281": {
      "dePart": "Schlimmer als sterben ist die schimpfliche Ruhe.",
      "enPart": "",
      "category": "preDraft",
      "id": 3281
    },
    "3282": {
      "dePart": "Der Tor hat Angst vorm Schicksal, der Weise trägt es.",
      "enPart": "",
      "category": "preDraft",
      "id": 3282
    },
    "3283": {
      "dePart": "Deine Seele ist eine auserlesene Landschaft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3283
    },
    "3284": {
      "dePart": "Mir scheint, dass wir alles erst dann erleben,wenn's schon vorüber ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3284
    },
    "3285": {
      "dePart": "Wenn's auch nicht wahr wäre, man wird doch immer sagen: die gute alte Zeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3285
    },
    "3286": {
      "dePart": "Jeder glaubt gar leicht, was er fürchtet und was er wünscht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3286
    },
    "3287": {
      "dePart": "Alles auf einmal tun zu wollen zerstört alles auf einmal.",
      "enPart": "",
      "category": "preDraft",
      "id": 3287
    },
    "3288": {
      "dePart": "Originelle Formulierungen sind noch lange nicht originelle Einsichten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3288
    },
    "3289": {
      "dePart": "Sentimentalität und Grausamkeitsind leibliche Schwestern.",
      "enPart": "",
      "category": "preDraft",
      "id": 3289
    },
    "3290": {
      "dePart": "Es gibt nichts Beständigeres als die Unbeständigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3290
    },
    "3291": {
      "dePart": "Alles ist komisch, solange es jemand anderem passiert.",
      "enPart": "",
      "category": "preDraft",
      "id": 3291
    },
    "3292": {
      "dePart": "Wo keine Hoffnung ist,gibt es kein Sterben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3292
    },
    "3293": {
      "dePart": "Ein jeder Tag bricht dir was abvon deiner Schönheit bis ins Grab.",
      "enPart": "",
      "category": "preDraft",
      "id": 3293
    },
    "3294": {
      "dePart": "In bescheidenen Verhältnissen ist es leicht, Gleichheit walten zu lassen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3294
    },
    "3295": {
      "dePart": "Das Ziel des Lebens ist Freiheit von Wahn.",
      "enPart": "",
      "category": "preDraft",
      "id": 3295
    },
    "3296": {
      "dePart": "Wer jung ist, glaubt nie, dass er alt wird.",
      "enPart": "",
      "category": "preDraft",
      "id": 3296
    },
    "3297": {
      "dePart": "Tüchtigkeit ist nur ein Wert unter vielen - Lebenskunst ist ein anderer.",
      "enPart": "",
      "category": "preDraft",
      "id": 3297
    },
    "3298": {
      "dePart": "Das Landleben kann man nicht erlernen, auch nicht aus Büchern.",
      "enPart": "",
      "category": "preDraft",
      "id": 3298
    },
    "3299": {
      "dePart": "Alles Große und Wertvolle ist mit Einsamkeit verknüpft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3299
    },
    "3300": {
      "dePart": "Erfahrung ist eine so schöne Sache, dass man sie nicht teuer genug kaufen kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 3300
    },
    "3301": {
      "dePart": "Ein Feind, über den man im Bilde ist, ist weniger gefährlich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3301
    },
    "3302": {
      "dePart": "Im Kampf gegen die Armut entstehen alle großen menschlichen Leistungen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3302
    },
    "3303": {
      "dePart": "Durch albernes Geheule wird nie was in er Welt gebessert.",
      "enPart": "",
      "category": "preDraft",
      "id": 3303
    },
    "3304": {
      "dePart": "Wir schätzen die Tugend, aber wir erwähnen sie nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3304
    },
    "3305": {
      "dePart": "Man muss nicht viel sprechen, man muss wesentlich sprechen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3305
    },
    "3306": {
      "dePart": "Nichts in der Geschichte des Lebens ist beständiger als der Wandel.",
      "enPart": "",
      "category": "preDraft",
      "id": 3306
    },
    "3307": {
      "dePart": "Wenn alle müde werden - die Kraft einer Mutter erlahmt nie.",
      "enPart": "",
      "category": "preDraft",
      "id": 3307
    },
    "3308": {
      "dePart": "Wohin kämen wir, wenn nicht ab und zu etwas unseren Weg versperrte?",
      "enPart": "",
      "category": "preDraft",
      "id": 3308
    },
    "3309": {
      "dePart": "Enthusiasmus ist das schönste Wort der Erde.",
      "enPart": "",
      "category": "preDraft",
      "id": 3309
    },
    "3310": {
      "dePart": "Man muß das Leben so einrichten, dass jeder Augenblick bedeutungsvoll ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3310
    },
    "3311": {
      "dePart": "Der Hund bellt, wenn ihn etwas erregt.Der Mensch spricht immer",
      "enPart": "",
      "category": "preDraft",
      "id": 3311
    },
    "3312": {
      "dePart": "Solange das Herz noch Erinnerungen bewahrt, bewahrt der Geist seine Illusion",
      "enPart": "",
      "category": "preDraft",
      "id": 3312
    },
    "3313": {
      "dePart": "Nichts ist trügerischer als eine offenkundige Tatsache.",
      "enPart": "",
      "category": "preDraft",
      "id": 3313
    },
    "3314": {
      "dePart": "Der menschlichen Kunst der Täuschung sind Grenzen gezogen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3314
    },
    "3315": {
      "dePart": "Warum können Engel fliegen?Weil sie sich leicht nehmen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3315
    },
    "3316": {
      "dePart": "Die Richtung unseres Geistes ist wichtigerals sein Fortschritt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3316
    },
    "3317": {
      "dePart": "Mache dieses Herz vollkommen und so, wie du es haben willst.",
      "enPart": "",
      "category": "preDraft",
      "id": 3317
    },
    "3318": {
      "dePart": "Es gibt keine andere Motivation, auf Berge zu steigen, als den Egoismus.",
      "enPart": "",
      "category": "preDraft",
      "id": 3318
    },
    "3319": {
      "dePart": "Reifer werden heißt,schärfer trennen, inniger verbinden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3319
    },
    "3320": {
      "dePart": "Der Mensch ist seiner Natur nach eine tapfere Bestie.",
      "enPart": "",
      "category": "preDraft",
      "id": 3320
    },
    "3321": {
      "dePart": "Zum Selbstgefühl erwachen, heißt schon frei sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3321
    },
    "3322": {
      "dePart": "Nichts ist verloren, wo der Same des Guten bleibt",
      "enPart": "",
      "category": "preDraft",
      "id": 3322
    },
    "3323": {
      "dePart": "Eine Seele ohne Zwang ist auch ohne Laster.",
      "enPart": "",
      "category": "preDraft",
      "id": 3323
    },
    "3324": {
      "dePart": "Ein gut beschriebenes Leben ist fast so selten wie ein gut gelebtes.",
      "enPart": "",
      "category": "preDraft",
      "id": 3324
    },
    "3325": {
      "dePart": "Der Ruf zeigt das Urteil der Umwelt, das Gewissen das Urteil Gottes an.",
      "enPart": "",
      "category": "preDraft",
      "id": 3325
    },
    "3326": {
      "dePart": "Das Geheimnis, zu gefallen, besteht darin, kein solches Geheimnis zu haben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3326
    },
    "3327": {
      "dePart": "Der Glaube versetzt Berge, der Zweifel erklettert sie.",
      "enPart": "",
      "category": "preDraft",
      "id": 3327
    },
    "3328": {
      "dePart": "Kain und Abel bilden so ziemlich den ganzen Inhalt der Weltgeschichte",
      "enPart": "",
      "category": "preDraft",
      "id": 3328
    },
    "3329": {
      "dePart": "Der Fanatismus ist verderblicher als der Atheismus.",
      "enPart": "",
      "category": "preDraft",
      "id": 3329
    },
    "3330": {
      "dePart": "Ein jeder Kelch verschäumt, das Schönste welkt, und nichts auf Erden währt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3330
    },
    "3331": {
      "dePart": "Das Wesentlichebefindet sich unter einem Berg täglichen Kleinkrams.",
      "enPart": "",
      "category": "preDraft",
      "id": 3331
    },
    "3332": {
      "dePart": "Das Bemühen zu lebenwährt lebenslänglich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3332
    },
    "3333": {
      "dePart": "Wer Vorschriften braucht,der soll sie machen — aber nur für sich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3333
    },
    "3334": {
      "dePart": "Die gute alte Zeitist die gute alte Zeit,weil man damals jung gewesen ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3334
    },
    "3335": {
      "dePart": "Wer begreift, was er fühlt,kann für sich handeln.",
      "enPart": "",
      "category": "preDraft",
      "id": 3335
    },
    "3336": {
      "dePart": "Alt zu werden ist eine Gnade —aber nicht immer ein Vergnügen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3336
    },
    "3337": {
      "dePart": "Gleichgültigkeitnimmt die Farbe aus dem Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3337
    },
    "3338": {
      "dePart": "Erfahrung vermehrt unsere Weisheit, verringert aber nicht unsere Torheiten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3338
    },
    "3339": {
      "dePart": "Durch den Gebrauch unserer zwei Augen wird unser Leben nicht besser",
      "enPart": "",
      "category": "preDraft",
      "id": 3339
    },
    "3340": {
      "dePart": "Die besten Wahrheiten sind die leisesten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3340
    },
    "3341": {
      "dePart": "Ich liebe alles, was alt: Alte Freunde, Zeiten, Bräuche, Bücher und Weine.",
      "enPart": "",
      "category": "preDraft",
      "id": 3341
    },
    "3342": {
      "dePart": "Die Hoffnung aufgeben bedeutet, nach der Gegenwart auch die Zukunft preisgeben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3342
    },
    "3343": {
      "dePart": "Man muss den schönsten Tag nicht vor dem Abend loben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3343
    },
    "3344": {
      "dePart": "Was besteht und wirkt, muss einen Grund seines Bestehens und Wirkens haben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3344
    },
    "3345": {
      "dePart": "Der irrende Glaube ist immer der tiefste.",
      "enPart": "",
      "category": "preDraft",
      "id": 3345
    },
    "3346": {
      "dePart": "Ich lehne es ab, anzuerkennen, dass es Unmöglichkeiten gibt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3346
    },
    "3347": {
      "dePart": "Jede Bedrängnis ist nur ein Engpass zu einer Weite.",
      "enPart": "",
      "category": "preDraft",
      "id": 3347
    },
    "3348": {
      "dePart": "Eine große Landstraß' ist unsere Erd, wir Menschen sind Passagiere.",
      "enPart": "",
      "category": "preDraft",
      "id": 3348
    },
    "3349": {
      "dePart": "Die Dummheit schützt vor Schande, gleich wie die Dreistigkeit vor Armut.",
      "enPart": "",
      "category": "preDraft",
      "id": 3349
    },
    "3350": {
      "dePart": "Alles hängt vom Geschmack ab und von der Fähigkeit, die Dinge anzuordnen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3350
    },
    "3351": {
      "dePart": "Jede große Zeit erfasst den ganzen Menschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3351
    },
    "3352": {
      "dePart": "Das Ideal ist nichts als die Wahrheit von weitem.",
      "enPart": "",
      "category": "preDraft",
      "id": 3352
    },
    "3353": {
      "dePart": "Der Weise mit Hoffnung erträgt die Qualen des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 3353
    },
    "3354": {
      "dePart": "Vernunft ist Begabung, Dummheit Veranlagung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3354
    },
    "3355": {
      "dePart": "Das Leben ist eine Krankheit, die tödlich endet",
      "enPart": "",
      "category": "preDraft",
      "id": 3355
    },
    "3356": {
      "dePart": "Ein Grab ist doch immer die beste Befestigungwider die Stürme des Schicksals.",
      "enPart": "",
      "category": "preDraft",
      "id": 3356
    },
    "3357": {
      "dePart": "Die Zukunft wird von der Gegenwart verfolgt",
      "enPart": "",
      "category": "preDraft",
      "id": 3357
    },
    "3358": {
      "dePart": "Um Wunder zu erleben, muss man an sie glauben",
      "enPart": "",
      "category": "preDraft",
      "id": 3358
    },
    "3359": {
      "dePart": "Jede wahre Kraft, der entgegengewirkt wird, verstärkt sich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3359
    },
    "3360": {
      "dePart": "Idole und Ideen sind wandelbar. Ideale allein sind unsterblich",
      "enPart": "",
      "category": "preDraft",
      "id": 3360
    },
    "3361": {
      "dePart": "Wie viele Fehler geschehen in der Welt,weil man ohne Überlegung zu Werke geht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3361
    },
    "3362": {
      "dePart": "Der Hauptfehler des Menschen bleibt, dass er so viele kleine hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3362
    },
    "3363": {
      "dePart": "Der Ziellose erleidet sein Schicksal, der Zielbewußte gestaltet es.",
      "enPart": "",
      "category": "preDraft",
      "id": 3363
    },
    "3364": {
      "dePart": "Die Leuchte des Geistes ohne Wärme des Herzens wird oft zum Irrlicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3364
    },
    "3365": {
      "dePart": "Das große unzerstörbare Wunder ist der Menschenglaube an Wunder.",
      "enPart": "",
      "category": "preDraft",
      "id": 3365
    },
    "3366": {
      "dePart": "Zu den Blitzen des Geniesmachen die Talente den Donner",
      "enPart": "",
      "category": "preDraft",
      "id": 3366
    },
    "3367": {
      "dePart": "Man kann sich das Leben auch durch zu großen Ernst verscherzen",
      "enPart": "",
      "category": "preDraft",
      "id": 3367
    },
    "3368": {
      "dePart": "Man kann einen kleinen Mundund doch ein großes Maul haben",
      "enPart": "",
      "category": "preDraft",
      "id": 3368
    },
    "3369": {
      "dePart": "Ein Ton aus unserer Jugendzeit wird oft zur Dominante für das ganze Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3369
    },
    "3370": {
      "dePart": "Wem das Leben keine Lasten auflädt, dem wird es selbst zur Last.",
      "enPart": "",
      "category": "preDraft",
      "id": 3370
    },
    "3371": {
      "dePart": "Das Volk muß für sein Gesetz kämpfen wie für seine Stadtmauer",
      "enPart": "",
      "category": "preDraft",
      "id": 3371
    },
    "3372": {
      "dePart": "Wer die Wahrheit hat, für den ist Erfolg und Misserfolg dasselbe",
      "enPart": "",
      "category": "preDraft",
      "id": 3372
    },
    "3373": {
      "dePart": "Der Schwache zweifelt vor der Entscheidung; der Starke danach.",
      "enPart": "",
      "category": "preDraft",
      "id": 3373
    },
    "3374": {
      "dePart": "Jugend ist kein Lebensabschnitt,sondern eine Geisteshaltung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3374
    },
    "3375": {
      "dePart": "Ein Volk wird danach beurteilt, wie es seine Toten bestattet",
      "enPart": "",
      "category": "preDraft",
      "id": 3375
    },
    "3376": {
      "dePart": "Zu lange Opfer zu bringen kann das Herz versteinern.",
      "enPart": "",
      "category": "preDraft",
      "id": 3376
    },
    "3377": {
      "dePart": "Das Streben nach Vollkommenheit macht manchen Menschen vollkommen unerträglich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3377
    },
    "3378": {
      "dePart": "Der Anfang des rechten Lebens ist das rechte Hören.",
      "enPart": "",
      "category": "preDraft",
      "id": 3378
    },
    "3379": {
      "dePart": "Es genügt nicht, dass die Hände sauber sind.Es bedarf auch eines reinen Geistes",
      "enPart": "",
      "category": "preDraft",
      "id": 3379
    },
    "3380": {
      "dePart": "Schwachheit ist das größte Unglückund der größte Fehler",
      "enPart": "",
      "category": "preDraft",
      "id": 3380
    },
    "3381": {
      "dePart": "Der Mensch weiß wohl um das Gute, auch wenn er es nicht tut.",
      "enPart": "",
      "category": "preDraft",
      "id": 3381
    },
    "3382": {
      "dePart": "Instinkt ist nichts anderes als die Intelligenz der Schöpfung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3382
    },
    "3383": {
      "dePart": "Die Geschichte der Menschheitwie des Einzelnenbeginnt mit einem Traum",
      "enPart": "",
      "category": "preDraft",
      "id": 3383
    },
    "3384": {
      "dePart": "Zur Energie des Handelnsgehört auch die Energie des Entsagens",
      "enPart": "",
      "category": "preDraft",
      "id": 3384
    },
    "3385": {
      "dePart": "Wer niemals träumt,verschläft sein schönstes Leben",
      "enPart": "",
      "category": "preDraft",
      "id": 3385
    },
    "3386": {
      "dePart": "Überlasse die Entscheidung nicht der Leidenschaft, sondern dem Verstande!",
      "enPart": "",
      "category": "preDraft",
      "id": 3386
    },
    "3387": {
      "dePart": "Alle wichtigen Fragen entscheiden sich besser in der Nacht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3387
    },
    "3388": {
      "dePart": "Talent haben ist das Beste, das zweite, etwas lernen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3388
    },
    "3389": {
      "dePart": "Deine natürlichen Fähigkeiten reichen nicht aus, um dich bedeutsam zu machen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3389
    },
    "3390": {
      "dePart": "Nichts können ist noch lange keine neue Richtung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3390
    },
    "3391": {
      "dePart": "Jede Zufluchtsstätte der Vergangenheit ist ein Gefängnis.",
      "enPart": "",
      "category": "preDraft",
      "id": 3391
    },
    "3392": {
      "dePart": "Der Himmel hilft niemals denen, die nicht handeln",
      "enPart": "",
      "category": "preDraft",
      "id": 3392
    },
    "3393": {
      "dePart": "Die Sinne sind uns Brücke vom Unfassbaren zum Fassbaren.",
      "enPart": "",
      "category": "preDraft",
      "id": 3393
    },
    "3394": {
      "dePart": "Die Welt ist ein Schauplatz: Du kommst, du siehst und gehst vorüber",
      "enPart": "",
      "category": "preDraft",
      "id": 3394
    },
    "3395": {
      "dePart": "Enthusiasmus ist das Leben der Seele, Schwärmerei ihr Tod.",
      "enPart": "",
      "category": "preDraft",
      "id": 3395
    },
    "3396": {
      "dePart": "Es ist eines Menschen Zunge, nicht die Tat, die alles lenkt",
      "enPart": "",
      "category": "preDraft",
      "id": 3396
    },
    "3397": {
      "dePart": "Das Leben ist fast zu gemein, um ertragen zu werden",
      "enPart": "",
      "category": "preDraft",
      "id": 3397
    },
    "3398": {
      "dePart": "In der Provinz ist schon Regen eine Zerstreuung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3398
    },
    "3399": {
      "dePart": "Leichtgläubigkeit ist eines Mannes Schwäche und eines Kindes Stärke",
      "enPart": "",
      "category": "preDraft",
      "id": 3399
    },
    "3400": {
      "dePart": "Man behält immer die Spuren seiner Abstammung",
      "enPart": "",
      "category": "preDraft",
      "id": 3400
    },
    "3401": {
      "dePart": "Wenn das Herz am rechten Fleck ist, spielt es keine Rolle, wo der Kopf ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3401
    },
    "3402": {
      "dePart": "Die Menschenrechte haben kein Geschlecht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3402
    },
    "3403": {
      "dePart": "Es gibt ein Lachen, das sich wie der Klang gefälschter Münzen anhört.",
      "enPart": "",
      "category": "preDraft",
      "id": 3403
    },
    "3404": {
      "dePart": "Häufig ist die Prophezeiung die Hauptursache für das prophezeite Ereignis.",
      "enPart": "",
      "category": "preDraft",
      "id": 3404
    },
    "3405": {
      "dePart": "Das Gewissen eines jeden Bürgers ist sein Gesetz.",
      "enPart": "",
      "category": "preDraft",
      "id": 3405
    },
    "3406": {
      "dePart": "Das große Ziel des Lebens ist nicht Wissen, sondern Handeln",
      "enPart": "",
      "category": "preDraft",
      "id": 3406
    },
    "3407": {
      "dePart": "Ein Ziel im Leben ist das einzige Vermögen, das sich zu erwerben lohnt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3407
    },
    "3408": {
      "dePart": "Die Lust der Zerstörung ist gleichzeitig eine schaffende Lust.",
      "enPart": "",
      "category": "preDraft",
      "id": 3408
    },
    "3409": {
      "dePart": "Des Lebens größte Krankheit ist die Langeweile.",
      "enPart": "",
      "category": "preDraft",
      "id": 3409
    },
    "3410": {
      "dePart": "Das Gedächtnis ist so kurz und das Leben so lang.",
      "enPart": "",
      "category": "preDraft",
      "id": 3410
    },
    "3411": {
      "dePart": "Alles, was wir wirklich akzeptieren, unterliegt dem Wandel.",
      "enPart": "",
      "category": "preDraft",
      "id": 3411
    },
    "3412": {
      "dePart": "Es ist den Menschen allen eins gemeinsam: fehlzugehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3412
    },
    "3413": {
      "dePart": "Nur die mutigsten Träume sollten das Aufwachen riskieren.",
      "enPart": "",
      "category": "preDraft",
      "id": 3413
    },
    "3414": {
      "dePart": "Die Leidenschaften ändern sich nicht,aber Sitte, Brauch und Meinung ändern sich",
      "enPart": "",
      "category": "preDraft",
      "id": 3414
    },
    "3415": {
      "dePart": "Die Zeit ist nur ein Wort oder ein Maß, aber kein reales Wesen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3415
    },
    "3416": {
      "dePart": "Doch wie einen Zug im Brettspiel das Leben zu wiederholen das gibt es nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3416
    },
    "3417": {
      "dePart": "Die Symphonie unseres Lebens besteht aus den Motiven unserer Jugend",
      "enPart": "",
      "category": "preDraft",
      "id": 3417
    },
    "3418": {
      "dePart": "Nur in der Bewegung, so schmerzlich sie sei, ist Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3418
    },
    "3419": {
      "dePart": "Das Schicksal setzt den Hobel an und hobelt alle gleich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3419
    },
    "3420": {
      "dePart": "Das Leben ist zu kurz für eine große Geschichte.",
      "enPart": "",
      "category": "preDraft",
      "id": 3420
    },
    "3421": {
      "dePart": "Nichts schädigt das Individuum mehr als Selbstverkleinerung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3421
    },
    "3422": {
      "dePart": "Die Mutter eines Ängstlichen pflegt nicht zu weinen",
      "enPart": "",
      "category": "preDraft",
      "id": 3422
    },
    "3423": {
      "dePart": "Das Schwierige im Leben ist die Entscheidung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3423
    },
    "3424": {
      "dePart": "Das Wort \"Familienbande\" hat einen Beigeschmack von Wahrheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3424
    },
    "3425": {
      "dePart": "Wer das Laufen erdacht, war ein kluger Mann; es rette aus mancher Not.",
      "enPart": "",
      "category": "preDraft",
      "id": 3425
    },
    "3426": {
      "dePart": "Die Reue nähert uns mehr dem Ewigen, als uns die Sünde von ihm entfernt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3426
    },
    "3427": {
      "dePart": "Der Mann hat anderes zu tun als der Jüngling",
      "enPart": "",
      "category": "preDraft",
      "id": 3427
    },
    "3428": {
      "dePart": "Ein ganz klein wenig Süßes kann viel Bitteres verschwinden lassen",
      "enPart": "",
      "category": "preDraft",
      "id": 3428
    },
    "3429": {
      "dePart": "Es gibt ja nichts so Unwilliges wie das Alter.",
      "enPart": "",
      "category": "preDraft",
      "id": 3429
    },
    "3430": {
      "dePart": "Das Leben eilt dahin, und auch nicht eine kurze Zeit lässt es sich halten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3430
    },
    "3431": {
      "dePart": "Das Leben, das Ende und den Tag, lobt erst der Abend.",
      "enPart": "",
      "category": "preDraft",
      "id": 3431
    },
    "3432": {
      "dePart": "Alle Lust der Welt ist ein kurzer Traum nur!",
      "enPart": "",
      "category": "preDraft",
      "id": 3432
    },
    "3433": {
      "dePart": "Auf Erden die größte Pein heißt: auf Kommando geistreich sein",
      "enPart": "",
      "category": "preDraft",
      "id": 3433
    },
    "3434": {
      "dePart": "Wer alles werden will, wird nie vollkommen werden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3434
    },
    "3435": {
      "dePart": "Mut und Geduld kann alles zwingen,Not lehrt uns empfindsam sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3435
    },
    "3436": {
      "dePart": "Die Philosophie schweigt,wo die Gerechtigkeit den Verstand verliert",
      "enPart": "",
      "category": "preDraft",
      "id": 3436
    },
    "3437": {
      "dePart": "Jeder Schritt im Leben ist ein Schritt dem Tode entgegen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3437
    },
    "3438": {
      "dePart": "Das Leben ist eine Anstrengung, die einer besseren Sache würdig wäre.",
      "enPart": "",
      "category": "preDraft",
      "id": 3438
    },
    "3439": {
      "dePart": "Das Leben ist einfach ein verdammtes Ding nach dem anderen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3439
    },
    "3440": {
      "dePart": "Das Leben ist eine Quarantäne für das Paradies",
      "enPart": "",
      "category": "preDraft",
      "id": 3440
    },
    "3441": {
      "dePart": "Wer im Leben keine Richter hat, dem zahlt der Tod seine Missetat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3441
    },
    "3442": {
      "dePart": "Zwar entwerfen wir unsere Häuser, aber später prägen sie uns.",
      "enPart": "",
      "category": "preDraft",
      "id": 3442
    },
    "3443": {
      "dePart": "Selbst der Fleißigste ist ohne Phantasie nichts",
      "enPart": "",
      "category": "preDraft",
      "id": 3443
    },
    "3444": {
      "dePart": "Selbstlosigkeit ist ausgereifter Egoismus",
      "enPart": "",
      "category": "preDraft",
      "id": 3444
    },
    "3445": {
      "dePart": "Leben ist die dauernde Anpassung innerer Beziehung an äussere.",
      "enPart": "",
      "category": "preDraft",
      "id": 3445
    },
    "3446": {
      "dePart": "Der Mensch ist entweder Opfer seines Schicksals oder Meister seiner Bestimmung",
      "enPart": "",
      "category": "preDraft",
      "id": 3446
    },
    "3447": {
      "dePart": "Die Gerechtigkeit hat den Vorrang vor der Wohltätigkeit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3447
    },
    "3448": {
      "dePart": "Es gibt Menschen, die immer in Biedermeiertracht herumlaufen sollten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3448
    },
    "3449": {
      "dePart": "Allen Menschen ist es gegeben, sich selbst zu erkennen und klug zu sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3449
    },
    "3450": {
      "dePart": "Ohne Seele wäre die Welt ein Haufen Kehricht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3450
    },
    "3451": {
      "dePart": "Auf irgendeine Weise sind alle Menschen Krüppel",
      "enPart": "",
      "category": "preDraft",
      "id": 3451
    },
    "3452": {
      "dePart": "Die Geschichte ist eine ewige Wiederholung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3452
    },
    "3453": {
      "dePart": "Nirgends ist man so gut aufgehoben wie in einem Sarg.",
      "enPart": "",
      "category": "preDraft",
      "id": 3453
    },
    "3454": {
      "dePart": "Verantwortung entsteht dadurch, dass man nicht rechtzeitig Nein sagt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3454
    },
    "3455": {
      "dePart": "Der Mensch war immer Mensch, voll Unvollkommenheit.",
      "enPart": "",
      "category": "preDraft",
      "id": 3455
    },
    "3456": {
      "dePart": "Wir wissen, wie wir leben sollen, und leben wie zuvor.",
      "enPart": "",
      "category": "preDraft",
      "id": 3456
    },
    "3457": {
      "dePart": "Die schlimmste Knechtschaft ist: ein Sklave der Sinne sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3457
    },
    "3458": {
      "dePart": "Das Schweigen allein ist groß; alles andere ist Schwäche.",
      "enPart": "",
      "category": "preDraft",
      "id": 3458
    },
    "3459": {
      "dePart": "Liebt das, was ihr zum zweiten Mal nie wiedersehen werdet.",
      "enPart": "",
      "category": "preDraft",
      "id": 3459
    },
    "3460": {
      "dePart": "Der Mensch ist dazu da, um zu lieben, und wenn er nicht liebt, lebt er nicht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3460
    },
    "3461": {
      "dePart": "Man soll frei sein, aber man soll gerecht und maßvoll sein.",
      "enPart": "",
      "category": "preDraft",
      "id": 3461
    },
    "3462": {
      "dePart": "Die Toren der Vergangenheit sind die Weisen der Zukunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3462
    },
    "3463": {
      "dePart": "\"Alte Erfahrungen\" nennt man jene, die immer neu gemacht werden müssen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3463
    },
    "3464": {
      "dePart": "In jedem Jahr sind die schönsten Tage am kürzesten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3464
    },
    "3465": {
      "dePart": "Leidenschaft ist konsequenter als Vernunft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3465
    },
    "3466": {
      "dePart": "Zu den gewandtesten Advokaten dieser Welt gehört das Gewissen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3466
    },
    "3467": {
      "dePart": "Zu den schlechtesten Logikern gehören Furcht und Hoffnung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3467
    },
    "3468": {
      "dePart": "Das Leben ist ein Gemälde – um so schöner, je ungenauer man es betrachtet.",
      "enPart": "",
      "category": "preDraft",
      "id": 3468
    },
    "3469": {
      "dePart": "Unsere Jugend sammelt nur Seufzer für das Alter",
      "enPart": "",
      "category": "preDraft",
      "id": 3469
    },
    "3470": {
      "dePart": "Der betrauert die Toten, der nach ihrem Wunsche lebt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3470
    },
    "3471": {
      "dePart": "Der Mensch hört früher auf zu fühlen als zu hoffen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3471
    },
    "3472": {
      "dePart": "Wir bemerken die Zeit nur aus ihrem Verluste.",
      "enPart": "",
      "category": "preDraft",
      "id": 3472
    },
    "3473": {
      "dePart": "Nicht in dem Großen liegt das Gute,sondern in dem Guten liegt das Große.",
      "enPart": "",
      "category": "preDraft",
      "id": 3473
    },
    "3474": {
      "dePart": "Der ist elend, der den Tod wünscht; noch elender aber der, der ihn fürchtet.",
      "enPart": "",
      "category": "preDraft",
      "id": 3474
    },
    "3475": {
      "dePart": "Es gibt nacheinander und nebeneinander unzählig viele Welten.",
      "enPart": "",
      "category": "preDraft",
      "id": 3475
    },
    "3476": {
      "dePart": "Geist ist Mut zum Leben, wo man ihn nicht für möglich halten sollte.",
      "enPart": "",
      "category": "preDraft",
      "id": 3476
    },
    "3477": {
      "dePart": "Wir sind, was wir müssen, und nicht, was wir wünschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3477
    },
    "3478": {
      "dePart": "Wer dem Leben gegenüber gleichgültig bleibt, ist verantwortungslos.",
      "enPart": "",
      "category": "preDraft",
      "id": 3478
    },
    "3479": {
      "dePart": "Die Gewalt besitzt nicht halb so viel Macht wie die Milde.",
      "enPart": "",
      "category": "preDraft",
      "id": 3479
    },
    "3480": {
      "dePart": "Der neue Besen kehrt gut, bevor er voll Staub ist",
      "enPart": "",
      "category": "preDraft",
      "id": 3480
    },
    "3481": {
      "dePart": "So guten Bogen gibt es nicht,der überspannt und nicht zerbricht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3481
    },
    "3482": {
      "dePart": "Wer um alle Dinge sorgen will, den erwartet Leides viel.",
      "enPart": "",
      "category": "preDraft",
      "id": 3482
    },
    "3483": {
      "dePart": "Es ist betrogen deren Mut,die sich selber dünken gut.",
      "enPart": "",
      "category": "preDraft",
      "id": 3483
    },
    "3484": {
      "dePart": "Auf Erden nichts geschaffen ist,was hat Bestand für lange Frist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3484
    },
    "3485": {
      "dePart": "Je mehr sich die Dinge ändern, um so mehr bleiben sie sich gleich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3485
    },
    "3486": {
      "dePart": "Wir können ohne Freude leben, aber nicht ohne Hoffnung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3486
    },
    "3487": {
      "dePart": "Wem's Haus brennt, dem ist Eilen gut, sonst nicht",
      "enPart": "",
      "category": "preDraft",
      "id": 3487
    },
    "3488": {
      "dePart": "Vergesst nie, dass auch euer Tun einmal Tradition wird.",
      "enPart": "",
      "category": "preDraft",
      "id": 3488
    },
    "3489": {
      "dePart": "Ehe sie den Wert des Lebens erkannt,haben die meisten es schon halb durcheilt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3489
    },
    "3490": {
      "dePart": "Einsamkeit heißt Wirksamkeit für den,der Erinnerung und Ziel besitzt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3490
    },
    "3491": {
      "dePart": "Dem Tatendrange geweiht sei Dein Morgenund den Erinnerungen Dein Abend.",
      "enPart": "",
      "category": "preDraft",
      "id": 3491
    },
    "3492": {
      "dePart": "Hoffnung heißt der Morgenstern,Sehnsucht der Abendstern.",
      "enPart": "",
      "category": "preDraft",
      "id": 3492
    },
    "3493": {
      "dePart": "Man kann sein Vaterland nicht mit einer Binde um die Augen lieben",
      "enPart": "",
      "category": "preDraft",
      "id": 3493
    },
    "3494": {
      "dePart": "Eine dünne Freiheitist immer noch besser alsfettes Sklaventum",
      "enPart": "",
      "category": "preDraft",
      "id": 3494
    },
    "3495": {
      "dePart": "Der Eckstein der Gerechtigkeit ist die Gleichheit vor dem Gesetz",
      "enPart": "",
      "category": "preDraft",
      "id": 3495
    },
    "3496": {
      "dePart": "Wenn du erwartest, dass Dinge passieren, passieren sie seltsamerweise",
      "enPart": "",
      "category": "preDraft",
      "id": 3496
    },
    "3497": {
      "dePart": "Man hat immer zwei Gründe etwas zu tun: Einen anständigen und den wahren",
      "enPart": "",
      "category": "preDraft",
      "id": 3497
    },
    "3498": {
      "dePart": "Was nützt das Tempo, wenn unterwegs das Gehirn ausgeronnen ist?",
      "enPart": "",
      "category": "preDraft",
      "id": 3498
    },
    "3499": {
      "dePart": "Ein Mensch ist erst alt, wenn Reue den Platz von Träumen einnimmt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3499
    },
    "3500": {
      "dePart": "Versuchung im vorgerückten Alter ist wie ein Dudelsack ohne Luft",
      "enPart": "",
      "category": "preDraft",
      "id": 3500
    },
    "3501": {
      "dePart": "Das Leben, Freunde, ist langweilig. Das dürfen wir nicht sagen",
      "enPart": "",
      "category": "preDraft",
      "id": 3501
    },
    "3502": {
      "dePart": "Das Verlassen des Mutterschoßes ist der Eintritt in tausend Tode",
      "enPart": "",
      "category": "preDraft",
      "id": 3502
    },
    "3503": {
      "dePart": "Ein grausamer Mensch weidet sich an Tränen, er wird nicht durch sie gebrochen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3503
    },
    "3504": {
      "dePart": "Alles Leben hat keinen Stillstand und das Schönste ist das Schnellste.",
      "enPart": "",
      "category": "preDraft",
      "id": 3504
    },
    "3505": {
      "dePart": "Selbstständig wirken ist Leben; mechanisch tun müssen Tod",
      "enPart": "",
      "category": "preDraft",
      "id": 3505
    },
    "3506": {
      "dePart": "Wer lauter große Dinge sehen will, muss sich zu einer Mücke wünschen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3506
    },
    "3507": {
      "dePart": "Gewohnheit ist eine schreckliche Tyrannin",
      "enPart": "",
      "category": "preDraft",
      "id": 3507
    },
    "3508": {
      "dePart": "Das Leben zehrt den Tod auf, und nicht der Tod das Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3508
    },
    "3509": {
      "dePart": "Leben und Tod; daraus ist alles zusammengesetzt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3509
    },
    "3510": {
      "dePart": "Jede Gegend nimmt mit der Zeit die Seelen ihrer Einwohner an sich.",
      "enPart": "",
      "category": "preDraft",
      "id": 3510
    },
    "3511": {
      "dePart": "Geduld ist mehr wertals hektische Geschäftigkeit",
      "enPart": "",
      "category": "preDraft",
      "id": 3511
    },
    "3512": {
      "dePart": "Schütze dich vor Monotomie, sie ist die Mutter aller tödlichen Sünden",
      "enPart": "",
      "category": "preDraft",
      "id": 3512
    },
    "3513": {
      "dePart": "Menschenkenntnis macht den Verständigen ebenso nachsichtig als bescheiden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3513
    },
    "3514": {
      "dePart": "Das Gegenteil eines erfüllten Lebensist ein leeres oder überfülltes Leben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3514
    },
    "3515": {
      "dePart": "Altersproblem: Die Zeit nimmt ab und die Langsamkeit zu.",
      "enPart": "",
      "category": "preDraft",
      "id": 3515
    },
    "3516": {
      "dePart": "Hinter dem Traum der ewigen Jugend steht oft die Realität ewiger Unreife.",
      "enPart": "",
      "category": "preDraft",
      "id": 3516
    },
    "3517": {
      "dePart": "Die Weltgeschichte liest sich wie eine Krankengeschichte der Menschheit",
      "enPart": "",
      "category": "preDraft",
      "id": 3517
    },
    "3518": {
      "dePart": "Die Langsamkeit bietet die Chance, das, was wir tun, auch zu erleben",
      "enPart": "",
      "category": "preDraft",
      "id": 3518
    },
    "3519": {
      "dePart": "Manche Menschen sind bereit, für ihre Sicherheitdie größten Risiken einzugehen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3519
    },
    "3520": {
      "dePart": "Die wenigsten Menschen genießen ein Leben,das nur aus Genuss besteht.",
      "enPart": "",
      "category": "preDraft",
      "id": 3520
    },
    "3521": {
      "dePart": "Die Wirklichkeit des Lebens besteht nicht aus Gefühl, sondern aus Aktivität",
      "enPart": "",
      "category": "preDraft",
      "id": 3521
    },
    "3522": {
      "dePart": "Das Geschöpf muss glauben, dass es nicht ist, um alles zu sein",
      "enPart": "",
      "category": "preDraft",
      "id": 3522
    },
    "3523": {
      "dePart": "Es bleibt immer etwas hängenam Menschen von seiner Heimat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3523
    },
    "3524": {
      "dePart": "Der beste Test für Disziplin ist ihre Abwesenheit",
      "enPart": "",
      "category": "preDraft",
      "id": 3524
    },
    "3525": {
      "dePart": "Das Äußere des Menschen ist der Text zu allem,was man über ihn sagen kann.",
      "enPart": "",
      "category": "preDraft",
      "id": 3525
    },
    "3526": {
      "dePart": "In der Tiefe ihres Gemütes verbirgt die Menschheit ihre Schätze.",
      "enPart": "",
      "category": "preDraft",
      "id": 3526
    },
    "3527": {
      "dePart": "Alles, was wir wissen, bezieht sich auf etwas, was wir nicht wissen",
      "enPart": "",
      "category": "preDraft",
      "id": 3527
    },
    "3528": {
      "dePart": "Die Tränen sind für das Herz, was das Wasser für die Fische ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3528
    },
    "3529": {
      "dePart": "Das Alltägliche genügt, um es zu tun. Soll man auch noch davon reden?",
      "enPart": "",
      "category": "preDraft",
      "id": 3529
    },
    "3530": {
      "dePart": "Tränen sind der Zoll für den Garten Gottes.",
      "enPart": "",
      "category": "preDraft",
      "id": 3530
    },
    "3531": {
      "dePart": "Es ist schändlich, um des Lebens willen den Sinn des Lebens zu verlieren.",
      "enPart": "",
      "category": "preDraft",
      "id": 3531
    },
    "3532": {
      "dePart": "Wir vertreiben uns die Zeit so lange, bis sie uns vertreibt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3532
    },
    "3533": {
      "dePart": "Altwerden ist nur steigende Müdigkeit vor dem Schlafengehen",
      "enPart": "",
      "category": "preDraft",
      "id": 3533
    },
    "3534": {
      "dePart": "Alle Ordnung ist für den einzelnen Freiheitsbeschränkung.",
      "enPart": "",
      "category": "preDraft",
      "id": 3534
    },
    "3535": {
      "dePart": "Tugend schärft nur ihre Kraft,wenn sie schweigend schafft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3535
    },
    "3536": {
      "dePart": "Wer Leidenschaft zum Vernunftgenossen macht,hintergeht sein Gewissen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3536
    },
    "3537": {
      "dePart": "Die nach allen Freuden rennen darfst Du leere Köpfe nennen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3537
    },
    "3538": {
      "dePart": "Freiheit wünschen nur wenige edle Menschen, Willkür der große Haufen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3538
    },
    "3539": {
      "dePart": "Die Willkür kennt nur den blinden Eigenwillen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3539
    },
    "3540": {
      "dePart": "Oft ist es die Angst vor einem Übel,die uns zu einem schlimmeren führt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3540
    },
    "3541": {
      "dePart": "Nichts zu tun haben ist eine fürchterliche Bürde",
      "enPart": "",
      "category": "preDraft",
      "id": 3541
    },
    "3542": {
      "dePart": "Das Leben ist ein Mysterium, das man vom schöpferischen Herzen aus erlebt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3542
    },
    "3543": {
      "dePart": "Des Menschen Feind, Bruder, ist der Mensch!",
      "enPart": "",
      "category": "preDraft",
      "id": 3543
    },
    "3544": {
      "dePart": "Es ist nötig, dass wir den Schwerpunktdes Lebens in unser Inneres verlegen.",
      "enPart": "",
      "category": "preDraft",
      "id": 3544
    },
    "3545": {
      "dePart": "Nur wer die Gegenwart gekannthat, weiß wirklich, was die Hölle ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3545
    },
    "3546": {
      "dePart": "Aller Fortschritt bringt nur Variationen zu dem alten,ewigen Liede des Lebens.",
      "enPart": "",
      "category": "preDraft",
      "id": 3546
    },
    "3547": {
      "dePart": "Erfahrung trügt so oft, weil niemals zwei Fälle ganz gleich gelagert sind.",
      "enPart": "",
      "category": "preDraft",
      "id": 3547
    },
    "3548": {
      "dePart": "Ängstlichkeit nimmt nicht dem Morgen seine Sorge, aber dem Heute seine Kraft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3548
    },
    "3549": {
      "dePart": "Man verzählt sich stets, wenn man mit Angst und Hoffnung rechnet.",
      "enPart": "",
      "category": "preDraft",
      "id": 3549
    },
    "3550": {
      "dePart": "Angst ist der einzige sichere Ratgeber, den das Leben überhaupt hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3550
    },
    "3551": {
      "dePart": "Nichts nutzt die Eile; man starte zur rechten Zeit",
      "enPart": "",
      "category": "preDraft",
      "id": 3551
    },
    "3552": {
      "dePart": "Das Leben gleicht eher einem Gemäldeals einer exakt zu lösenden Rechenaufgabe.",
      "enPart": "",
      "category": "preDraft",
      "id": 3552
    },
    "3553": {
      "dePart": "Kein Mörder hat ewiges Leben, das in ihm bleibt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3553
    },
    "3554": {
      "dePart": "Auch im Orchester des Lebens dringt das Blech am meisten durch.",
      "enPart": "",
      "category": "preDraft",
      "id": 3554
    },
    "3555": {
      "dePart": "Es ist hart zu leben; aber härter ist es noch zu sterben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3555
    },
    "3556": {
      "dePart": "Um das Große zu schätzen und lieben,musst du dich erst an dem Kleinen üben.",
      "enPart": "",
      "category": "preDraft",
      "id": 3556
    },
    "3557": {
      "dePart": "Manche Leute wollen nie, sondern \"möchten\" bloß",
      "enPart": "",
      "category": "preDraft",
      "id": 3557
    },
    "3558": {
      "dePart": "Man besitzt nur die Seele, die sich uns gab, nicht die, die man sich nahm.",
      "enPart": "",
      "category": "preDraft",
      "id": 3558
    },
    "3559": {
      "dePart": "Am Ende erlebt man nur viel,um viel vergessen zu können.",
      "enPart": "",
      "category": "preDraft",
      "id": 3559
    },
    "3560": {
      "dePart": "Niemand ist vollkommen frei, da man auch ein Sklave seiner Unabhängigkeit ist.",
      "enPart": "",
      "category": "preDraft",
      "id": 3560
    },
    "3561": {
      "dePart": "Wo wäre das Verdienst, wenn die Helden niemals Angst hätten?",
      "enPart": "",
      "category": "preDraft",
      "id": 3561
    },
    "3562": {
      "dePart": "Die Menschen werden alt, aber selten reif",
      "enPart": "",
      "category": "preDraft",
      "id": 3562
    },
    "3563": {
      "dePart": "Die in der Sehnsucht warten, wachsen zu Riesen",
      "enPart": "",
      "category": "preDraft",
      "id": 3563
    },
    "3564": {
      "dePart": "Die Wirklichkeit ist nie so quälerischals die Gedankenwelt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3564
    },
    "3565": {
      "dePart": "Jeder Mensch baut sich durch seine Lebensweiseseine Todesweise auf.",
      "enPart": "",
      "category": "preDraft",
      "id": 3565
    },
    "3566": {
      "dePart": "Sehnsucht haben heisst so heisses Blut kriegen,daaa alles ringsum verdorrt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3566
    },
    "3567": {
      "dePart": "Dem wahrhaft Neugierigen erschließt sich alles, was das Leben zu bieten hat.",
      "enPart": "",
      "category": "preDraft",
      "id": 3567
    },
    "3568": {
      "dePart": "Gut vertragen sich miteinander ein stumpfer Geist und eine spitze Zunge.",
      "enPart": "",
      "category": "preDraft",
      "id": 3568
    },
    "3569": {
      "dePart": "Nächst dem Hunger ist die Eitelkeit die stärkste Triebkraft.",
      "enPart": "",
      "category": "preDraft",
      "id": 3569
    },
    "3570": {
      "dePart": "Was wir nicht zu erkennen vermögen,ist uns zu wissen nicht notwendig.",
      "enPart": "",
      "category": "preDraft",
      "id": 3570
    },
    "3571": {
      "dePart": "Furcht vor Gefahr ist zehntausendmal beängstigender als die Gefahr selbst.",
      "enPart": "",
      "category": "preDraft",
      "id": 3571
    },
    "3572": {
      "dePart": "Leben heisst lachen mit blutenden Wunden.",
      "enPart": "",
      "category": "preDraft",
      "id": 3572
    },
    "3573": {
      "dePart": "Das Talent liebt Schwierigkeiten, aber solche,die es sich selber wählt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3573
    },
    "3574": {
      "dePart": "Bücher sind für viele Leute nur Möbel aus Papier",
      "enPart": "",
      "category": "preDraft",
      "id": 3574
    },
    "3575": {
      "dePart": "Ein jeder hat Luftschlösser und der Arme zumeist",
      "enPart": "",
      "category": "preDraft",
      "id": 3575
    },
    "3576": {
      "dePart": "Sterben ist eine wilde Nacht und ein neuer Weg.",
      "enPart": "",
      "category": "preDraft",
      "id": 3576
    },
    "3577": {
      "dePart": "Das Alter kommt plötzlich und nicht nach und nach, wie man immer sagt.",
      "enPart": "",
      "category": "preDraft",
      "id": 3577
    },
    "3578": {
      "dePart": "Eigentlich jung kann man nur im Alter sein",
      "enPart": "",
      "category": "preDraft",
      "id": 3578
    },
    "3579": {
      "dePart": "Die Natur erklären wir, das Seelenleben verstehen wir.",
      "enPart": "",
      "category": "preDraft",
      "id": 3579
    },
    "3580": {
      "dePart": "Nur was der Geist geschaffen hat, versteht er.",
      "enPart": "",
      "category": "preDraft",
      "id": 3580
    },
    "3581": {
      "dePart": "Was der Mensch sei, sagt ihm die Geschichte.",
      "enPart": "",
      "category": "preDraft",
      "id": 3581
    },
    "3582": {
      "dePart": "Das Leben verlangt gebieterisch eine Leitung durch den Gedanken.",
      "enPart": "",
      "category": "preDraft",
      "id": 3582
    }
